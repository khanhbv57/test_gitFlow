YUI().use('io-base', 'node-base', function (Y) {

    // XXX michaeln 2011-09-16 bug=851660 Autoload further batches of results.
    var reviews_uri = "/cat/applications/saucy/ibus-unikey/reviews-ajax/";
    Y.io(reviews_uri, {
        on: {
            complete: function(id, response){
                if (response.status == 200) {
                    var reviews_html = response.responseText;
                    var reviews_div = Y.one('#reviews_placeholder');
                    reviews_div.set("innerHTML", reviews_html);
                }
            }
        }
    });
    var recommends_div = Y.one('.portlet.recommendedportlet');
    if (recommends_div){
        var recommendations_uri = "/cat/recommends/ibus-unikey/";
        Y.io(recommendations_uri, {
            on: {
                complete: function(id, response){
                    if (response.status == 200) {
                        var recommends_html = response.responseText;
                        recommends_div.set("innerHTML", recommends_html);
                    }
                }
            }
        });
    }
});
</script>


        
