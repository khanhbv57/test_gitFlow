    setSelectedOption(this.$('.vmp-font-family select'), values.fontFamily);
    setSelectedOption(this.$('.vmp-fg-color > select'), values.color);
    setSelectedOption(this.$('.vmp-text-opacity > select'), values.textOpacity);
    setSelectedOption(this.$('.vmp-bg-color > select'), values.backgroundColor);
    setSelectedOption(this.$('.vmp-bg-opacity > select'), values.backgroundOpacity);
    setSelectedOption(this.$('.window-color > select'), values.windowColor);
    setSelectedOption(this.$('.vmp-window-opacity > select'), values.windowOpacity);

    var fontPercent = values.fontPercent;

    if (fontPercent) {
      fontPercent = fontPercent.toFixed(2);
    }

    setSelectedOption(this.$('.vmp-font-percent > select'), fontPercent);
  };

