	mePlayer.on("adplay", function(){
		this.mePoster.statusPlay.classList.add("disabled-ui")
		this.mmWatermark && this.mmWatermark.el_.classList.add("disabled-ui");
		console.log("ADPLAY .............")
		this.sharetools.hide && this.sharetools.hide()
		this.videoRelated && this.videoRelated.hide()
	})

	mePlayer.on("contentresume", function(){
		this.mePoster.statusPlay.classList.remove("disabled-ui")
		this.mmWatermark &&  this.mmWatermark.el_.classList.remove("disabled-ui");
		if (data.playerProfile && data.playerProfile.playScreen.showSharing)
			this.sharetools.show && this.sharetools.show()
		else
			this.sharetools.hide && this.sharetools.hide()

		if (data.playerProfile && data.playerProfile.playScreen.showRelatedButton)
			this.videoRelated && this.videoRelated.showButtonRelated()
		else
			this.videoRelated && this.videoRelated.hide()

		console.log("CONTENT RESUME...............")
	})

	mePlayer.on("pause", function() {
