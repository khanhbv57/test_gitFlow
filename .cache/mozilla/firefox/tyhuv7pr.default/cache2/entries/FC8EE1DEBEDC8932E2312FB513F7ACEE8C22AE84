                                                        c-0.125,0.125-0.327,0.125-0.452,0L0.094,4.271z"></path>\
                                                </g>\
                                                </svg>'
        var a = document.createElement('a');
        a.class = 'btn-next-pl';
        a.innerHTML = str;

        return a;

    }

    mmPlaylist.prototype.createElementScrollPrevButton = function () {
        var str = '<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="5.198px" height="8.09px" viewBox="0 0 5.198 8.09" enable-background="new 0 0 5.198 8.09" xml:space="preserve">\
                                                <g>\
                                                    <path d="M1.379,7.995c-0.125,0.125-0.327,0.125-0.452,0L0.094,7.162c-0.125-0.125-0.125-0.327,0-0.452L2.76,4.045L0.094,1.379\
                                                        c-0.125-0.125-0.125-0.327,0-0.452l0.833-0.833c0.125-0.125,0.327-0.125,0.452,0l3.725,3.725c0.125,0.125,0.125,0.327,0,0.452\
                                                        L1.379,7.995z"></path>\
                                                </g>\
                                                </svg>'
        var a = document.createElement('a');
        a.class = 'btn-next-pl';
        a.innerHTML = str;

        return a;
    }

    /*
     * BUtton playlist
     */
    //create button
    var defaults = {};
    //get component Menubutton
    var menuButton = videojs.getComponent('MenuButton');

    //Class playlistButton
    var PlaylistButton = videojs.extend(menuButton, {
        constructor: function (player, options, settings) {
            menuButton.call(this, player, options);
            this.controlText(options.name);
            //image logo
            var logo = options.logo;
            logo.id = options.id;
            logo.classList.add(options.nameClass);
            this.el().appendChild(logo);
        }
    });
    /*
     * Function create button
     * 
     */
    var LogoPlaylist = function () {
        //logo
        var logo = document.createElement('div');
        logo.className = 'asset-ele-r btn-playlist has-tooltip';
        logo.style.padding = '10px 0 0 0'; // chinh css ok 
