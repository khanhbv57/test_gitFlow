        });
        return div;
    }

//hàm dùng để tính toán giá trị kéo , và định giá tri mặc định cho content
    calPx = function (playlist, type, startMarginX, distance) {
        var w = 0;
        var controlbarWidth = playlist.player.controlBar.width();
        w = startMarginX;
        var val = parseInt(controlbarWidth / itemWidth) * itemWidth;
        var x = 0;
        switch (type) {
            case "next":
                x = w - val;
                break;
            case "prev":
                x = w + val;
                break;
            case "pointer":
                x = w - distance;
                break;
        }

        //Nếu số item ít quá thì không kéo dc
        if ((playlist.videos.length) * itemWidth < controlbarWidth) {
            return 0;
        }

        //không được quá giá trị max là 140 * số item
        var maxWidth = -((playlist.videos.length) * itemWidth) + controlbarWidth - 20;
        if (x >= 0) {
            x = 0;
        } else if (x < maxWidth) {
            x = maxWidth;
        }
        return x + "px";
    }

//thay đổi current video , thêm class selected vào content playlist
    mmPlaylist.prototype.changeCurrentVideo = function (i) {
        if (this.current >= 0 && this.current < this.videos.length) {
			console.log("remove select");
            removeClass(this.id + "-item-" + this.current, "selected");
        }
		console.log("current : "+this.current);
		console.log("change current : "+i);
        this.current = i;
		console.log("add class select : "+this.id+"-item-"+i);
        addClass(this.id + "-item-" + i, "selected")
    }

//thêm sự kiên cho item content playlist
    mmPlaylist.prototype.addEventClickPlaylist = function (li, i, href) {
        var playlist = this;
        li.addEventListener('click', function () {
            //console.log(window.scrollPlaylist);
            if (window.scrollPlaylist) {
                window.scrollPlaylist = false;
            } else {
                playlist.updateSrc(i);
                if (playlist.settings.autoPlay) {
                    playlist.player.play();
                }
            }
        });
        //event double click
        li.addEventListener('dblclick', function () {
			if(typeof(href) == 'undefined' || href.length == 0){
				return false;
			} 
            window.location.href = href;
        });
    }

    //event next video
    mmPlaylist.prototype.nextVideo = function () {
        var select = 0;
        if (this.current != null) {
            select = Math.abs(parseInt(this.current) + 1) % this.videos.length;
        }
        this.updateSrc(select);
    }
//event prev video
    mmPlaylist.prototype.prevVideo = function () {
        var select = 0;
        if (this.current != null) {
            select = this.current - 1;
            if (select < 0) {
                select = this.videos.length - 1;
            }
        }
        this.updateSrc(select);
    }

	
	//1 repeat 1 video ,2 shufle , 3 repeat playlist , 0 no action
	mmPlaylist.prototype.getPlayType = function(){
		var playlist = this;
		if(!playlist.player.id_){
			console.log("id_ not found");
			playlist.settings.playType = 0;
			return 0;
		}
		var playType = sessionStorage.getItem(playlist.player.id_+"playType");
		console.log("play type : "+playType);
		if(playType != null && typeof(playType) != 'undefined' ){
			console.log("play type != null");
			playlist.settings.playType=playType;
			return playType;
		}else{
			console.log("play type == null");
			playlist.settings.playType = 0;
			sessionStorage.setItem(playlist.player.id_+"playType",0);
			return 0;
		}
		console.log("next play type : "+playType);
	}
	mmPlaylist.prototype.nextPlayType = function(){
		var playlist = this;
		if(!playlist.player.id_){
			return;
		}
		var type = this.settings.playType;
		if(typeof(type) == 'undefined' ){
			type= 0;
		}
		type++;
		if(type> 3 ){
			type=0;
		}
		this.settings.playType = type;
		sessionStorage.setItem(playlist.player.id_+"playType",type);
		console.log("next play type : "+type);
	}

	
	//end play type
	
    mmPlaylist.prototype.event = function () {
		console.log("event -");
        var playlist = this;
        if (!playlist.settings) {
            return;
        }
		var type = playlist.getPlayType();
		
		console.log("type : "+type);
		console.log("type : "+typeof(type));
		//if(typeof(type) == 'number' ){
			switch(Number(type)){
				case 0 :
					console.log("case = 0");
					if (playlist.current + 1 == playlist.videos.length) {
						return;
					}
					playlist.updateSrc(Math.abs(playlist.current + 1) % playlist.videos.length);
					return;
				case 1 : //repeat 1 video
					console.log("repeat 1 video");
					playlist.player.play();
					return;
				case 2 : // shuffle
					var idShuffle = Math.floor((Math.random() * this.videos.length));
					console.log("shuffle : " + idShuffle)
					this.updateSrc(idShuffle);
					return;
				case 3 : //repeat playlist
					console.log("repeat playlist");
					playlist.updateSrc(Math.abs(playlist.current + 1) % playlist.videos.length);
					return;
			}
		//}
		
		
		
		
       

    }



    //create button next element 
    mmPlaylist.prototype.createElementScrollNextBottom = function () {
        var str = '<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="5.198px" height="8.09px" viewBox="0 0 5.198 8.09" enable-background="new 0 0 5.198 8.09" xml:space="preserve">\
