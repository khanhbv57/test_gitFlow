        overlay.onclick = this.overlayClickEvent;
        document.addEventListener('keyup', this.keyUpEvent, false);
        player.el().insertBefore(overlay, player.controlBar.el());
      },
	      keyUpEvent: function(e) {
	        if (e.keyCode === 27) {
	          this.teardown();
	        }
	      },
      
	      teardown: function() {
			  var checkopen = this.open;
			  
	        var overlays = player.el().getElementsByClassName('sharetools-overlay');
	        var netwk = player.el().getElementsByClassName('network-share-mep show');
			var embedshare = player.el().getElementsByClassName('embed-share-mep show');
	        if (overlays.length > 0) {
	            document.removeEventListener('keyup', this.keyUpEvent);
	            
	            if(netwk[0] != null){

	  		        netwk[0].classList.remove('show');
	            }
	    			  if(embedshare.length > 0){
	    				  
	    				  embedshare[0].setAttribute('style','top:-100%;');
	    			  }
	    				  
	        		  setTimeout(function(){
	                if (overlays[0] != null){
	                    player.el().removeChild(overlays[0]);
	                }
	        			 
	        		  },100);//1000
	            
	  		  
	        }

	        this.open = false;

	        // start player playing again
			
	        if (player.paused() && lastStatus == true && checkopen == true) {
				
	          player.play();
	        }
			if ((player.paused() && lastShowOverlay == true) || (player.paused() && settings.showOnPause == true) ){
				player.play();
			}

	      },
		  overlayClickEvent: function(e) {
			var clipboard = new Clipboard('.btn1');

			clipboard.on('success', function(e) {
				console.log(e);
			});

			clipboard.on('error', function(e) {
				console.log(e);
			});
			var clipboard2 = new Clipboard('.btn2');

			clipboard2.on('success', function(e) {
				console.log(e);
