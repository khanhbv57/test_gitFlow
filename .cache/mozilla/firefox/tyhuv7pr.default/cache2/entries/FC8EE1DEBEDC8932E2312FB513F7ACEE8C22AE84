  var style = Dom.$('.vmp-styles-defaults');

  if (!style) {
    style = stylesheet.createStyleElement('vmp-styles-defaults');
    var head = Dom.$('head');

    if (head) {
      head.insertBefore(style, head.firstChild);
    }
    stylesheet.setTextContent(style, '\n      .video-js {\n        width: 300px;\n        height: 150px;\n      }\n\n      .vmp-fluid {\n        padding-top: 56.25%\n      }\n    ');
  }
}

