    mmPlaylist.prototype.createHeaderInnerBottom = function () {
        var playlist = this;

        //head -playlist
        var divHead = document.createElement('div');
        divHead.className = 'head-playlist';

        //title
        var title = document.createElement('h4');
        title.appendChild(document.createTextNode(playlist.title));
        divHead.appendChild(title);
        //end title

        //div pick num video
        var pickNum = playlist.createPickNum();
        divHead.appendChild(pickNum);
        //end divHead

        //btn next 
        var btnNext = playlist.createBtnNextPl();
        var btnPrev = playlist.createBtnPrevPl();
        divHead.appendChild(btnNext);
        divHead.appendChild(btnPrev);

        return divHead;

    }

    mmPlaylist.prototype.createElementBottom = function (i) {
        var playlist = this;
        var item = this.videos[i];

        //li
        var li = document.createElement('li');
        li.id = playlist.id + "-item-" + i;

        //a
        var a = document.createElement('a');
        a.className = 'item-video-pl clearfix';
        //div
        var divInfo = document.createElement('div');
        divInfo.className = 'info-video-pl';
        //span number video
        var spanNumberVideo = document.createElement('span');
        spanNumberVideo.className = 'number-video-pl';
        spanNumberVideo.appendChild(document.createTextNode(i + 1));
        divInfo.appendChild(spanNumberVideo);
        //end span number video 

        //span duration
        var spanDuration = document.createElement('span');
        spanDuration.className = "duration-video-pl";
        spanDuration.appendChild(document.createTextNode(convertDuration(item.duration)));
        divInfo.appendChild(spanDuration);
        //end span duration

        //img
        var img = document.createElement("img");
        img.className = "thumb-video-pl";
        img.src = item.img;
        divInfo.appendChild(img);
        //end img

        //p title 
        var title = document.createElement("p");
        title.className = "title-video-pl";
        title.appendChild(document.createTextNode(item.title));
        divInfo.appendChild(title);
        //end title

        //span custom
        var custom = document.createElement('span');
        custom.className = "channel-video-pl";
        custom.appendChild(document.createTextNode(item.customField));
        divInfo.appendChild(custom);
        //end custom

        a.appendChild(divInfo);
        //end div

        li.appendChild(a);
        //end a 
        playlist.addEventClickPlaylist(li, i, item.href);
        return li;

    }
    /*
     *CONTENT HEADER MOBILE
