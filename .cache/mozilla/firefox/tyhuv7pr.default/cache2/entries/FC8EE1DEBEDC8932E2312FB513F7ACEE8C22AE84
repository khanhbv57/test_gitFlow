//Hàm tạo ô chọn số
    mmPlaylist.prototype.createPickNum = function () {
        var playlist = this;
        //pick num video
        var pickNum = document.createElement('div');
        pickNum.className = 'pick-num-video';
        //a button
        var buttonPick = document.createElement('a');
        buttonPick.id = 'btn-pick-num';
        buttonPick.className = 'btn-pick-num';
        // buttonPick.href = '';
        buttonPick.appendChild(document.createTextNode('1/' + this.videos.length + ' videos'));
        pickNum.appendChild(buttonPick);
        //end button

        //ul list number id
        var ul = document.createElement('ul');
        ul.style.cursor = 'pointer';
        ul.id = 'list-num-video';
        ul.className = 'list-num-video';
        for (i = 0; i < this.videos.length; i++) {
            //li
            var li = document.createElement('li');
            li.id = playlist.id + "-" + i;
            //a
            var a = document.createElement('a');
            //a.href = '';

            a.appendChild(document.createTextNode(i + 1));
            li.appendChild(a);
            li.addEventListener('click', function () {
                //console.log("click li : " + this.id);
                var id = this.id.substring(this.id.length - 1, this.id.length);
                playlist.updateSrc(id);
                if (playlist.settings.autoPlay) {
                    playlist.player.play();
                }
            });
            //end a
            ul.appendChild(li);
            //end li
        }
        // buttonPick.appendChild(ul);

        //add event btn click
        buttonPick.addEventListener('click', function () {
            var className = ul.getAttribute("class")
            if (className == "list-num-video") {
                ul.classList.add('show');
            } else {
                ul.className = "list-num-video show";
                ul.classList.remove('show');
            }
        });
        buttonPick.style.cursor = 'pointer';
        pickNum.addEventListener('mouseleave', function () {
            ul.classList.remove('show');
        });
        pickNum.appendChild(ul);
        return pickNum;
    }

    /*
     * 
     * Tạo button
     */

    //Create button prev playlist
    mmPlaylist.prototype.createBtnPrevPl = function () {
        var playlist = this;
        var aBtnPrev = document.createElement('a');
        var str = '<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="5.198px" height="8.09px" viewBox="0 0 5.198 8.09" enable-background="new 0 0 5.198 8.09" xml:space="preserve">\
                                            <g>\
                                                <path d="M0.094,4.271c-0.125-0.125-0.125-0.327,0-0.452l3.725-3.725c0.125-0.125,0.327-0.125,0.452,0l0.833,0.833\
                                                    c0.125,0.125,0.125,0.327,0,0.452L2.438,4.045L5.104,6.71c0.125,0.125,0.125,0.327,0,0.452L4.271,7.995\
                                                    c-0.125,0.125-0.327,0.125-0.452,0L0.094,4.271z"></path>\
                                            </g>\
                                            </svg>';
        aBtnPrev.innerHTML = str;
        aBtnPrev.className = 'btn-prev-pl';

        //event dịch chuyển content playlist
        aBtnPrev.addEventListener('click', function () {
            var ulMargin = document.getElementById(playlist.id + "-list-playlist").style.marginLeft;
            document.getElementById(playlist.id + "-list-playlist").style.marginLeft = calPx(playlist, "prev", parseInt(ulMargin) || 0);
        });
        return aBtnPrev;
    }

    //create button playlist next playlist
    mmPlaylist.prototype.createBtnNextPl = function () {
        var playlist = this;
        var aBtnNext = document.createElement('a');
        var str = '<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="5.198px" height="8.09px" viewBox="0 0 5.198 8.09" enable-background="new 0 0 5.198 8.09" xml:space="preserve">\
                                            <g>\
                                                <path d="M1.379,7.995c-0.125,0.125-0.327,0.125-0.452,0L0.094,7.162c-0.125-0.125-0.125-0.327,0-0.452L2.76,4.045L0.094,1.379\
                                                    c-0.125-0.125-0.125-0.327,0-0.452l0.833-0.833c0.125-0.125,0.327-0.125,0.452,0l3.725,3.725c0.125,0.125,0.125,0.327,0,0.452\
                                                    L1.379,7.995z"></path>\
                                            </g>\
                                            </svg>';
        aBtnNext.innerHTML = str;
        aBtnNext.className = 'btn-next-pl';

        //event dịch chuyển khung content qua bên phải
        aBtnNext.addEventListener('click', function () {
            //console.log('btn next');
            var ulMargin = document.getElementById(playlist.id + "-list-playlist").style.marginLeft;
            document.getElementById(playlist.id + "-list-playlist").style.marginLeft = calPx(playlist, "next", parseInt(ulMargin) || 0);
        });
        return aBtnNext;
    }

    //crreate button next videoId 
    mmPlaylist.prototype.createBtnNextVideoPl = function () {
        var playlist = this;
