}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
},{}],8:[function(require,module,exports){
exports = module.exports = function(window, videojs) {
	
	var defaults = {
		statusUrl : "//mecloud.vn/live/status",
		vid: "",
		enable: false,
		preview: true, 

	}

	//helper function
	function xhr_status(url, params, callback){
		var xhttp = new XMLHttpRequest();
		xhttp.onreadystatechange = function() {
			if (xhttp.readyState == 4 && xhttp.status == 200) {
				var reply = JSON.parse(xhttp.responseText)
				callback(null, reply)
			}
		};
		xhttp.open("POST", url, true);
		xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
		xhttp.send(params);
	}

	function offSignal(publish, scheduleTime){
		player.liveState = "off"
		if (publish==false && scheduleTime > 0)
			player.updateOnline(false. scheduleTime)
		else
			player.updateOnline(false)
	}

	var currentTime = 0;
	var lastcurTime = 0;
	var checkonBuff;
	var bufTime = 0;

	function onSignal(visitor){
		player.updateViewer(visitor)
		player.liveState = "on"
		player.updateOnline(true)
		if (checkonBuff) {
			clearInterval(checkonBuff)
		}
		checkonBuff = setInterval(function(){
			if (!player.paused()){
				lastcurTime = currentTime;
				currentTime = player.currentTime();
				if (lastcurTime==currentTime){
					bufTime++;
				} else {
					bufTime = 0;
				}
				if (bufTime==2){ //check buff time 2 seconds
					clearInterval(checkonBuff)
					checkStatus(player.settings.statusUrl, "aliasId="+player.settings.vid+"&preview="+player.settings.preview, 1000)
				}
			} else
				bufTime = 0;
		},1000)
	}

	function checkStatus(url,params, time){
		console.log("checkStatus " + params)
		setTimeout(function(){
			xhr_status(url, params,  function(err, data){
				if (!err) {
					if (data.state=="new"){
						// console.log("new")

						offSignal(data.publish, data.scheduleTime)
						checkStatus(url, params,2000)
					} else if (data.state=="started"){
						// console.log("started")
						console.log(data)
						if (data.signal==false){
							offSignal(data.publish, data.scheduleTime)
							checkStatus(url, params,2000)
						} else {
							if (data.src) {
								if (player.liveState=="off"){
									player.updateSrc(data.src)
									player.play()
								}
								onSignal(data.realtime || 0)
							} else {
								offSignal(data.publish, data.scheduleTime)
								checkStatus(url, params,2000)
							}
						}

					} else if (data.state=="end"){
						// console.log("state end")
						if (data.src){
							player.updateSrc(data.src)
							player.updateOnline(true)
						} else {
							offSignal()
							document.getElementsByClassName("txt-offline-title")[0].innerHTML = "PROCESSING"
						}
						//TODO: update video src (if available)
					} else {
						//console.log("somgthing wrong")
						//TODO: something wrong
					}
				} else {
					console.log("error")
				}
			})
		},time)
	}

	var player = "";
	//plugin declaration
	var mecloudLiveStream = function(options){
		var settings = videojs.mergeOptions(defaults, options);
		player = this;
		player.settings = settings;
		player.liveState="off"

		//check if live is enabled
		if (!settings.enable) {
			return;
		}

		//remove icon-play
		var iconPlay = player.el_.querySelector(".icon-play")
		if (iconPlay)
			iconPlay.innerHTML = "";
		
		//get status 
		checkStatus(settings.statusUrl, "aliasId="+settings.vid+"&preview="+settings.preview, 1)
		
		//realtime
		setInterval(function(){
			xhr_status(settings.statusUrl, "aliasId="+settings.vid+"&preview="+settings.preview,  function(err, data){
				if (!err) {
					if (typeof data.realtime != "undefined"){
						player.updateViewer(data.realtime)
					}
				}		
			})
		},10000)


	}
	//register plugin
	videojs.plugin('mecloudLiveStream', mecloudLiveStream);
}
},{}],9:[function(require,module,exports){

exports = module.exports = function (window, videojs) {

    /*
     COmmon
     Các hàm sử dụng chung
     */

    //Var global
    //define item width
    //1 item có chiều rộng là 140px
    var itemWidth = 140;

    getCss = function (file) {
        var head = document.getElementsByTagName('head')[0];
        var css = document.createElement("link");
        css.type = "text/css"
        css.rel = "stylesheet";
        css.href = file;
        head.appendChild(css);
    }
    //get script


    //function remove class in item
    removeClass = function (id, nameClass) {
        var e;
        if (id == null || nameClass == null || nameClass === "") {
            return;
        }

        if (typeof (id) === "string") {
            if (id === "") {
                return;
            }
            e = document.getElementById(id);
        } else if (typeof (id) === "object") {
            e = id;
        }
        if (e && e.className.indexOf(nameClass) > 0) {
            var classId = "";
            for (i = 0; i < e.classList.length; i++) {
				console.log(e.classList[i]);
                if (e.classList[i] !== nameClass) {
					console.log(e.classList[i]);
                    classId += e.classList[i] + " ";
                }
            }
            e.className = classId;
        }
    }

    //function add class in object /id
    addClass = function (id, nameClass) {
        if (id == null || nameClass == null || id === "" || nameClass === "") {
            return;
        }
        var e;
        if (typeof (id) === 'string') {
            e = document.getElementById(id);
        } else if (typeof (id) === 'object') {
            e = id;
        }

        e.className += ' ' + nameClass;
    }
    //convert duration 60s -> 1:00
    convertDuration = function (time) {
        var rs = '';
        if (time <= 0) {
            return "00:00";
        }
        while (time > 0) {
            var temp = time % 60;
            rs = (temp <= 9 ? "0" : "") + temp + rs;
            time = Math.floor(time / 60);
            if (time > 0) {
                rs = ":" + rs;
            }
        }
        if (rs.length == 2) {
            rs = "00:" + rs;
        }
        return rs;
    }

    //class itemPLaylist demo
    function mmItemPlaylist(id, img, title, duration) {
        this.id = id;
        this.title = title;
        this.duration = duration;
        this.img = img;
    }
    //function to String mmItemPlaylist demo
    mmItemPlaylist.prototype.toString = function () {
        console.log("videoId :" + this.id + "  title : " + this.title);
    }

    //end class itemplaylist



    //Create playlist 
    //
    /*
     * KHởi tạo playlist - bằng object setting
     * @param {type} titlePlaylist
     * @param {type} settings
     * @returns {playlist_L1.mmPlaylist}
     */
    // var cssPlaylist = "playlist-2.00.css";

	//thêm pw, chèn vào div ngoài
    function mmPlaylist(settings) {
		console.log("playlist setting");
		console.log(settings);
		console.log("init current : "+settings.current);
		
		if (settings.pluginFolder) {
            //console.log("pluginFolder : " + settings.pluginFolder);
            //console.log("URL : " + settings.pluginFolder + "playlist/css/" + (settings.cssPlaylist ? settings.cssPlaylist : cssPlaylist));
            // getCss(settings.pluginFolder + "playlist/css/" + (settings.cssPlaylist ? settings.cssPlaylist : cssPlaylist));
			
        }
	
			this.title = settings.title;  // title player 
			this.type = settings.type;   /// type video : inner , bottom , 
			this.settings = settings;   // save info 
			this.videos = settings.videos;   /// videos [list video playlist]
			this.player = settings.player;   //player [ player use playlist ]
			settings.player.playlist = this;
			this.current = settings.current;  // current videoId  :  
			this.createTime = new Date().getTime();  //time create 
			
			if(typeof(this.settings.autoNext) == 'undefined'){
				this.settings.autoNext = true;
			}
			this.id = "mmPlaylist-" + this.createTime;  // id playlist
		
    }

    /*
     * start playlist with setting
     * Sau khi khởi tạo playlist, Chạy playlist 
     */
    mmPlaylist.prototype.start = function () {
        //choose video with current videoId
        var playlist = this;
		
        //console.log("setting current : " + this.current);
        if (isNaN(Number(this.current))) {
            this.current = 0;
        }

        //update source with id current
        if (Number(this.current) !== -1) {
            this.updateSrc(this.current);
        }

        //if autoNext = true ->when player end -> updateSrc 
		/*
        if (this.settings.autoNext) {
            playlist.player.on("ended", function () {
				playlist.nextVideo();
			});
        }
		*/
		
		//isShow
		if(this.settings.isShow){
			playlist.show();
		}
		
        playlist.player.on("ended", function () {
            playlist.event();
        });

        playlist.player.on("release.all", function (a, b, c) {
            playlist.hide();
        });
		
		playlist.player.on("playlist.dispose", function () {
            playlist.dispose();
        });
    }

    //update src video + all video quality 
    mmPlaylist.prototype.updateSrc = function (i) {
        //check
        var playlist = this;

		if(!this.videos){
			console.log("videos source : "+ this.videos);
			return;
		}
        if (i < 0 || i >= this.videos.length) {
			console.log("index is invalid : "+i);
            return;
        }
        //change class active video
		
        this.changeCurrentVideo(i);
        var id = Number(i) + 1;
        var pickNumBtn = document.getElementById('btn-pick-num');
        //change box pick num 
        if (pickNumBtn) {
            pickNumBtn.innerHTML = (id) + '/' + this.videos.length + ' videos';
        }
        //get Itemvideo in list 
        var item = this.videos[i];
        if (!item) {
            return;
        }
        var arrSrc = []; // array src of quality
		if(item.listSrc){
			
			for (var j = 0; j < item.listSrc.length; j++) {
				var v = item.listSrc[j];
				arrSrc.push({type: v.type, src: v.src, label: v.label, res: v.res});
			}
			this.player.src(arrSrc);
		}
        //change button pause -> play 
        //change
        playlist.player.play();
        if (playlist && playlist.settings && playlist.settings.autoPlay) {

        } else {
            playlist.player.pause();
        }

        //event nextVideo
		console.log("ban event nextVideo.play ");
		console.dir(item);
        playlist.player.trigger("nextVideo.play",item);
    }

//hàm thay đổi list video của playlist
    mmPlaylist.prototype.changeContentPlaylist = function (listVideo) {
        var playlist = this;
        if (listVideo) {
            playlist.videos = listVideo;
            playlist.createPlayList();
        }
    }

//hàm dùng để thay đổi nội dung của playlist, thay đổi tất cả 
    mmPlaylist.prototype.changePlaylist = function (settings) {
        var playlist = this;
        if (settings) {
            if (settings.title) {
                this.title = settings.title;  // title player 
            }
            if (settings.type) {
                this.type = settings.type;   /// type video : inner , bottom , 
            }
            this.settings = settings;   // save info 
            if (settings.videos && settings.videos.length > 0) {
                this.videos = settings.videos;   /// videos [list video playlist]
            }
            if (this.current && this.current >= 0 && this.current < this.videos.length) {
                this.current = settings.current;  // current videoId  :  
            } else {
                this.current = 0;
            }
            playlist.createPlayList();
            playlist.start();
        }
    }

 
	
	mmPlaylist.prototype.dispose = function(){
		var playlist = this;
		 var e = document.getElementById(playlist.id);
		e.remove();
	}

    mmPlaylist.prototype.show = function () {
        var playlist = this;
        var e = document.getElementById(this.id);
        if (e) {
            if (e.className.indexOf("show") < 0) {
                addClass(e, 'show');
                playlist.player.controlBar.addClass('active');
				console.log("show ");
				if(!playlist.settings.isShow && playlist.type && playlist.type == 'outterPlaylistRight'){
					//console.log("offsetWidth : "+playlist.player.el_.offsetWidth);
					var width = Number(playlist.player.el_.parentElement.parentElement.style.width.replace("px",""));
					//console.log("width ban dau : "+width);
					playlist.player.el_.parentElement.parentElement.style.width = ( width-300)+"px";
					playlist.player.el_.parentElement.parentElement.style.height = ((width -300)*9/16)+"px";
					playlist.player.el_.style.height = ((width-300)*9/16)+"px";
					
				}else{
					playlist.settings.isShow =false;
				}
            }
        }
    }
	   mmPlaylist.prototype.hide = function () {
        var playlist = this;
        var e = document.getElementById(playlist.id);
        if (e && e.className.indexOf("show") > 0) {
            removeClass(e, 'show');
            playlist.player.controlBar.removeClass('active');
			//console.log("hide ");
			if(playlist.type && playlist.type == 'outterPlaylistRight'){
					var width = (playlist.player.el_.parentElement.offsetWidth+300);
					playlist.player.el_.parentElement.parentElement.style.width  =width+ "px";
					playlist.player.el_.parentElement.parentElement.style.height = (width*9/16)+"px";
					playlist.player.el_.style.height = (width*9/16) +"px";
					
				}
        }
		
    }
	
	mmPlaylist.prototype.clickBtnPlaylist = function(){
		var playlist = this;
		 var e = document.getElementById(this.id);
                if (e) {
                    if (e.className.indexOf('show') >= 0) {
                       playlist.hide();
					    playlist.player.trigger("playlistBtn.hide");
                    } else {
                        playlist.show();
                        playlist.player.trigger("playlistBtn.show");
                    }
                }
	}



    /*
     * Create  playlist Common
     * Hàm tạo playlist gồm (tạo wraper , header , content)
     */
    mmPlaylist.prototype.createPlayList = function () {
        //div play list 
        var dPlaylist = document.getElementById(this.id);
        if (dPlaylist) {
            dPlaylist.innerHTML = "";
        } else {
            dPlaylist = document.createElement('div');
            //setting
            //type 
            dPlaylist.className = this.type+" playlistEventControlbar";
            dPlaylist.id = this.id; //id playlist
        }

        //div wrapper  video playlist
        var wrapper = document.createElement('div');
        wrapper.className = 'wrapper-video-playlist';

        //header 
        var header;
        var content;

        //choose type of playlist
        switch (this.type) {
            case "innerPlaylistRight":
            case "outterPlaylistRight":
            case "outterPlaylistBottomList":
                header = this.createPlaylistHeader();
                break;
            case "innerPlaylistBottom":
            case "outterPlaylistBottom":
                header = this.createHeaderInnerBottom();
                break;
            case "innerPlaylistBottomMobile":
                header = this.createHeaderInnerMobile();
                break;
            default:
                console.log("Type error;");
                return;
        }

        //create playlist content
        content = this.createPlaylistContent();
        //append
        wrapper.appendChild(header);
        wrapper.appendChild(content);
        //end content playlist

        dPlaylist.appendChild(wrapper);
        //end wraper
        return dPlaylist;
    }
    mmPlaylist.prototype.createPlaylistContent = function () {
        var playlist = this;
        //content play list
        var content_playlist = document.createElement('div');
        content_playlist.className = 'content-playlist';
        //ul list 

        var ul = document.createElement('ul');
        //mouse hover pointer
        ul.style.cursor = 'pointer';
        ul.className = 'list-playlist';
        ul.id = this.id + "-list-playlist";

        //type
        for (i = 0; i < this.videos.length; i++) {
            switch (playlist.type) {
                case "innerPlaylistRight":
                case "outterPlaylistRight":
                case "outterPlaylistBottomList":
                    ul.appendChild(this.createElement(i));
                    break;
                case "innerPlaylistBottom":
                case "outterPlaylistBottom":
                case "innerPlaylistBottomMobile":
                    ul.appendChild(this.createElementBottom(i));


                    /*
                     * Event scroll
                     */

                    //event , rê chuột kéo khung =))
                    var press = 0, startMarginX = 0;
                    var val = 0;
                    //nếu là thằng ngang thì mới add event này vào

                    //nhấn nút lưu lại vị trí hiện tại trục x
                    ul.addEventListener('mousedown', function (e) {
                        press = e.pageX;
                        e.preventDefault();
                        startMarginX = parseInt(ul.style.marginLeft) || 0;
                    });
                    //thả ra thì xóa biến press đi (phải để windown , vì nếu ra ngoài khung thì event này không thực hiện được
                    window.addEventListener('mouseup', function (e) {
                        press = 0;
                        e.preventDefault();
                    });
                    //khi di chuyển ta gọi hàm callPx ( tính toán rồi move content )
                    ul.addEventListener('mousemove', function (e) {
                        if (press > 0) {
                            window.scrollPlaylist = true;
                            var x = press - e.pageX;
                            ul.style.marginLeft = calPx(playlist, "pointer", startMarginX, x); //(startMarginX - x) + "px";//
                        }
                        e.preventDefault();
                    });

                    //evevt dành cho android , ios không sài videojs dc  , 
                    ul.addEventListener("touchstart", function (e) {
                        press = e.changedTouches[0].pageX;
                        startMarginX = parseInt(ul.style.marginLeft) || 0;
                    });
                    ul.addEventListener("touchend", function (e) {
                        press = 0;
                    });
                    ul.addEventListener("touchmove", function (e) {
                        if (press > 0) {
                            window.scrollPlaylist = true;
                            var x = press - e.changedTouches[0].pageX;
                            ul.style.marginLeft = calPx(playlist, "pointer", startMarginX, x); //(startMarginX - x) + "px";//
                        }
                        //e.preventDefault();
                    });
                    //end event


                    break;
                default:
                    console.log("Type error;");
                    return;
            }
        }
        //end ul list 
        content_playlist.appendChild(ul);
        //end ul list
        return content_playlist;
    }

    /* 
     * CONTENT INNER PLAYLIST RIGHT 
     * Tạo header cho playlist Inner right
     */
    mmPlaylist.prototype.createPlaylistHeader = function () {
        var playlist = this;
        //head playlist
        var header = document.createElement('div');
        header.className = 'head-playlist';
        //title
        var title = document.createElement('h4');
        title.appendChild(document.createTextNode(this.title));
        header.appendChild(title);
        //end title

        var pickNum = playlist.createPickNum();
        header.appendChild(pickNum);
        //end pick num video

        return header;
    }


//Hàm tạo chung cho 1 video
    mmPlaylist.prototype.createElement = function (i) {
        var playlist = this;
        var item = this.videos[i];
        if (!item) {
            return;
        }

        var li = document.createElement('li');
        //li.id = "item-playlist-" + i; //id li
        li.id = playlist.id + "-item-" + i;

        //a href 
        var a = document.createElement('a');
        a.className = 'item-video-pl clearfix';

        if (item.liveHref) {
            a.href = item.liveHref;   //dont have  href 
        }
        //id
        var id = document.createElement('span');
        id.className = 'number-video-pl';
        id.appendChild(document.createTextNode(i + 1));
        a.appendChild(id);
        //end id

        //span Image 
        //span 1
        var span = document.createElement('span');
        span.className = 'thumb-video-pl';
        //span 2
        var span_ = document.createElement('span');
        //img 
        var img = document.createElement('img');
        //img.className = 'thumb-video-pl';
		img.src ="";
		if(item.img){
			img.src = item.img;
		}
        span_.appendChild(img);
        //end img

        span.appendChild(span_);
        //end span 2

        a.appendChild(span);
        //end span 1 

        //end span Image

        //div 
        var div_info = document.createElement('div');
        div_info.className = 'info-video-pl';
        //title
        var title = document.createElement('p');
        title.className = 'title-video-pl';
		
		if(item.title){
			title.appendChild(document.createTextNode(item.title));
		}else{
			title.appendChild(document.createTextNode(""));
		}
        div_info.appendChild(title);
        //end title

        //fields
        var fields = document.createElement('div');
        fields.className = 'field-video-pl clearfix';
        //name channel
        var channel = document.createElement('span');
        channel.className = 'channel-video-pl';
		if(item.customField){
			channel.appendChild(document.createTextNode(item.customField));
		}else{
			channel.appendChild(document.createTextNode(""));
		}
        fields.appendChild(channel);
        //end channel

        //duration 
        var duration = document.createElement('span');
        duration.className = 'duration-video-pl';
		if(item.duration){
			duration.appendChild(document.createTextNode(convertDuration(item.duration)));
		}else{
			duration.appendChild(document.createTextNode(convertDuration(0)));
		}
        fields.appendChild(duration);
        //end duration

        div_info.appendChild(fields);
        //end fields

        a.appendChild(div_info);
        //end div_info


        var playlist = this;

        playlist.addEventClickPlaylist(li, i, item.href);
        //end event

        li.appendChild(a);
        return li;
    }
    //END CONTENT INNER PLAYLISTRIGHT
    /* 
     * END INNER PLAY LIST RIGHT 
     */

    /* 
     * INNER PLAY LIST BOTTOM 
