							for (var i = 0; i < buttons.length; i++) {
								
								if(buttons[i].classList.contains('share') == true)continue;

					            buttons[i].style.display = 'none';
					        };
						}
					}

					
			  });
		  },
		  showBtnShareAllTime : function(){
		  	
			  var divCbTopRight = player.el_.getElementsByClassName('controls-bar-top-right');
			  divCbTopRight[0].classList.add("showAllTime");
			  divCbTopRight[0].classList.add("active");
		  },
		  UnShowBtnShareAllTime: function(){
			  var divCbTopRight = player.el_.getElementsByClassName('controls-bar-top-right');
			  divCbTopRight[0].classList.remove("showAllTime");
		  },
		  showBtnSharePlayEnd : function(){
		  		player.on('ended', function() {
					var divCbTopRight = player.el_.getElementsByClassName('controls-bar-top-right');
					  divCbTopRight[0].classList.add("showAllTime");
					  divCbTopRight[0].classList.add("active");
				});
			  
		  },
		  UnShowBtnSharePlayEnd: function(){
			  var divCbTopRight = player.el_.getElementsByClassName('controls-bar-top-right');
			  divCbTopRight[0].classList.remove("showAllTime");
		  },
		  update : function(e){ 
			settings.showButtonShare = (e.showButtonShare === true) ? true : (e.showButtonShare === false) ? false : settings.showButtonShare ;  
			settings.showOnPause = (e.showOnPause === true) ? true : (e.showOnPause === false) ? false : settings.showOnPause ; 
			settings.showBeforePlay = (e.showBeforePlay === true) ? true : (e.showBeforePlay === false) ? false : settings.showBeforePlay ; 
			settings.showEnd = (e.showEnd === true) ? true : (e.showEnd === false) ? false : settings.showEnd ; 
			if(settings.showButtonShare == false){
				this.removeButtonShare();
			}else{
				this.addButtonShare();
			}
			
			if(settings.showOnPause == true){
				  this.showBtnOnPaused();
				
			}else{
				player.on('pause', function() {
				  });
			}
			if(settings.showBeforePlay == true){
				this.showBtnShareAllTime();
			}else{
				this.UnShowBtnShareAllTime();
			}

			if(settings.showEnd == true){
				this.showBtnSharePlayEnd();
			}else{
				this.UnShowBtnSharePlayEnd();
			}
			
		  },
		  show: function(e){ 
		  		
				this.addButtonShare();
		  },
		  hide: function(e){ 
		  		
				this.removeButtonShare();
				
		  }
	  
    };
  };

  
  var initShareToolsPlugin = function (options) {
	
    var player = this;
	
	
    var settings = extend({}, defaults, options || {});

    player.sharetools = new ShareTools(player, settings);
	
    // create share button
    var shareButton;
    shareButton = player.el_.getElementsByClassName('controls-bar-top-right');
    

	player.sharetools.addButtonShare();

	if (settings.showButtonShare == false) {
		player.sharetools.removeButtonShare();
		var contentshareButton = player.el_.getElementsByClassName('button-bar-top-right share');
		if (contentshareButton != null && contentshareButton.length > 0){
			player.sharetools.removeButtonShare();
		}
		
    }else{
    	// Not do something
    }
	
	
  };
	
  vjs.plugin('sharetools', initShareToolsPlugin);

}

}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
