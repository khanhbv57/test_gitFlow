	if (config){
		var loaded = 0, needToLoad = 0, onReady = [], errors={};
		
		// load script function
		var loadScript = function(url, callback, pluginName){
			var script = document.createElement('script'), ld = false;
			script.type = 'text/javascript';
			script.src = url;
			script.onreadystatechange = function () {
				if (this.readyState === 'complete' || this.readyState === 'loaded'){
					if (ld) return;
					ld = true;
					if (callback) callback();
					else {
						loaded ++;
						// loadPlugin();
					}
				}
			};
			var checkLoad = function(){
				if (ld) return;
				ld = true;
				if (callback) callback();
				else {
					loaded ++;
					// loadPlugin();
				}
			}
			script.onload = checkLoad
			script.onerror = function(){
				checkLoad()
				errors[pluginName]=true;
			}

			needToLoad ++;
			document.head.appendChild(script);
		}
		
		

		// load plugins
		var loadPlugin = function(){
			if (window.$_VConfig && window.$_VConfig.plugins){
				var plugins = window.$_VConfig.plugins;
				for (var plugin in plugins){
					var version = plugins[plugin];
					var scriptUrl = window.$_VConfig.pluginFolder + plugin + '/' + plugin + '-' + version +'.js';
					
					// load script
					loadScript(scriptUrl,function(){
						loaded ++;
						if (loaded >= needToLoad && onReady.length)
							for (var i = 0; i < onReady.length;i++){
								onReady[i]();
							}
					},plugin);
				}
			}
		};
