	mePlayer.on("postrollended", function() {
		this.mePoster.statusPlay.classList.remove("disabled-ui")
		this.mmWatermark &&  this.mmWatermark.el_.classList.remove("disabled-ui");

		if (data.playerProfile && data.playerProfile.endScreen.option == "SOCIAL") {
			this.sharetools.show && this.sharetools.show()
			this.videoRelated && this.videoRelated.hide()
			this.videoRelated && this.videoRelated.showButtonRelated()
		}

		if (data.playerProfile && data.playerProfile.endScreen.option == "RELATED") {
			this.videoRelated && this.videoRelated.showElementRelated()
			this.sharetools.hide && this.sharetools.hide()
			this.sharetools.showBtnSharePlayEnd && this.sharetools.showBtnSharePlayEnd()
		}
		console.log("postrollended...............")
		// trigger('complete',{
		// 	vid:data.vid, session:data.session
		// });
	})


	Logger.info("Final code")
}


