/*! videojs-related - v0.0.0 - 2015-12-16
 * Copyright (c) 2015 Phuong Dao
 * Modified by Phuong Dao */
exports = module.exports = function(window, videojs) {

  var defaults = {},
      videoJsRelated,
      autoPlayTimeout,
      durationTimeout;

  /*
  * Duration convert to time
  * @param duration
  * @return String time
  */
  function convertTimeToHHMMSS(duration) {
    var secNum = parseInt(duration, 10); // don't forget the second param
    var hours   = Math.floor(secNum / 3600);
    var minutes = Math.floor((secNum - (hours * 3600)) / 60);
    var seconds = secNum - (hours * 3600) - (minutes * 60);

    if (hours   < 10) {hours   = "0"+hours;}
    if (minutes < 10) {minutes = "0"+minutes;}
    if (seconds < 10) {seconds = "0"+seconds;}
    var time    = hours+':'+minutes+':'+seconds;
    return time;
  }

  /*
  * Mobile and Tablet detect via userAgen
  */
  var mobileDetect = {
    isMobile: function() {
      var check = false;
      (function(a){if(/(android|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|iris|kindle|lge |maemo|midp|mmp|mobile.+firefox|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows ce|xda|xiino/i.test(a)||/1207|6310|6590|3gso|4thp|50[1-6]i|770s|802s|a wa|abac|ac(er|oo|s\-)|ai(ko|rn)|al(av|ca|co)|amoi|an(ex|ny|yw)|aptu|ar(ch|go)|as(te|us)|attw|au(di|\-m|r |s )|avan|be(ck|ll|nq)|bi(lb|rd)|bl(ac|az)|br(e|v)w|bumb|bw\-(n|u)|c55\/|capi|ccwa|cdm\-|cell|chtm|cldc|cmd\-|co(mp|nd)|craw|da(it|ll|ng)|dbte|dc\-s|devi|dica|dmob|do(c|p)o|ds(12|\-d)|el(49|ai)|em(l2|ul)|er(ic|k0)|esl8|ez([4-7]0|os|wa|ze)|fetc|fly(\-|_)|g1 u|g560|gene|gf\-5|g\-mo|go(\.w|od)|gr(ad|un)|haie|hcit|hd\-(m|p|t)|hei\-|hi(pt|ta)|hp( i|ip)|hs\-c|ht(c(\-| |_|a|g|p|s|t)|tp)|hu(aw|tc)|i\-(20|go|ma)|i230|iac( |\-|\/)|ibro|idea|ig01|ikom|im1k|inno|ipaq|iris|ja(t|v)a|jbro|jemu|jigs|kddi|keji|kgt( |\/)|klon|kpt |kwc\-|kyo(c|k)|le(no|xi)|lg( g|\/(k|l|u)|50|54|\-[a-w])|libw|lynx|m1\-w|m3ga|m50\/|ma(te|ui|xo)|mc(01|21|ca)|m\-cr|me(rc|ri)|mi(o8|oa|ts)|mmef|mo(01|02|bi|de|do|t(\-| |o|v)|zz)|mt(50|p1|v )|mwbp|mywa|n10[0-2]|n20[2-3]|n30(0|2)|n50(0|2|5)|n7(0(0|1)|10)|ne((c|m)\-|on|tf|wf|wg|wt)|nok(6|i)|nzph|o2im|op(ti|wv)|oran|owg1|p800|pan(a|d|t)|pdxg|pg(13|\-([1-8]|c))|phil|pire|pl(ay|uc)|pn\-2|po(ck|rt|se)|prox|psio|pt\-g|qa\-a|qc(07|12|21|32|60|\-[2-7]|i\-)|qtek|r380|r600|raks|rim9|ro(ve|zo)|s55\/|sa(ge|ma|mm|ms|ny|va)|sc(01|h\-|oo|p\-)|sdk\/|se(c(\-|0|1)|47|mc|nd|ri)|sgh\-|shar|sie(\-|m)|sk\-0|sl(45|id)|sm(al|ar|b3|it|t5)|so(ft|ny)|sp(01|h\-|v\-|v )|sy(01|mb)|t2(18|50)|t6(00|10|18)|ta(gt|lk)|tcl\-|tdg\-|tel(i|m)|tim\-|t\-mo|to(pl|sh)|ts(70|m\-|m3|m5)|tx\-9|up(\.b|g1|si)|utst|v400|v750|veri|vi(rg|te)|vk(40|5[0-3]|\-v)|vm40|voda|vulc|vx(52|53|60|61|70|80|81|83|85|98)|w3c(\-| )|webc|whit|wi(g |nc|nw)|wmlb|wonu|x700|yas\-|your|zeto|zte\-/i.test(a.substr(0,4)))check = true})(navigator.userAgent||navigator.vendor||window.opera);
      return check;
    },
    isTablet: function() {
      var check = false;
      (function(a){if(/(android|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|iris|kindle|lge |maemo|midp|mmp|mobile.+firefox|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows ce|xda|xiino|android|ipad|playbook|silk/i.test(a)||/1207|6310|6590|3gso|4thp|50[1-6]i|770s|802s|a wa|abac|ac(er|oo|s\-)|ai(ko|rn)|al(av|ca|co)|amoi|an(ex|ny|yw)|aptu|ar(ch|go)|as(te|us)|attw|au(di|\-m|r |s )|avan|be(ck|ll|nq)|bi(lb|rd)|bl(ac|az)|br(e|v)w|bumb|bw\-(n|u)|c55\/|capi|ccwa|cdm\-|cell|chtm|cldc|cmd\-|co(mp|nd)|craw|da(it|ll|ng)|dbte|dc\-s|devi|dica|dmob|do(c|p)o|ds(12|\-d)|el(49|ai)|em(l2|ul)|er(ic|k0)|esl8|ez([4-7]0|os|wa|ze)|fetc|fly(\-|_)|g1 u|g560|gene|gf\-5|g\-mo|go(\.w|od)|gr(ad|un)|haie|hcit|hd\-(m|p|t)|hei\-|hi(pt|ta)|hp( i|ip)|hs\-c|ht(c(\-| |_|a|g|p|s|t)|tp)|hu(aw|tc)|i\-(20|go|ma)|i230|iac( |\-|\/)|ibro|idea|ig01|ikom|im1k|inno|ipaq|iris|ja(t|v)a|jbro|jemu|jigs|kddi|keji|kgt( |\/)|klon|kpt |kwc\-|kyo(c|k)|le(no|xi)|lg( g|\/(k|l|u)|50|54|\-[a-w])|libw|lynx|m1\-w|m3ga|m50\/|ma(te|ui|xo)|mc(01|21|ca)|m\-cr|me(rc|ri)|mi(o8|oa|ts)|mmef|mo(01|02|bi|de|do|t(\-| |o|v)|zz)|mt(50|p1|v )|mwbp|mywa|n10[0-2]|n20[2-3]|n30(0|2)|n50(0|2|5)|n7(0(0|1)|10)|ne((c|m)\-|on|tf|wf|wg|wt)|nok(6|i)|nzph|o2im|op(ti|wv)|oran|owg1|p800|pan(a|d|t)|pdxg|pg(13|\-([1-8]|c))|phil|pire|pl(ay|uc)|pn\-2|po(ck|rt|se)|prox|psio|pt\-g|qa\-a|qc(07|12|21|32|60|\-[2-7]|i\-)|qtek|r380|r600|raks|rim9|ro(ve|zo)|s55\/|sa(ge|ma|mm|ms|ny|va)|sc(01|h\-|oo|p\-)|sdk\/|se(c(\-|0|1)|47|mc|nd|ri)|sgh\-|shar|sie(\-|m)|sk\-0|sl(45|id)|sm(al|ar|b3|it|t5)|so(ft|ny)|sp(01|h\-|v\-|v )|sy(01|mb)|t2(18|50)|t6(00|10|18)|ta(gt|lk)|tcl\-|tdg\-|tel(i|m)|tim\-|t\-mo|to(pl|sh)|ts(70|m\-|m3|m5)|tx\-9|up(\.b|g1|si)|utst|v400|v750|veri|vi(rg|te)|vk(40|5[0-3]|\-v)|vm40|voda|vulc|vx(52|53|60|61|70|80|81|83|85|98)|w3c(\-| )|webc|whit|wi(g |nc|nw)|wmlb|wonu|x700|yas\-|your|zeto|zte\-/i.test(a.substr(0,4)))check = true})(navigator.userAgent||navigator.vendor||window.opera);
      return check;
    },
    getDevice: function(){

      if(this.isMobile() == true) return 'mobile';

      if(this.isTablet() == true) return 'tablet';

      return 'desktop';
    }
  }

  /*
  * Get class
  */
  function getClass(number){
    
    var className = '';
    var device = mobileDetect.getDevice();

    switch(number){
      case 1:
        className = 'one-item';
      break;
      case 2:
        className = 'two-item';
      break;
      case 3:
        className = 'three-item';
      break;
      case 4:
        className = 'four-item';
      break;
      case 5:
        className = 'five-item';
      break;
      case 6:
        className = 'six-item';
      break;
      case 7:
        className = 'seven-item';
      break;
      case 8:
        className = 'eight-item';
      break;
      case 9:
        className = 'nine-item';
      break;
    }

    if(device == 'mobile' || device == 'tablet') className+= ' mobile';

    return className;
  }

  /*
  * Get max item
  */
  function getMaxItem(number){

    var device = mobileDetect.getDevice();

    if(device == 'mobile') return number > 2 ? 2 : number;
    
    if(device == 'tablet') return number > 6 ? 6 : number;

    return number;
  }

  

  function addJS(js){
    var head = document.getElementsByTagName('head')[0];
    var script = document.createElement('script');
    script.src = js;
    script.type = 'text/javascript';
    head.appendChild(script);
  }


    var related = {
      lang: null,
      eDuration: null,
      firstVideoItem: null,
      paused: true,
      updateSource: function (player, videoItem) {
        try{
          clearTimeout(autoPlayTimeout);
          if (typeof(player) == "undefined" || typeof(videoItem) == "undefined") return;   
          player.videoRelated.video = videoItem;
          player.src(videoItem.listSrc);      
          player.play();
          console.log(videoItem);
          player.trigger("select.related", videoItem);
        }catch(e){
          console.log(e)
        }        
      },
      cancelAutoplay: function(e){
        clearTimeout(autoPlayTimeout);
        e.parentNode.parentNode.removeChild(e.parentNode);      
        if(this.firstVideoItem != null) this.firstVideoItem.removeChild(this.firstVideoItem.childNodes[0]);      
      },
      createElementVideoItem: function(player, videoItem, loading){

        if (typeof(player) == "undefined" || typeof(videoItem) == "undefined") return;

        if (typeof(loading) == "undefined") loading = null;
        
        var aHref = document.createElement('a');
        aHref.className = 'item-relate-video';
        aHref.style.backgroundImage = 'url('+ videoItem.img +')';
        aHref.href  = 'javascript:void(0)';
        if(loading != null) aHref.innerHTML = loading;
        

        var divInfo = document.createElement('div');
        divInfo.className = 'info-relate-video';
        aHref.appendChild(divInfo);
   
        var spanTitle = document.createElement('span');
        spanTitle.className = 'title-relate-video';
        spanTitle.appendChild(document.createTextNode(videoItem.title));
        divInfo.appendChild(spanTitle);

        var spanChannel = document.createElement('span');
        spanChannel.className = 'channel-relate-video hidden-mobile';
        spanChannel.appendChild(document.createTextNode(videoItem.description));
        divInfo.appendChild(spanChannel);

        var spanDuration = document.createElement('span');
        spanDuration.className = 'duration-relate-video hidden-mobile';
        spanDuration.appendChild(document.createTextNode(convertTimeToHHMMSS(videoItem.duration)));
        divInfo.appendChild(spanDuration);
        aHref.addEventListener('click', function () {
            related.updateSource(player, videoItem);    
        });
        return aHref;
      },
      createElementVideoRelated: function(player, videoList, length, autoplay, duration){

        if (typeof(player) == "undefined" || typeof(videoList) == "undefined" || videoList.length <= 0) return;

        if (typeof(autoplay) == "undefined") autoplay = false;

        if (typeof(duration) == "undefined") duration = 10;      

        var className = getClass(length);  
        var divRelated = document.createElement('div');
        divRelated.id = 'relate-video';
        divRelated.className = 'relate-video ' + className;
        if(autoplay == true){
          var divAutoNext = document.createElement('div');
          divAutoNext.className = 'autoplay-next';
          divAutoNext.innerHTML = this.lang.autoplay_next_in;

          var spanDuration = document.createElement('span');
          spanDuration.innerHTML = duration;
          divAutoNext.appendChild(spanDuration);
          divAutoNext.appendChild(document.createTextNode(this.lang.second));

          var buttonCancel = document.createElement('button');
          buttonCancel.type = 'button';
          buttonCancel.className = 'btn-cancel';
          buttonCancel.appendChild(document.createTextNode(this.lang.cancel));
          buttonCancel.addEventListener('click', function () {
            related.cancelAutoplay(this);    
          });
          divAutoNext.appendChild(buttonCancel);
          divRelated.appendChild(divAutoNext);

          this.eDuration = spanDuration;
        }

        var divWrapperRelate = document.createElement('div');
        divWrapperRelate.className = 'wrapper-relate-video';
        divRelated.appendChild(divWrapperRelate);

        var divContentVideo = document.createElement('div');
        divContentVideo.id = 'content-video';
        divContentVideo.className = 'content-relate-video';
        divWrapperRelate.appendChild(divContentVideo);
        
        for (var i = 0; i < length; i++) {

          var loading = null;      
          if(autoplay == true && i == 0) loading = '<svg xmlns="http://www.w3.org/2000/svg" class="vmp-related-autoplay-svg"><rect class="vmp-related-autoplay-frame" width="100%" height="100%"></rect><rect class="vmp-related-autoplay-frame-progress" width="100%" height="100%" style="animation-duration: '+ duration +'s;"></rect></svg>';
          var aVideoItem = this.createElementVideoItem(player, videoList[i], loading);
          if(i == 0) this.firstVideoItem = aVideoItem;
          divContentVideo.appendChild(aVideoItem); 
        };
        return divRelated;
      },
      setElementClose: function(player){
        var divControlsBarTopRight,
            divButtonBarTopRight,
            buttonClose,
            divTooltip;

        if(typeof(player.videoRelated) == "undefined" || 
            typeof(player.videoRelated.videoList) == "undefined" || 
            player.videoRelated.videoList == null) return;

        divControlsBarTopRight = player.el_.getElementsByClassName('controls-bar-top-right');

        if(divControlsBarTopRight.length <= 0){

          divControlsBarTopRight = document.createElement('div');
          divControlsBarTopRight.className = 'controls-bar-top-right';

        }else{
          divControlsBarTopRight = divControlsBarTopRight[0];        
        }

        divButtonBarTopRight = document.createElement('div');
        divButtonBarTopRight.className = 'button-bar-top-right';
        divControlsBarTopRight.appendChild(divButtonBarTopRight);
        player.el_.appendChild(divControlsBarTopRight);

        buttonClose = document.createElement('button');
        buttonClose.className = 'btn-related-close btn-close has-tooltip-bar-top-right show';

        divTooltip = document.createElement('div');
        divTooltip.className = 'tooltip-bar-top-right';
        divTooltip.appendChild(document.createTextNode(this.lang.close));
        buttonClose.appendChild(divTooltip);
        buttonClose.innerHTML+= '<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="20px" height="20px" viewBox="0 0 20 20" enable-background="new 0 0 20 20" xml:space="preserve">\
                                  <path d="M12.323,10l7.357-7.357c0.426-0.426,0.426-1.123,0-1.549l-0.774-0.774c-0.426-0.426-1.123-0.426-1.549,0L10,7.677\
                                      L2.643,0.319c-0.426-0.426-1.123-0.426-1.549,0L0.319,1.094c-0.426,0.426-0.426,1.123,0,1.549L7.677,10l-7.357,7.357\
                                      c-0.426,0.426-0.426,1.123,0,1.549l0.774,0.774c0.426,0.426,1.123,0.426,1.549,0L10,12.323l7.357,7.357\
                                      c0.426,0.426,1.123,0.426,1.549,0l0.774-0.774c0.426-0.426,0.426-1.123,0-1.549L12.323,10z"></path>\
                              </svg>';

        buttonClose.addEventListener('click', function () {

          var buttons = player.el_.getElementsByClassName('button-bar-top-right');
          if(buttons.length > 0)
            for (var i = 0; i < buttons.length; i++) {
              buttons[i].style.display = '';
            };

          related.remove(player);
          if(related.paused == false) player.play();

          var contronlsBarTop = player.el_.getElementsByClassName('controls-bar-top-right');
          if(contronlsBarTop.length > 0) contronlsBarTop[0].className = 'controls-bar-top-right';
          
        });

        divButtonBarTopRight.appendChild(buttonClose);
      },
      setElementAdd: function(player){
        var divControlsBarTopRight,
            divButtonBarTopRight, 
            buttonAdd,
            divTooltip;

        if(typeof(player.videoRelated) == "undefined" || 
            typeof(player.videoRelated.videoList) == "undefined" || 
            player.videoRelated.videoList == null) return;

        divControlsBarTopRight = player.el_.getElementsByClassName('controls-bar-top-right');

        if(divControlsBarTopRight.length <= 0){

          divControlsBarTopRight = document.createElement('div');
          divControlsBarTopRight.className = 'controls-bar-top-right';      
          player.el_.appendChild(divControlsBarTopRight);
        }else{
          divControlsBarTopRight = divControlsBarTopRight[0];
        } 

        divButtonBarTopRight = document.createElement('div');
        divButtonBarTopRight.className = 'button-bar-top-right';
        divControlsBarTopRight.appendChild(divButtonBarTopRight);

        buttonAdd = document.createElement('button');
        buttonAdd.className = 'btn-related-add has-tooltip-bar-top-right show';
        divTooltip = document.createElement('div');
        divTooltip.className = 'tooltip-bar-top-right';
        divTooltip.appendChild(document.createTextNode(related.lang.video_related));
        buttonAdd.appendChild(divTooltip);
        buttonAdd.innerHTML+= '<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="18px" height="18px" viewBox="2 1 18 18" enable-background="new 2 1 18 18" xml:space="preserve">\
                                  <circle cx="6" cy="5" r="4"></circle>\
                                  <circle cx="16" cy="5" r="4"></circle>\
                                  <circle cx="6" cy="15" r="4"></circle>\
                                  <circle cx="16" cy="15" r="4"></circle>\
                              </svg>';

        buttonAdd.addEventListener('click', function () { 

          var buttons = player.el_.getElementsByClassName('button-bar-top-right');
          if(buttons.length > 0)
            for (var i = 0; i < buttons.length; i++) {
              buttons[i].style.display = 'none';
            };

          related.paused = player.paused()
          related.removeButtonAdd(player);
          related.show(player, true);        
          divControlsBarTopRight.className = 'controls-bar-top-right active';
        });
        divButtonBarTopRight.appendChild(buttonAdd);
        
      },
      removeButtonAdd: function(player){
        if (typeof(player.el_) == "undefined") return;
        var btnAdd = player.el_.getElementsByClassName('btn-related-add');
        if(btnAdd.length > 0) btnAdd[0].parentNode.parentNode.removeChild(btnAdd[0].parentNode);
      },
      removeButtonClose: function(player){
        if (typeof(player.el_) == "undefined") return;
        var btnClose = player.el_.getElementsByClassName('btn-related-close');
        if(btnClose.length > 0) btnClose[0].parentNode.parentNode.removeChild(btnClose[0].parentNode);
      },
      remove: function(player){
        if (typeof(player.el_) == "undefined") return;
        if (typeof(player.videoRelated) == "undefined" || typeof(player.videoRelated.elementRelated) == "undefined" || player.videoRelated.elementRelated == null) return;
        this.removeButtonClose(player);   
        this.setElementAdd(player);           
        player.el_.removeChild(player.videoRelated.elementRelated);
        player.videoRelated.elementRelated = null;
      },
      show: function(player, addClick){
        var autoplay,
            videoList;

        clearTimeout(autoPlayTimeout);
        clearInterval(durationTimeout);

        player.pause();
        videoList = player.videoRelated.videoList;    

        if (typeof(videoList) == "undefined" || videoList == null) return;

        var length = videoList.length > 9 ? 9 : videoList.length;
        length = getMaxItem(length);

        if(typeof(addClick) == "undefined" || addClick == true){
          autoplay = false;
        }else{
          autoplay = typeof(player.videoRelated.autoplay) == "undefined" ? false : player.videoRelated.autoplay;
        }
        
        if(typeof(player.videoRelated.elementRelated) != "undefined" && 
          player.videoRelated.elementRelated != null) player.videoRelated.elementRelated.parentNode.removeChild(player.videoRelated.elementRelated);

        player.videoRelated.elementRelated = related.createElementVideoRelated(player, videoList, length, autoplay, player.videoRelated.duration);
        player.el_.appendChild(player.videoRelated.elementRelated);
        var btnClose = player.el_.getElementsByClassName('btn-related-close');
        if(btnClose.length == 0) 
          related.setElementClose(player); 
        else
          btnClose.style.display = 'block';

        related.removeButtonAdd(player);
        if(autoplay == true){

          var duration = player.videoRelated.duration,
              self = player;

          durationTimeout = setInterval(function(){
            duration--;
            if(duration == 0) clearInterval(durationTimeout);
            related.eDuration.innerHTML = duration;
          },1000);

          autoPlayTimeout = setTimeout(function(){
            related.updateSource(self, videoList[0]);

          }, duration * 1000);
        }
      }
    };

  var videoRelated = function(){
    return {
      player: null,
      ended: true,
      autoplay: false,
      duration: 10,
      video: null,
      videoList: null,
      elementRelated: null,
      getVideo: function(){
        return this.video;
      },
      setVideo: function(video){    
        if (video === undefined)  return;     
        this.video = video;
      },
      getVideoList: function(){
        return this.videoList;
      },
      setVideoList: function(videoList){      
        if (videoList === undefined)  return;     
        this.videoList = videoList;
      },
      getPlayer: function(){
        return this.player;
      },
      setPlayer: function(player){
        this.player = player;
      },
      showButtonRelated: function(){

        if(this.player == null || 
            typeof(this.player.videoRelated) == "undefined" || 
            typeof(this.player.videoRelated.videoList) == "undefined" || 
            this.player.videoRelated.videoList == null) return;


        related.remove(this.player);
        related.removeButtonClose(this.player);
        
        var btnAdd = this.player.el_.getElementsByClassName('btn-related-add');
        if(btnAdd.length <= 0) related.setElementAdd(this.player);                
      },
      showElementRelated: function(){
        if(this.player == null) return;
        
        related.remove(this.player);
        related.removeButtonClose(this.player);
        related.removeButtonAdd(this.player);

        var buttons = this.player.el_.getElementsByClassName('button-bar-top-right');
        if(buttons.length > 0)
          for (var i = 0; i < buttons.length; i++) {
            buttons[i].style.display = 'none';
          };
        console.log("related show elementRelated .........")
        related.show(this.player, false);
      },
      hide: function(){
        if(this.player == null) return;
        
        related.remove(this.player);
        related.removeButtonClose(this.player);
        related.removeButtonAdd(this.player);
      },   
    };
  } 

  function loadJSON(callback, url) {   

    var xobj = new XMLHttpRequest();
    xobj.overrideMimeType("application/json");
    xobj.open('GET', url, true);
    xobj.onreadystatechange = function () {
          if (xobj.readyState == 4 && xobj.status == "200") {
            callback(xobj.responseText);
          }
    };
    xobj.send(null);  
  }


  /**
   * Initialize the plugin.
   * @param options (optional) {object} configuration for the plugin
   */
  videoJsRelated = function(options) {
    var settings = videojs.mergeOptions(defaults, options),
        player = this,
        lang;    

    if(typeof(player.videoRelated) == "undefined" || player.videoRelated == null) player.videoRelated = new videoRelated();   
    if(typeof(player.videoRelated.videoList) == "undefined" || player.videoRelated.videoList == null) player.videoRelated.videoList = settings.videos;
    if(typeof(settings.duration) != "undefined" && settings.duration != null) player.videoRelated.duration = settings.duration;
    if(typeof(settings.autoplay) != "undefined" && settings.autoplay != null) player.videoRelated.autoplay = settings.autoplay;    
    if(typeof(settings.ended) != "undefined" && settings.duration != null) player.videoRelated.ended = settings.ended;
    
    var language = (typeof(settings.lang) != "undefined" && settings.lang != null) ? settings.lang : 'vn';
    console.log("..............language: " + language)
    
    loadJSON(function(response) {
      related.lang = JSON.parse(response);      
      player.videoRelated.setPlayer(player);

      var showAdd = true;
      if(typeof(settings.showDuringPlay) == "undefined" || 
        settings.showDuringPlay == null ||
        settings.showDuringPlay == false){
        showAdd = false;
      }

      if(showAdd) related.setElementAdd(player);

    }, settings.pluginFolder + 'related/lang/' + language + '.json');
    

    if(player.videoRelated.ended == true){
      player.on('ended',function(){
        var buttons = player.el_.getElementsByClassName('button-bar-top-right');
          if(buttons.length > 0)
            for (var i = 0; i < buttons.length; i++) {
              buttons[i].style.display = 'none';
            };
        console.log("related ended .........");
        player.videoRelated.hide();
        related.show(player, false);
      });
    }
    
    player.on('play',function(){
      related.remove(this);
    });
  };  

  // register the plugin
  videojs.plugin('videoJsRelated', videoJsRelated);
}
},{}],12:[function(require,module,exports){
/*! videojs-resolution-switcher - v0.0.0 - 2015-7-26
 * Copyright (c) 2015 Kasper Moskwiak
 * Modified by Pierre Kraft
 * Licensed under the Apache-2.0 license. */
exports = module.exports = function(window, videojs) {
  /* jshint eqnull: true*/
  'use strict';

  var defaults = {},
      videoJsResolutionSwitcher;
  // var firstView = true

  function setSourcesSanitized(player, sources) {

    //Assign Label HD Resolution
    //vmp-resolution-button-staticlabel
    var _children = player.controlBar.resolutionSwitcher.el_.children;
    var staticLabel = getElementFromArrayByClassName(_children, 'vmp-resolution-button-staticlabel');
    //var staticLabel = player.controlBar.resolutionSwitcher.el_.children[1];
    var labelHD = "";
    if(sources[0].res == '1080' || sources[0].res == '720') labelHD = "<i>HD</i>";
    if (staticLabel != false) staticLabel.innerHTML = labelHD;
      
    //End Assign Label HD Resolution
    if (!player.firstView){
        player.firstView = true
        player.preload("none")
    } else {
        player.preload("auto")
    }
    // if (sources[0].type=="application/x-mpegURL" && firstView == false){
    //   player.preload("auto")
    // }
    // else{
    //   firstView = false
    //   player.preload("none")
    // }
    
    return player.src(sources.map(function(src) {
      return {src: src.src, type: src.type, res: src.res};
    }));
  }

  function getElementFromArrayByClassName(elements, className){
    if (typeof(elements) != "undefined"){
      for (var i = 0; i < elements.length; i++) {
        if(elements[i].className == className) return elements[i];
      };
    }
    
    return false;
  }

 
  


  /*
   * Resolution menu item
   */
  var MenuItem = videojs.getComponent('MenuItem');

  var ResolutionMenuItem = videojs.extend(MenuItem, {
    constructor: function(player, options, onClickListener, label){
      this.onClickListener = onClickListener;
      this.label = label;
      // Sets this.player_, this.options_ and initializes the component
      MenuItem.call(this, player, options);
      this.src = options.src;

      this.on('click', this.onClick);
      this.on('touchstart', this.onClick);

      if (options.initialySelected) {
        this.showAsLabel();
        this.selected(true);
      }
     
    },
    showAsLabel: function() {
      // Change menu button label to the label of this item if the menu button label is provided
      if(this.label) {
        
        if (this.el_.parentNode){
          var selecteItem = this.el_.parentNode.querySelector(".vmp-selected")
          selecteItem.classList.remove("vmp-selected")

        }
        this.label.innerHTML = this.options_.label;
        this.el_.classList.add("vmp-selected")
      }
    },
    onClick: function(){
      this.onClickListener(this);
      // console.log(this)
      // Hide bigPlayButton
      this.player_.bigPlayButton.hide();
      // Remember player state
      var currentTime = this.player_.currentTime();
      var isPaused = this.player_.paused();
      this.showAsLabel();
      // Change player source and wait for loadeddata event, then play video
      // loadedmetadata doesn't work right now for flash.
      // Probably because of https://github.com/videojs/video-js-swf/issues/124
      setSourcesSanitized(this.player_, this.src).one('loadedmetadata', function() {
          console.log(this.player_.techName_)
        this.player_.currentTime(currentTime);
        if(!isPaused){
          // Start playing and hide loadingSpinner (flash issue ?)
          // console.log("handle tech seek ")
          this.player_.play().handleTechSeeked_();
        } else {
          this.player_.play().handleTechSeeked_();
          this.player_.pause()
        }

        this.player_.trigger('resolutionchange');
      });
    }
  });


  /*
   * Resolution menu button
   */


   

   var MenuButton = videojs.getComponent('MenuButton');
   var ResolutionMenuButton = videojs.extend(MenuButton, {
     constructor: function(player, options, settings, label){
      this.sources = options.sources;
      this.label = label;
      this.label.innerHTML = options.initialySelectedLabel;
      // Sets this.player_, this.options_ and initializes the component
      MenuButton.call(this, player, options);
      //videojs.controlBar.resolutionSwitcher.children_[0]
      this.controlText('Quality');
      this.controlTextEl_.parentNode.removeChild(this.controlTextEl_);



      if(settings.dynamicLabel){
        this.el().appendChild(label);
      }else{
        //console.log(this.el());
        var staticLabel = document.createElement('span');
        staticLabel.classList.add('vmp-resolution-button-staticlabel');
        this.el().appendChild(staticLabel);

      }



     },

     /*handleClick: function() {
      if (this.buttonPressed_) {
        this.unpressButton();
        console.log('1');
      } else {
        this.pressButton();
        console.log('2');
      }    
      var self = this;
       window.addEventListener('mouseup',function(event) {
        var vmpMenu = document.getElementsByClassName("vmp-menu");
        if (event.target != vmpMenu[5] && event.target.parentNode != vmpMenu[5]){
          self.unpressButton();
          console.log(vmpMenu[5]);
        }
      });   
     },*/
     createItems: function(){
       var menuItems = [];
       var labels = (this.sources && this.sources.label) || {};
       var onClickUnselectOthers = function(clickedItem) {
        menuItems.map(function(item) {
          item.selected(item === clickedItem);
        });
       };

       for (var key in labels) {
         if (labels.hasOwnProperty(key)) {
          menuItems.push(new ResolutionMenuItem(
            this.player_,
            {
              label: key,
              src: labels[key],
              initialySelected: key === this.options_.initialySelectedLabel
            },
            onClickUnselectOthers,
            this.label));
          }
       }

       
       
       return menuItems;



     }
   });

  /**
   * Initialize the plugin.
   * @param options (optional) {object} configuration for the plugin
   */
  videoJsResolutionSwitcher = function(options) {
    var settings = videojs.mergeOptions(defaults, options),
        player = this,
        label = document.createElement('span');

    label.classList.add('vmp-resolution-button-label');

    

    player.updateSrc = function(src){
      //Return current src if src is not given
      if(!src){ return player.src(); }
      // Dispose old resolution menu button before adding new sources
      if(player.controlBar.resolutionSwitcher){
        player.controlBar.resolutionSwitcher.dispose();
        delete player.controlBar.resolutionSwitcher;
      }
      //Sort sources
      src = src.sort(compareResolutions);
      var groupedSrc = bucketSources(src);
      var choosen = chooseSrc(groupedSrc, src);
      var menuButton = new ResolutionMenuButton(player, { sources: groupedSrc, initialySelectedLabel: choosen.label , initialySelectedRes: choosen.res}, settings, label);
      menuButton.el().classList.add('vmp-resolution-button');

      
      player.controlBar.resolutionSwitcher = player.controlBar.addChild(menuButton);
      // if (src[0].type == "application/x-mpegURL"){
      //   menuButton.el().style.display="none"
      // }

      //custom quality UI
      var vmpTitleRes = player.controlBar.resolutionSwitcher.controlTextEl_;
      //var vmpMenuRes = player.controlBar.resolutionSwitcher.el_.children[0];
      //console.log(vmpMenuRes)
      //vmpMenuRes.insertBefore(vmpTitleRes,vmpMenuRes.children[0]);
      // end custom quality UI


      
      return setSourcesSanitized(player, choosen.sources);


    };



    /**
     * Method used for sorting list of sources
     * @param   {Object} a source object with res property
     * @param   {Object} b source object with res property
     * @returns {Number} result of comparation
     */
    function compareResolutions(a, b){
      if(!a.res || !b.res){ return 0; }
      return (+b.res)-(+a.res);
    }

    /**
     * Group sources by label, resolution and type
     * @param   {Array}  src Array of sources
     * @returns {Object} grouped sources: { label: { key: [] }, res: { key: [] }, type: { key: [] } }
     */
    function bucketSources(src){
      var resolutions = {
        label: {},
        res: {},
        type: {}
      };
      src.map(function(source) {
        initResolutionKey(resolutions, 'label', source);
        initResolutionKey(resolutions, 'res', source);
        initResolutionKey(resolutions, 'type', source);

        appendSourceToKey(resolutions, 'label', source);
        appendSourceToKey(resolutions, 'res', source);
        appendSourceToKey(resolutions, 'type', source);
      });
      return resolutions;
    }

    function initResolutionKey(resolutions, key, source) {
      if(resolutions[key][source[key]] == null) {
        resolutions[key][source[key]] = [];
      }
    }

    function appendSourceToKey(resolutions, key, source) {
      resolutions[key][source[key]].push(source);
    }

    /**
     * Choose src if option.default is specified
     * @param   {Object} groupedSrc {res: { key: [] }}
     * @param   {Array}  src Array of sources sorted by resolution used to find high and low res
     * @returns {Object} {res: string, sources: []}
     */
    function chooseSrc(groupedSrc, src){
      var selectedRes = settings.defaultRes;
      var selectedLabel = '';

      for (var i in src){
        if ( src[i].res == (selectedRes+"") ){
            selectedRes = src[i].res;
            selectedLabel = src[i].label;
        }
      }

      if (!selectedLabel) {
        selectedRes = src[src.length-1].res;
        selectedLabel = src[src.length-1].label;
      }
      
      // if (selectedRes === '360') {
      //   selectedRes = src[0].res;
      //   selectedLabel = src[0].label;
      // } else if (selectedRes === '480' || selectedRes == null) {
      //   // Select low-res if default is low or not set
      //   selectedRes = src[src.length - 1].res;
      //   selectedLabel = src[src.length -1].label;
      // } else if (groupedSrc.res[selectedRes]) {
      //   selectedLabel = groupedSrc.res[selectedRes][0].label;
      // }
      
      // if(selectedRes === undefined){
      //   return {res: selectedRes, label: selectedLabel, sources: groupedSrc.label[selectedLabel]};
      // }
      return {res: selectedRes, label: selectedLabel, sources: groupedSrc.res[selectedRes]};
    }

    // Create resolution switcher for videos form <source> tag inside <video>
    if(player.options_.sources.length > 1){
      player.updateSrc(player.options_.sources);

      //console.log(player.options_.sources);

      /*var arrayRes = player.options_.sources;
      console.log(arrayRes);

      for (i = 0; i<arrayRes.length; i++) {
        if(arrayRes[i].res == "1080" || arrayRes[i].res == "720") {

        }
      }
      if (player.options_.sources.res == 1080) {
        console.log('Full HD');
      }*/
    }

  };

  


  // register the plugin
  videojs.plugin('videoJsResolutionSwitcher', videoJsResolutionSwitcher);
};

