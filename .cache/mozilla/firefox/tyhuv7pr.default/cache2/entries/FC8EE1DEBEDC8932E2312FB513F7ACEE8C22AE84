    this.controlTextEl_ = Dom.createEl('span', {
      className: 'vmp-control-text'
    });

    if (el) {
      el.appendChild(this.controlTextEl_);
    }

    this.controlText(this.controlText_, el);

    return this.controlTextEl_;
  };

  /**
