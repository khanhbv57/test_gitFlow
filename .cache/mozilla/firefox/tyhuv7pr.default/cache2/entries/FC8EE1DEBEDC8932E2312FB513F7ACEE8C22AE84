    mmPlaylist.prototype.createHeaderInnerMobile = function () {
        var playlist = this;

        //head -playlist
        var divHead = document.createElement('div');
        divHead.className = 'head-playlist';

        //title
        var title = document.createElement('h4');
        title.appendChild(document.createTextNode(playlist.title));
        divHead.appendChild(title);
        //end title

        var wrapperControl = document.createElement('div');
        wrapperControl.className = 'wrapper-control-pl';

        //next prev button
        var prevBtn = playlist.createBtnPrevVideoPl();
        wrapperControl.appendChild(prevBtn);
        var pickNum = playlist.createPickNum();
        wrapperControl.appendChild(pickNum);
        var nextBtn = playlist.createBtnNextVideoPl();
        wrapperControl.appendChild(nextBtn);
        //end
        //div pick num video

        //end divHead

        //btn next 
        var btnNext = playlist.createBtnNextPl();
        var btnPrev = playlist.createBtnPrevPl();
        wrapperControl.appendChild(btnNext);
        wrapperControl.appendChild(btnPrev);

        divHead.appendChild(wrapperControl);
        return divHead;

    }
