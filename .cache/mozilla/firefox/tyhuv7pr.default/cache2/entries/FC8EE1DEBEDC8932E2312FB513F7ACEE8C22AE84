	this.logDiv = document.getElementById(id)
}

Logger.prototype.info = function(s){
	if (!this.logDiv) return
	var pa = document.createElement("p")
	var time = new Date();
	pa.innerHTML = s;
	this.logDiv.appendChild(pa);
	this.logDiv.scrollTop = this.logDiv.scrollHeight;
}

