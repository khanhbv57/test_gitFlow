					});
					//mePlayer.el_.style.height = mePlayer.el_.parentNode.offsetWidth*9/16+"px";
				}
				
				mePlayer.on("nextVideo.play", function(event, videoObj){
					var arr = document.querySelectorAll('div[id^="MeCloudVideoPlayer"][session="'+data.session+'"]');
					console.log(arr);
					if (arr && arr[0]){
						var container = arr[0].parentElement || arr[0].parentNode;
						console.log(container);
						container.setAttribute("data-embed","");
						var e = document.createElement('script');
						e.type = 'text/javascript';
						if (videoObj.embed.indexOf("?")>0)
							videoObj.embed = videoObj.embed + '&autoplay=true';
						else										
							videoObj.embed = videoObj.embed + '?autoplay=true';
						e.src = videoObj.embed + '&targetId=' + container.id +'&rand=' + Math.floor(Math.random() * 100000) + '&srcSession=' + data.session + '&token=' + videoObj.playToken;
						container.innerHTML = '';
						mePlayer.playlist.player.dispose();
						document.body.appendChild(e);
					}
				})
			}
		},50)
	}

