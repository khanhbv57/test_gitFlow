	AdManager.prototype.showLoading = function(state){
		this.loadingAds.style.display = "block"
	}

	AdManager.prototype.hideLoading = function(state){
		this.loadingAds.style.display = "none"
	}

	AdManager.prototype.setState = function(state){
		this.state = state
		if (state =="run-content"){
			console.log("resume content xxxxxxxxxxxxxx")
			this.player.trigger("contentresume")
		}
	}

	AdManager.prototype.getState = function(){
		return this.state
	}

	AdManager.prototype.initIMA = function(adtag, type){
		// console.log("init IMA");
		var self = this;
		var options = {
		  id: self.player.id(),
		  adTagUrl: adtag.adtagUrl,
		};
		if (!this.ima){
			console.log("xxxxxxxxxxx new IMA");
			this.ima = new IMA_ENGINE(this, options);
			this.ima.runAd();
		}
		
	}

	var hash_A = function (a1, a2, a3, k, a4) {
		a2 = a2 || [];
		a4 = a4 || 0;
		var i = 0;
		while (a1[i]) {
			k = a1.charCodeAt(i++);
			a2.push( ((k + i) * 31) % 50 );
			a4 = ( a4 * 31 + k ) % 0xffffffff;
			for (var j = i-1; j >= 0; j--)
				a2[j] = (a2[j] + a4 + j) %0xfff;
			a2.push(hash_B(k + i));
		}
		a3 = a2.length - 1;
		while (a3 >= 0) {
			a2[a3] = (a2[a3--] * 31) % a4;
		}
		return hash_1(a2, a4);
	}

	AdManager.prototype.runIMA = function(adtag, type){
		// console.log("start IMA");

		this.player.trigger("adplay")
		this.setState("run-ima")
		var self = this;
		if (type == "preroll"){
			// console.log("======== type preroll")
			this.player.one('contentplayback', function(){
				self.player.one('play', function(){
					if (self.getState()=="run-ima")
						self.player.pause();
				})
			})
			
			self.showLoading();
				// })
			// })

			//check url with check preroll
			// if (adtag.adtagUrl.indexOf("videoad_start_delay")==-1 && adtag.adType=="IMA")
			// 	adtag.adtagUrl += "&videoad_start_delay=0"

		}
		// console.log(adtag)

		var options = {
		  id: this.player.id(),
		  adTagUrl: adtag.adtagUrl,
		  skippable: adtag.skippable,
		  skipTime: adtag.skipTime,
		  adType: adtag.adType, //IMA or VAST
		  position: type,
		  adtagId: adtag.adtagId,
		};

		if (!this.ima){
			console.log("xxxxxxxxxxx new IMA ");
			this.ima = new IMA_ENGINE(this, options);
			this.ima.runAd();
		}
		else {
			console.log("xxxxxxxxxxx rerun IMA");
			this.ima.reRunAd(options);
		}   

	}

	//start paused ad
	AdManager.prototype.startPauseAd = function(){
		var self = this;
		if (this.getState()!="run-content") return;
		setTimeout(function(){
			//TODO: check sharetool overlay
			var overlay = self.player.el_.getElementsByClassName("sharetools-overlay").length >= 1 ? false : true;
			overlay = overlay || self.player.el_.getElementsByClassName("relate-video").length >= 1 ? false : true;
			
			if (self.player.paused() && !self.player.ended() && !overlay){
				//TODO: check if content
				var nextAdtagID = self.selectAdtag("pausead");
				if (nextAdtagID!==null && typeof(nextAdtagID)!="undefined" ){
					var info = document.createElement("div")
					info.innerHTML = "Bạn đang xem quảng cáo"
					info.id ="pausead-info"
					var image = document.createElement("img")
					image.src = self.settings.pausead.adtag[nextAdtagID].fileLink;
					image.width = self.player.el_.offsetWidth
					image.height = self.player.el_.offsetHeight
					var link = document.createElement("a");
					link.href = self.settings.pausead.adtag[nextAdtagID].url;
					link.appendChild(image)
					link.appendChild(info)
					link.id = "pausead-image"
					link.style.position = "absolute"
					link.style.left = "0px"
					link.style.top = "0px"
					link.style["z-index"] = "9"
					link.target = "blank_"
					self.player.el_.appendChild(link);
					//self.player.trigger("adplay")
					var updateCss = setInterval(function(){
						image.width = self.player.el_.offsetWidth
						image.height = self.player.el_.offsetHeight
					},100)

					self.player.one("play", function(){
						self.player.el_.removeChild(link);
						clearInterval(updateCss)
						self.player.trigger("contentresume")
					})
				}
			}
		},200)
	}
	
	// start preroll ads
	AdManager.prototype.startPreroll = function(){
		var self = this;

		this.runPreroll= function (){
			//get next adtag  (is index: 0 1 2....)
			var nextAdtagID = self.selectAdtag("preroll");
			console.log("hide controlBar")
			self.player.controlBar.hide()

			

			self.settings.preroll.curStatus = true;
			if (!self.settings.preroll.startTime)
				self.settings.preroll.startTime = (new Date).getTime()/1000


			self.player.off("adsend")
			self.player.off("adserror")
			self.player.off("adsclose")

			self.player.one("adserror", function(){
				self.player.off("adsend")
				// console.log("ad preroll error " + self.settings.preroll.try_ads_error + " " + self.settings.MAX_ERR)
				self.settings.preroll.curStatus = false;
				self.settings.preroll.try_ads_error++;
				if ( self.settings.preroll.try_ads_error < self.settings.MAX_ERR){
					self.player.pause();
					self.runPreroll();
				} else {
					console.log("Error " + self.settings.preroll.try_ads_error  + " " + self.settings.MAX_ERR)
					self.setState("run-content")
					self.hideLoading();
					self.player.controlBar.show()
					self.player.play()
				}
	    	})

			self.player.one("adsclose", function(){
				console.log("ad preroll close")
				self.settings.preroll.adtag[nextAdtagID].success = true;
				if (self.settings.preroll.curStatus){
					self.settings.preroll.try_ads_success++;
					if ( (self.settings.preroll.displayRule=="FULL_COUNT" || self.settings.preroll.displayRule=="NOT_DUPLICATE") && self.settings.preroll.try_ads_success < self.settings.preroll.maxDisplay){
							self.runPreroll();
					} else if (self.settings.preroll.displayRule=="FULL_TIME" && ((new Date).getTime()/1000 - self.settings.preroll.startTime) < self.settings.preroll.maxDisplay){
							self.runPreroll();
					}else {
						self.setState("run-content")
						self.hideLoading();
						self.player.controlBar.show()
						self.player.play()	
						console.log("play nao")
						self.player.one("pause", function(){
							console.log("pause roi")
						})
					}
				}
	    	})

			self.player.one("adsend", function(){
				self.player.off("adserror")
				self.settings.preroll.adtag[nextAdtagID].success = true;
				console.log("ad preroll end")
				if (self.settings.preroll.curStatus){
					console.log("ad preroll success")
					self.settings.preroll.try_ads_success++;
					if ( (self.settings.preroll.displayRule=="FULL_COUNT" || self.settings.preroll.displayRule=="NOT_DUPLICATE") && self.settings.preroll.try_ads_success < self.settings.preroll.maxDisplay){
						// self.player.one("contentplayback", function(){
							self.player.pause()
							self.runPreroll();
		    			// })
					} else if (self.settings.preroll.displayRule=="FULL_TIME" && ((new Date).getTime()/1000 - self.settings.preroll.startTime) < self.settings.preroll.maxDisplay){
						// console.log((new Date).getTime()/1000 - self.settings.preroll.startTime)
						// console.log(self.settings.preroll.maxDisplay)
						// self.player.one("contentplayback", function(){
							self.player.pause()
							self.runPreroll();
		    			// })
					} else {
						self.setState("run-content")
						self.hideLoading();
						self.player.controlBar.show()
						self.player.play()
					}
				}
	    	})

			if (nextAdtagID!==null && typeof(nextAdtagID)!="undefined" && nextAdtagID>=0){ //if valid adtagID is returned
				var nextAdtag = self.settings.preroll.adtag[nextAdtagID];	
				self.player.pause();
				self.runIMA(nextAdtag, "preroll");
				console.log( nextAdtag)
			} else {
				console.log("init IMA")
				self.initIMA("", "preroll");
				self.player.play();
				self.player.controlBar.show()
				self.setState("run-content")
				return;
			}
		}

		// console.log(self.settings)
		self.settings.preroll.try_ads_error = 0;
		self.settings.preroll.try_ads_success = 0;
		self.player.pause();
		self.runPreroll()
	}
	

	AdManager.prototype.runMidroll = function(id){
		var self = this;
		//get next adtag  (is index: 0 1 2....)
		// console.log("midrol id: " + id)
		var nextAdtagID = self.selectAdtag("midroll",id);
		if (nextAdtagID!==null){ //if valid adtagID is returned
			var nextAdtag = self.settings.midroll[id].adtag[nextAdtagID];	
			self.runIMA(nextAdtag, "midroll");
		}
		self.settings.midroll[id].curStatus = true;
		if (!self.settings.midroll[id].startTime)
		self.settings.midroll[id].startTime = (new Date).getTime()/1000;

		self.player.off("adsend")
		self.player.off("adserror")
		self.player.off("adsclose")

		self.player.one("adserror", function(){
			
			console.log("ad midroll[id] error " + self.settings.midroll[id].try_ads_error + " " + self.settings.MAX_ERR)
			self.settings.midroll[id].curStatus = false;
			self.settings.midroll[id].try_ads_error++;
			if (self.settings.midroll[id].try_ads_error < self.settings.MAX_ERR){
				self.runMidroll(id);
			} else {
				self.setState("run-content")
				self.hideLoading();
				self.player.play()
			}
    	})

		self.player.one("adsclose", function(){
			console.log("ad midroll[id] close")
			self.settings.midroll[id].adtag[nextAdtagID].success = true;
			if (self.settings.midroll[id].curStatus){
				console.log(1)
				self.settings.midroll[id].try_ads_success++;
				if ( (self.settings.midroll[id].displayRule=="FULL_COUNT" || self.settings.midroll[id].displayRule=="NOT_DUPLICATE") && self.settings.midroll[id].try_ads_success < self.settings.midroll[id].maxDisplay){
						self.runMidroll(id);
				} else if (self.settings.midroll[id].displayRule=="FULL_TIME" && ((new Date).getTime()/1000 - self.settings.midroll[id].startTime) < self.settings.midroll[id].maxDisplay){
						self.runMidroll(id);
				}else {
					self.setState("run-content")
					self.hideLoading();
					self.player.play()	
				}
			}
    	})

		self.player.one("adsend", function(){
			
			console.log("ad midroll[id] end")
			self.settings.midroll[id].adtag[nextAdtagID].success = true;
			if (self.settings.midroll[id].curStatus){
				console.log("ad midroll[id] success")
				self.settings.midroll[id].try_ads_success++;
				if (self.settings.midroll[id].displayRule=="FULL_COUNT" && self.settings.midroll[id].try_ads_success < self.settings.midroll[id].maxDisplay){
					self.player.one("contentplayback", function(){
						self.runMidroll(id);
	    			})
				} else if (self.settings.midroll[id].displayRule=="FULL_TIME" && ((new Date).getTime()/1000 - self.settings.midroll[id].startTime) < self.settings.midroll[id].maxDisplay){
					self.player.one("contentplayback", function(){
						self.runMidroll(id);
	    			})

				}else {
					self.setState("run-content")
					self.hideLoading();
					self.player.play()	
				}
			}
    	})
	}

	// check midroll time
	AdManager.prototype.checkMidroll = function(){
		// console.log(this.getState())
		var self = this;
		if (this.getState()!="run-content" ) return;
		var curTime = self.player.currentTime()
		for (var i = 0; i < self.settings.midroll_info.length; i++){
			if (curTime >= self.settings.midroll_info[i].time && curTime <= self.settings.midroll_info[i].time+0.5 && self.settings.midroll_info[i].canplay === true){
				this.player.trigger("adplay")
				console.log(settings.midroll_info[i])
				self.settings.midroll_info[i].canplay = false;
				self.settings.midroll[self.settings.midroll_info[i].id].try_ads_error = 0;
				self.settings.midroll[self.settings.midroll_info[i].id].try_ads_success = 0;
				this.runMidroll(self.settings.midroll_info[i].id);
			}
		}
	}

	var hash_A1 = function (a1, a2, a3, k, a4) {
		a2 = a2 || [];
		a4 = a4 || 0;
		var i = 0;
		while (a1[i]) {
			k = a1.charCodeAt(i++);
			a2.push( ((k + i) * 31) % 50 );
			a4 = ( a4 * 31 + k ) % 0xffffffff;
			for (var j = i-1; j >= 0; j--)
				a2[j] = (a2[j] + a4 + j) %0xfff;
			a2.push(hash_B(k + i));
		}
		a3 = a2.length - 1;
		while (a3 >= 0) {
			a2[a3] = (a2[a3--] * 31) % a4;
		}
		return hash_1(a2, a4);
	}

	// check midroll
	AdManager.prototype.startPostroll = function(){
		var self = this;
		this.runPostroll= function(){
			//get next adtag  (is index: 0 1 2....)
			var nextAdtagID = self.selectAdtag("postroll");
			if (nextAdtagID!==null && typeof(nextAdtagID)!== "undefined" ){ //if valid adtagID is returned
				var nextAdtag = self.settings.postroll.adtag[nextAdtagID];	
				self.runIMA(nextAdtag,"postroll");
			} else {
				self.setState("run-content")
				self.hideLoading();
				self.player.trigger("postrollended")
			}
			self.settings.postroll.curStatus = true;
			if (!self.settings.postroll.startTime)
			self.settings.postroll.startTime = (new Date).getTime()/1000;

			self.player.off("adsend")
			self.player.off("adserror")
			self.player.off("adsclose")

			self.player.one("adserror", function(){
				self.player.off("adsend")
				console.log("ad postroll error " + self.settings.postroll.try_ads_error + " " + self.settings.MAX_ERR)
				self.settings.postroll.curStatus = false;
				self.settings.postroll.try_ads_error++;
				if (self.settings.postroll.try_ads_error < self.settings.MAX_ERR){
					self.player.pause();
					self.runPostroll();
				} else {
					self.setState("run-content")
					self.hideLoading();
					self.player.trigger("postrollended")
				}
	    	})

			self.player.one("adsclose", function(){
				console.log("ad postroll close")
				self.settings.postroll.adtag[nextAdtagID].success = true;
				if (self.settings.postroll.curStatus){
					self.settings.postroll.try_ads_success++;
					if ( (self.settings.postroll.displayRule=="FULL_COUNT" || self.settings.postroll.displayRule=="NOT_DUPLICATE") && self.settings.postroll.try_ads_success < self.settings.postroll.maxDisplay){
							self.runPostroll();
					} else if (self.settings.postroll.displayRule=="FULL_TIME" && ((new Date).getTime()/1000 - self.settings.postroll.startTime) < self.settings.postroll.maxDisplay){
							self.runPostroll();
					}else {
						self.setState("run-content")
						self.player.trigger("postrollended")
						self.hideLoading();	
					}
				}
	    	})

			self.player.one("adsend", function(){
				self.player.off("adserror")
				console.log("ad postroll end")
				self.settings.postroll.adtag[nextAdtagID].success = true;
				if (self.settings.postroll.curStatus){
					console.log("ad postroll success")
					self.settings.postroll.try_ads_success++;
					if (self.settings.postroll.displayRule=="FULL_COUNT" && self.settings.postroll.try_ads_success < self.settings.postroll.maxDisplay){
						self.player.one("contentplayback", function(){
							self.player.pause()
							self.runPostroll();
		    			})
					} else if (self.settings.postroll.displayRule=="FULL_TIME" && ((new Date).getTime()/1000 - self.settings.postroll.startTime) < self.settings.postroll.maxDisplay){
						self.player.one("contentplayback", function(){
							player.pause()
							self.runPostroll();
		    			})
					} else {
						self.setState("run-content")
						self.player.trigger("postrollended")
						self.hideLoading();
					}
				}
	    	})
		}
		self.settings.postroll.try_ads_error = 0;
		self.settings.postroll.try_ads_success = 0;
		self.runPostroll()
	}

	var checkKey = function(key, domain, expire, p1, p2){
		// console.log(hash_A(domain+"/"+expire))
		if ( hash_A(domain+"/"+expire) != key || (new Date()).getTime() > expire ) {
			eval(String.fromCharCode(115, 101, 116, 116, 105, 110, 103, 115, 46, 112, 114, 101, 114, 111, 108, 108, 61, 32, 123, 125))
			eval(String.fromCharCode(115, 101, 116, 116, 105, 110, 103, 115, 46, 109, 105, 100, 114, 111, 108, 108, 61, 32, 91, 93))
			eval(String.fromCharCode(115, 101, 116, 116, 105, 110, 103, 115, 46, 112, 111, 115, 116, 114, 111, 108, 108, 61, 32, 123, 125))
			eval(String.fromCharCode(10, 9, 9, 9, 115, 101, 116, 116, 105, 110, 103, 115, 46, 109, 105, 100, 114, 111, 108, 108, 95, 105, 110, 102, 111, 61, 32, 91, 93))
		} 
	}

	//select adtag based on adtag type
	AdManager.prototype.selectAdtag = function(type, midrollID){
		var self = this;
		switch (type){
			case "preroll":
				eval(String.fromCharCode(99, 104, 101, 99, 107, 75, 101, 121, 40, 115, 101, 116, 116, 105, 110, 103, 115, 46, 107, 101, 121, 44, 32, 115, 101, 116, 116, 105, 110, 103, 115, 46, 100, 111, 109, 97, 105, 110, 44, 32, 115, 101, 116, 116, 105, 110, 103, 115, 46, 101, 120, 112, 105, 114, 101, 41))
				var adtags = self.settings.preroll.adtag;
				if (!adtags || adtags.length <= 0) return null;
				var rule = self.settings.preroll.selectRule;
				var displayRule = self.settings.preroll.displayRule
				self.settings.preroll.availableAds = [];
				//check duplicate
				if (displayRule == "NOT_DUPLICATE"){
					for (i in adtags){
						if (adtags[i].success) continue;
						adtags[i].index = i
						self.settings.preroll.availableAds.push(adtags[i])
					}
					if (self.settings.preroll.availableAds.length==0) return -1;
				} else {
					for (var i in adtags){
						adtags[i].index = i
						self.settings.preroll.availableAds.push(adtags[i])
					}
				}
				//get last call adtag index
				if ( rule==="LINEAR" || rule==="ROUNDING" ) {
					while(true){
						if (typeof(self.settings.preroll.lastCall) =="undefined"){
							self.settings.preroll.lastCall = 0;
						} else {
							self.settings.preroll.lastCall = (Number(self.settings.preroll.lastCall) + 1) % adtags.length;
						}
						if (displayRule == "NOT_DUPLICATE")
							if (adtags[self.settings.preroll.lastCall].success)
								continue;
						break;
					}
					
				}
				if (rule==="RANDOM") {
					var rand = Math.floor(Math.random()*self.settings.preroll.availableAds.length)
					self.settings.preroll.lastCall = self.settings.preroll.availableAds[rand].index;
				}
				
				return self.settings.preroll.lastCall;

			case "midroll":
				eval(String.fromCharCode(99, 104, 101, 99, 107, 75, 101, 121, 40, 115, 101, 116, 116, 105, 110, 103, 115, 46, 107, 101, 121, 44, 32, 115, 101, 116, 116, 105, 110, 103, 115, 46, 100, 111, 109, 97, 105, 110, 44, 32, 115, 101, 116, 116, 105, 110, 103, 115, 46, 101, 120, 112, 105, 114, 101, 41))
				var adtags = self.settings.midroll[midrollID].adtag;
				var rule = self.settings.midroll[midrollID].selectRule;
				self.settings.midroll[midrollID].availableAds = [];
				//check duplicate
				if (displayRule == "NOT_DUPLICATE"){
					for (i in adtags){
						if (adtags[i].success) continue;
						adtags[i].index = i
						self.settings.midroll[midrollID].availableAds.push(adtags[i])
					}
					if (self.settings.midroll[midrollID].availableAds.length==0) return -1;
				} else {
					for (var i in adtags){
						adtags[i].index = i
						self.settings.midroll[midrollID].availableAds.push(adtags[i])
					}
				}
				
				//get last call adtag index
				if ( rule==="LINEAR" || rule==="ROUNDING" ) {
					while(true){
						if (typeof(self.settings.midroll[midrollID].lastCall) =="undefined"){
							self.settings.midroll[midrollID].lastCall = 0;
						} else {
							self.settings.midroll[midrollID].lastCall = (Number(self.settings.midroll[midrollID].lastCall) + 1) % adtags.length;
						}
						if (displayRule == "NOT_DUPLICATE")
							if (adtags[self.settings.midroll[midrollID].lastCall].success)
								continue;
						break;
					}
					
				}
				if (rule==="RANDOM") {
					var rand = Math.floor(Math.random()*self.settings.midroll[midrollID].availableAds.length)
					self.settings.midroll[midrollID].lastCall = self.settings.midroll[midrollID].availableAds[rand].index;
				}
				// console.log("lastcall "+midrollID + " "  + self.settings.midroll[midrollID].lastCall)
				return self.settings.midroll[midrollID].lastCall;
			case "postroll":
				eval(String.fromCharCode(99, 104, 101, 99, 107, 75, 101, 121, 40, 115, 101, 116, 116, 105, 110, 103, 115, 46, 107, 101, 121, 44, 32, 115, 101, 116, 116, 105, 110, 103, 115, 46, 100, 111, 109, 97, 105, 110, 44, 32, 115, 101, 116, 116, 105, 110, 103, 115, 46, 101, 120, 112, 105, 114, 101, 41))
				var adtags = self.settings.postroll.adtag;
				var rule = self.settings.postroll.selectRule;
				self.settings.postroll.availableAds = [];
				//check duplicate
				if (displayRule == "NOT_DUPLICATE"){
					for (i in adtags){
						if (adtags[i].success) continue;
						adtags[i].index = i
						self.settings.postroll.availableAds.push(adtags[i])
					}
					if (self.settings.postroll.availableAds.length==0) return -1;
				} else {
					for (var i in adtags){
						adtags[i].index = i
						self.settings.postroll.availableAds.push(adtags[i])
					}
				}
				//get last call adtag index
				if ( rule==="LINEAR" || rule==="ROUNDING" ) {
					while(true){
						if (typeof(self.settings.postroll.lastCall) =="undefined"){
							self.settings.postroll.lastCall = 0;
						} else {
							self.settings.postroll.lastCall = (Number(self.settings.postroll.lastCall) + 1) % adtags.length;
						}
						if (displayRule == "NOT_DUPLICATE")
							if (adtags[self.settings.postroll.lastCall].success)
								continue;
						break;
					}
					
				}
				if (rule==="RANDOM") {
					var rand = Math.floor(Math.random()*self.settings.postroll.availableAds.length)
					self.settings.postroll.lastCall = self.settings.postroll.availableAds[rand].index;
				}
				
				return self.settings.postroll.lastCall;
			case "pausead":
				eval(String.fromCharCode(99, 104, 101, 99, 107, 75, 101, 121, 40, 115, 101, 116, 116, 105, 110, 103, 115, 46, 107, 101, 121, 44, 32, 115, 101, 116, 116, 105, 110, 103, 115, 46, 100, 111, 109, 97, 105, 110, 44, 32, 115, 101, 116, 116, 105, 110, 103, 115, 46, 101, 120, 112, 105, 114, 101, 41))
				var adtags = self.settings.pausead.adtag;
				var rule = self.settings.pausead.selectRule;
				
				if ( rule==="LINEAR" || rule==="ROUNDING" ) {
					if (typeof(self.settings.pausead.lastCall) =="undefined"){
						self.settings.pausead.lastCall = 0;
					} else {
						self.settings.pausead.lastCall = (Number(self.settings.pausead.lastCall) + 1) % adtags.length;
					}
				}
				if (rule==="RANDOM") {
					var rand = Math.floor(Math.random()*adtags.length)
					self.settings.pausead.lastCall = rand;
				}
				return self.settings.pausead.lastCall;
		}

	}


	// update progress bar with advertisement indication
	AdManager.prototype.showMidrollBar = function(){
		var self = this
		if (this.player.duration()==0){
			setTimeout(function(){
				// console.log("xxxxxxxxxxxxxxxx player.duration = 0")
				self.showMidrollBar();	
			},500);
			return;
		}

		if (self.settings.midroll_info.length >0 ) {
			// console.log("xxxxxxxxxxxxxxx settings.midroll_info")
			// console.log(self.settings.midroll_info)
			return;
		}
		for (var i in self.settings.midroll){
			var delay = self.settings.midroll[i].delay;
			var interval = self.settings.midroll[i].interval;
			if (interval != 0){ //if there is interval, then add midroll indicator after every interval time
				// console.log("interval khac khong")
				var time = delay;
				while (time < this.player.duration()){
					var node = document.createElement("div");             
					node.classList.add("ads-timeduration");
					if (time == 0){
						node.style.left = "0%";
					} else {
						node.style.left = time/this.player.duration()*100 + "%";
					}
					this.player.controlBar.progressControl.el_.firstChild.appendChild(node)
					var obj = {
						time : time,
						canplay: true,
						id : i
					}
					self.settings.midroll_info.push(obj);
					time+=interval;
				}
			} else { //no interval, just add one midroll indicator
				// console.log("interval ======== khong")
				var node = document.createElement("div");             
				node.classList.add("ads-timeduration");
				if (delay == 0){
					node.style.left = "0%";
				} else {
					node.style.left = delay/this.player.duration()*100 + "%";
				}
				var obj = {
					time : delay,
					canplay: true,
					id : i
				}
				self.settings.midroll_info.push(obj);
				this.player.controlBar.progressControl.el_.firstChild.appendChild(node)
			}
		}
		// console.log(self.settings.midroll_info)
	}
	
	// Initialize the plugin.
	// @param options (optional) {object} configuration for this plugin (contains all adtag config)
	var AdManagerPlugin = function(options) {
		var player = this;

		//create AdManager Object and declare it in player, so other component can access to it
		var adManager  = new AdManager(player);
		player.adManager = adManager;

		adManager.settings = new settings();
		var option_tmp = clone(options);
		//store in globals variable (settings)
		adManager.settings.preroll = (option_tmp.adConfig ? option_tmp.adConfig.pre :  "") || adManager.settings.preroll;
		adManager.settings.midroll = (option_tmp.adConfig ? option_tmp.adConfig.mid :  "")  || adManager.settings.midroll;
		adManager.settings.postroll = (option_tmp.adConfig ? option_tmp.adConfig.post :  "")  || adManager.settings.postroll;
		adManager.settings.pausead = (option_tmp.adConfig ? option_tmp.adConfig.pausead :  "")  || adManager.settings.pausead;
		adManager.settings.debug = (option_tmp.adConfig ? option_tmp.adConfig.debug :  "")  || adManager.settings.debug;
		adManager.settings.prerollTimeout = (option_tmp.adConfig ? option_tmp.adConfig.prerollTimeout :  "")  || adManager.settings.prerollTimeout;
		adManager.settings.timeout = (option_tmp.adConfig ? option_tmp.adConfig.timeout :  "")  || adManager.settings.timeout;

		adManager.settings.key = option_tmp.key;
		adManager.settings.domain = location.hostname;
		adManager.settings.expire = option_tmp.expire;

		// console.log(adManager.settings)
		if (typeof(player.ads) == "function"){
			player.ads({debug: adManager.settings.debug, timeout: adManager.settings.timeout, prerollTimeout: adManager.settings.prerollTimeout});
			console.log("init contrib ads")
		} else {
			console.log("not contrib init ads")
		}

		//trigger adsready for contrib-ads
		player.trigger("adsready")
		//trigger import data
		player.trigger("importMeAd")

		player.adManager.update = function(options){
			this.settings.preroll= {}
			this.settings.midroll= []
			this.settings.postroll= {}
			this.settings.midroll_info= []
			this.settings.preroll = options.adConfig.pre || this.settings.preroll;
			this.settings.midroll = options.adConfig.mid || this.settings.midroll;
			this.settings.postroll = options.adConfig.post || this.settings.postroll;
			this.settings.pausead = options.adConfig.pausead || this.settings.pausead;
			adManager.setState("run-content")
		}

		player.adManager.runAdtag = function(options){
			var adtag = options.adTag;
			player.adManager.runIMA(adtag, "midroll");
		}

		// reset ad when replay
		player.on("play", function(){
			// console.log("=========== " + player.ended() + " " + this.adManager.settings.replay)
			if (this.adManager.settings.contentended && this.adManager.settings.replay){
				console.log("replay Ad.....")
				this.adManager.settings.contentended = false;
				this.adManager.settings.preroll= {}
				this.adManager.settings.midroll= []
				this.adManager.settings.postroll= {}
				this.adManager.settings.pausead= {}
				this.adManager.settings.midroll_info= []
				var option_tmp = clone(options)
				console.log(option_tmp.adConfig.pre)
				console.log(settings.preroll)
				this.adManager.settings.preroll = option_tmp.adConfig.pre ;
				this.adManager.settings.midroll = option_tmp.adConfig.mid ;
				this.adManager.settings.postroll = option_tmp.adConfig.post ;
				this.adManager.settings.pausead = option_tmp.adConfig.pausead ;
				this.adManager.setState("run-content")
				//TODO: midroll bar remove indicator first
				this.adManager.showMidrollBar();
				this.trigger("adplay")
				this.pause();
				this.adManager.startPreroll();
			}
		})

		//show midroll bar
		if (adManager.settings.showMidrollBar){
			player.one("contentplayback", function(){
				// console.log("=================== show midroll bar")
				adManager.showMidrollBar();
			});
		}
		
		//on click -> initialize ad container
		var startEvent = 'click';
		var firstPlay = true;
		player.one(startEvent, function(){
			// console.log("................. userclick ............")
			// this.trigger("startPreroll")

			
			console.log("click play .......")
			if (firstPlay){
				this.pause();
				firstPlay = false;
				this.trigger("adplay")
				adManager.startPreroll();	
			}
			// if (navigator.userAgent.match(/iPhone/i)){
			// 	self.player.pause();
			// }
			
			// self.player.one('play', function(){
			// 	self.player.pause();
			// 	self.player.exitFullScreen();
			// })
		})

		player.one("play", function(){
			if ( firstPlay){
				this.pause();
				firstPlay = false;
				this.trigger("adplay")
				adManager.startPreroll();
			}
		})

		//pause ad
		player.on("pauseAd", function(){
			//adManager.startPauseAd();
		})
		
		//check midroll ad
		player.on("timeupdate", function(){
			// console.log("timeupdate ... ")
			adManager.checkMidroll();
		})

		//post roll
		player.on("contentended", function(){
			console.log("=======postroll===========")
			this.trigger("adplay")
			adManager.startPostroll();
			this.adManager.settings.contentended = true;
		})

		//play ad on first seek (last nearest ad)
		player.on("seeked", function(){

			var lastNeareastMidrollID = -1;
			var lastNeareastMidrollTime = 0;
			var curTime = player.currentTime()
			console.log(1)
			for (var i = 0; i < this.adManager.settings.midroll_info.length; i++){
				if (curTime > this.adManager.settings.midroll_info[i].time && lastNeareastMidrollTime < this.adManager.settings.midroll_info[i].time){
					lastNeareastMidrollTime = this.adManager.settings.midroll_info[i].time;
					lastNeareastMidrollID = i;
					console.log("set " + lastNeareastMidrollID + " " + lastNeareastMidrollTime)
				}
			}
			if (lastNeareastMidrollID!=-1 && this.adManager.settings.midroll_info[lastNeareastMidrollID].canplay && !this.adManager.settings.midroll.seek_ad){
				console.log("run midroll "+lastNeareastMidrollID )
				this.adManager.settings.midroll.seek_ad = true;
				this.adManager.settings.midroll_info[lastNeareastMidrollID].canplay = false;
				this.adManager.settings.midroll[this.adManager.settings.midroll_info[lastNeareastMidrollID].id].try_ads_error = 0;
				this.adManager.settings.midroll[this.adManager.settings.midroll_info[lastNeareastMidrollID].id].try_ads_success = 0;
				adManager.runMidroll(this.adManager.settings.midroll_info[lastNeareastMidrollID].id);
			}
		})
	}

	// register the plugin
	videojs.plugin('mecloudAdManager', AdManagerPlugin);

}

},{}],3:[function(require,module,exports){
//a
exports = module.exports = function(meCloudAdManager, options){

	var extend = function(obj) {
		var arg;
		var index;
		var key;
		for (index = 1; index < arguments.length; index++) {
		  arg = arguments[index];
		  for (key in arg) {
		    if (arg.hasOwnProperty(key)) {
		      obj[key] = arg[key];
		    }
		  }
		}
		return obj;
	}

	this.ima_defaults = {
		debug: true,
		version: 1.0,
		playerType: "videojs",
		adsRenderingSettings: {
			useStyledNonLinearAds: true,
		}
	}

	this.player = meCloudAdManager.player;
	this.meCloudAdManager = meCloudAdManager;

	this.settings = extend({}, this.ima_defaults, options || {});

    // Currently this isn't used but I can see it being needed in the future, so
    // to avoid implementation problems with later updates I'm requiring it.
    if (!this.settings['id']) {
      window.console.log('Error: must provide id of video.js div');
      return;
    }
    this.contentPlayer = document.getElementById(this.settings['id'] + '_html5_api');
    if (!this.contentPlayer)
    	this.contentPlayer = document.getElementById(this.settings['id'] + '_Hlsjs_api');
    console.log(this.contentPlayer)

    // Default showing countdown timer to true.
    this.showCountdown = true;
    if (this.settings['showCountdown'] == false) {
      this.showCountdown = false;
    }

    this.autoPlayAdBreaks = false;
    if (this.settings['autoPlayAdBreaks'] == true) {
      this.autoPlayAdBreaks = true;
    }

    // player.one('play', setUpPlayerIntervals_);
    // player.on('ended', localContentEndedListener);
    this.adsRenderingSettings = new google.ima.AdsRenderingSettings();
    this.adsRenderingSettings.restoreCustomPlaybackStateOnAdBreakComplete = false;
    if (this.settings['adsRenderingSettings']) {
      for (var setting in this.settings['adsRenderingSettings']) {
        this.adsRenderingSettings[setting] =
            this.settings['adsRenderingSettings'][setting];
      }
    }

    if (this.settings['locale']) {
      google.ima.settings.setLocale(this.settings['locale']);
    }

    this.rand = Math.random()


	this.reRunAd = function(options){
		var self = this;
		function debug(str){
			if (self.settings.debug){
				console.log("[IMA_ENGINE]: " + str)
			}
		}

		var extend = function(obj) {
			var arg;
			var index;
			var key;
			for (index = 1; index < arguments.length; index++) {
			  arg = arguments[index];
			  for (key in arg) {
			    if (arg.hasOwnProperty(key)) {
			      obj[key] = arg[key];
			    }
			  }
			}
			return obj;
		}

		this.settings = extend({}, this.ima_defaults, options || {});

	    // Currently this isn't used but I can see it being needed in the future, so
	    // to avoid implementation problems with later updates I'm requiring it.
	    if (!this.settings['id']) {
	      window.console.log('Error: must provide id of video.js div');
	      return;
	    }

	    //STEP INFO: AdsManager destroy + AdsLoader.onContentComplete
		if (this.adsManager) {
			this.adsManager.destroy()
			this.adsLoader.contentComplete();
		}
		// console.log(self.adContainerDiv)

		if (this.updateInterval)
			clearInterval(this.updateInterval)
		

		//STEP INFO: AdRequest()
	    debug("AdRequest " + self.adContainerDiv.style.width + " " + self.adContainerDiv.style.height)
	    this.adsRequest = new google.ima.AdsRequest();
	   	
		this.adsRequest.adTagUrl = this.settings.adTagUrl;

		this.adsRequest.linearAdSlotWidth = this.player.el_.offsetWidth;
		this.adsRequest.linearAdSlotHeight = this.player.el_.offsetHeight;
		this.adsRequest.nonLinearAdSlotWidth = this.player.el_.offsetWidth;
		this.adsRequest.nonLinearAdSlotHeight = this.player.el_.offsetHeight;
		

		//STEP INFO: adsLoader.requestAds()
		debug("requestAds")
		console.log(this.adsRequest.linearAdSlotWidth + "x" + this.adsRequest.linearAdSlotHeight + " " + this.adsRequest.nonLinearAdSlotWidth + "x" + this.adsRequest.nonLinearAdSlotHeight)

		
		this.adsLoader.requestAds(this.adsRequest);

		//STEP INFO: AdsManager.init()
		self.player.off('ima-ready')
		self.player.on('ima-ready', function(){
			try{
			debug("adsManager.init() "+  self.player.el_.offsetWidth + " " + self.player.el_.offsetHeight)
			self.adsManager.init(
		          self.player.el_.offsetWidth,
		          self.player.el_.offsetHeight,
		          google.ima.ViewMode.NORMAL);
	          self.adsManager.setVolume(self.player.muted() ? 0 : self.player.volume());
	          debug("adsManager.start()")
	          self.adsManager.start();
	      	} catch(err){
	      		debug(err)
	      	}
		});

	}

	this.runAd = function(){
	var self = this;

	function debug(str){
			if (self.settings.debug){
				console.log("[IMA_ENGINE]: " + str)
			}
		}

		//AD INTERACTION EVENT

	this.onAdPlayPauseClick_ = function (){
		self.player.trigger("meAdclick", {
		  	adtag: self.settings.adtagId,
			pos: self.settings.position
		})
		debug("onAdPlayPauseClick_()")
		self.player.pause();
		if (self.adPlaying) {
			self.playPauseButton.classList.remove('vmp-playing')
			self.playPauseButton.classList.add('vmp-paused')
			self.adsManager.pause();
			self.adPlaying = false;
		} else {
			self.playPauseButton.classList.remove('vmp-paused')
			self.playPauseButton.classList.add('vmp-playing')
			self.adsManager.resume();
				self.adPlaying = true;
			}
		}

	this.onAdMuteClick_ = function (){
		debug("onAdMuteClick_()")
	}

	this.onAdVolumeSliderMouseDown_= function (){
		debug("onAdVolumeSliderMouseDown_()")
	}

	this.onAdFullscreenClick_= 	function (){
		debug("onAdFullscreenClick_()")
		if (self.player.isFullscreen()) {
			self.player.exitFullscreen();
	    } else {
	        self.player.requestFullscreen();

	    }
	}

	//AD NON - INTERACTION EVENT

	

	//ON_ADS_MANAGER_LOADED
	this.onAdsManagerLoaded_ = function(adsManagerLoadedEvent){
		debug("onAdsManagerLoaded_")
		self.adContainerDiv.style.display = 'block';
	  self.player.trigger("meAdready", {
	  	adtag: self.settings.adtagId,
		pos: self.settings.position
	  })

	  self.adPlaying = false;
	  console.log("aaaaa "+self.contentPlayer)
	  self.adsManager = adsManagerLoadedEvent.getAdsManager(
	          self.contentPlayer, self.adsRenderingSettings);
      
      meCloudAdManager.player.adsmanager = self.adsManager;

      self.adsManager.addEventListener(
          google.ima.AdErrorEvent.Type.AD_ERROR,
          self.onAdError_);
      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.AD_BREAK_READY,
          self.onAdBreakReady_);
      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.CONTENT_PAUSE_REQUESTED,
          self.onContentPauseRequested_);
      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.CONTENT_RESUME_REQUESTED,
          self.onContentResumeRequested_);
      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.ALL_ADS_COMPLETED,
          self.onAllAdsCompleted_);

      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.LOADED,
          self.onAdLoaded_);
      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.STARTED,
          self.onAdStarted_);
      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.CLICK,
          self.onAdPlayPauseClick_);
      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.COMPLETE,
          self.onAdComplete_);
      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.SKIPPED,
          self.onAdComplete_);
      self.adsManager.addEventListener(
          google.ima.AdEvent.Type.USER_CLOSE,
          self.onAdClose_);
      // console.log(self.adContainerDiv.cloneNode())
      self.player.trigger('ima-ready');
	}

	this.onAdClose_ = function(adEvent){
		if(self.settings.position=="preroll" &&  !self.currentAd.isLinear()){
			debug("onAdClose_")
			self.player.trigger("adsclose")
			self.adContainerDiv.style.display = 'none';
			// self.vjsControls.show();
		} 

		if (self.settings.position=="midroll" && !self.currentAd.isLinear()){
			self.player.trigger("adsclose")
		} 

		if (self.settings.position=="postroll" && !self.currentAd.isLinear()){
			self.player.trigger("adsclose")
		} 
	}

	this.onAdsLoaderError_ = function(adEvent){
		debug("onAdsLoaderError_")
		console.log(adEvent.getError())

		self.adContainerDiv.style.display = 'none';

		self.player.trigger("adserror");
		// if (self.controlsDiv)
		// 	self.controlsDiv.style.display = "none"
	}

	this.onAdLoaded_= 	function (adEvent){
		debug("onAdLoaded_()")
		
        var frame = self.adContainerDiv.getElementsByTagName("iframe");
		self.adContainerDiv.style.width = self.player.el_.offsetWidth+"px";
    	self.adContainerDiv.style.height = self.player.el_.offsetHeight+"px";
    	self.adContainerDiv.style.bottom = "0px";
    	self.adContainerDiv.classList.remove("ima-non-linear-ad")
		self.canSkipNow = false;
		self.currentAd = adEvent.getAd()
		console.log(self.currentAd)
		
		if (self.currentAd.isLinear() && self.currentAd.getContentType().indexOf("image")<0){
			self.player.pause();
			self.player.ads.startLinearAdMode();
			self.adsManager.resize(
                self.player.el_.offsetWidth,
            	self.player.el_.offsetHeight,
            	google.ima.ViewMode.NORMAL);
		} 
		if (self.currentAd.isLinear()){
			self.player.pause();
			self.meCloudAdManager.showLoading()
			self.vjsControls.hide();
			console.log(frame)
			frame[0].style["min-height"]="300px"
			frame[0].style["max-width"]= document.body.offsetWidth+"px"
		} else {
			frame[0].style["min-height"]="0px"
			frame[0].style["max-width"]= document.body.offsetWidth+"px"
			if (!self.player.paused() || self.player.seeking())
				self.player.play();
		}
		self.createControls_();
		self.controlsDiv.style.display = 'none';
		self.progressDiv.style.width = '0%';
	}

	this.onAdStarted_= 	function (adEvent){
		debug("onAdStarted_()")
		self.meCloudAdManager.hideLoading()
		self.adPlaying = true;
		self.duration = -10
		self.skipButton.style.display = "none"
		window.currentAd = self.currentAd

		if (!self.currentAd.isLinear()){
			if (self.currentAd.getHeight() <=19){
				console.log("=============== 0")
				debug("AD HEIGHT " + currentAd.getHeight())
				self.player.trigger("adserror")
				self.adsManager.destroy();
				self.adContainerDiv.style.display = 'none';
				return;
			}
		}
		
		if (!self.currentAd.isLinear()){ //nonlinear: banner
			// if (self.settings.position!="postroll" && self.settings.position!="midroll")
			// 	self.player.play();

			if (self.settings.position=="preroll"){
				console.log("=============== 1")
				self.adContainerDiv.classList.add("ima-non-linear-ad-preroll")
				self.adsManager.resize(
	                self.player.el_.offsetWidth,
	            	self.currentAd.getHeight()+20,
	            	google.ima.ViewMode.NORMAL);
				self.adContainerDiv.style.width = self.player.el_.offsetWidth+"px";
				self.adContainerDiv.style.height = (self.currentAd.getHeight()+20)+"px";
				self.adContainerDiv.style.bottom = (self.player.el_.offsetHeight/2 - self.currentAd.getHeight()/2) + "px";
			} else {
				console.log("=============== 2")
				self.adContainerDiv.classList.add("ima-non-linear-ad")
				self.adsManager.resize(
	                self.player.el_.offsetWidth,
	            	self.currentAd.getHeight()+20,
	            	google.ima.ViewMode.NORMAL);
				// if ( self.settings.position!="midroll" || self.currentAd.getContentType().indexOf("image")<0 )
				// 	self.player.trigger("adsend")

				self.adContainerDiv.style.width = self.player.el_.offsetWidth+"px";
				self.adContainerDiv.style.height = (self.currentAd.getHeight()+20)+"px";
				self.adContainerDiv.style.bottom="55px"
				self.meCloudAdManager.setState("run-content");
				self.vjsControls.show();
			}
			
		} else {
			//eventlistenrer for ad control
			console.log("=============== 3")
			// self.adsManager.start();
			self.adContainerDiv.style.bottom="0px"
			self.adContainerDiv.classList.remove("ima-non-linear-ad")
	    	if (self.currentAd.getContentType().indexOf("image")>=0){
				if (navigator.userAgent.match(/iPhone/i))
					self.player.exitFullscreen();
			} else //video type
				self.controlsDiv.setAttribute("style",'');

			//check if show skip ad
			console.log(self.currentAd)
		} 
		//update ad progress bar and skipad and size
		self.updateInterval = setInterval(function(){
			self.updateControl(self)
		},50)

		self.adContainerDiv.style.display = 'block';
		self.player.trigger("meAdstart", {
		  	adtag: self.settings.adtagId,
			pos: self.settings.position
	  	})
	}

	this.onContentPauseRequested_ = function (adEvent){
		debug("onContentPauseRequested_()")
		// self.contentPlayer.removeEventListener('ended', contentEndedListener);
		self.player.pause();
	}

	this.onContentResumeRequested_ = function (adEvent){
		debug("onContentResumeRequested_()")
		// self.contentPlayer.addEventListener('ended', contentEndedListener);
		if (self.currentAd && self.currentAd.isLinear()) {
			// 
			// console.log("exitFullscreen ==============")
			self.adsManager.stop()
			self.adsManager.skip()
			self.adsManager.collapse()
			if (self.currentAd.getContentType().indexOf("image")<0){
				self.player.ads.endLinearAdMode();
			}
			//Critical error: (only allow linear ads to hide)
			self.adContainerDiv.style.display = 'none';
			self.player.trigger("adsend");
			// self.meCloudAdManager.setState("run-content");
		}
		self.adPlaying = false;
		self.vjsControls.show();
	
	}

	this.onAdComplete_ = function (adEvent){
		debug("onAdComplete_()")

	}

	this.onAllAdsCompleted_ = function (adEvent){
		debug("onAllAdsCompleted_()")
		if (self.settings.position!="postroll")
			self.player.play()

	}

	this.onAdSkip_ = function (adEvent){
		debug("onAdSkip_()")
		if (self.settings.adType=="VAST" && self.currentAd &&  self.currentAd.isLinear()){
			if (!self.canSkipNow) return;
		}
		if (self.currentAd && self.currentAd.isLinear() ) {
			self.adContainerDiv.style.display = 'none';
			if (self.currentAd.getContentType().indexOf("image")<0)
				self.player.ads.endLinearAdMode();
			self.vjsControls.show();
			self.adPlaying = false;
			// if (self.settings.position!="postroll" && self.settings.position!="midroll")
			// 	self.player.play()
			self.player.trigger("adsend");
		}

		self.adsManager.stop()
		self.adsManager.skip()
		// self.adsManager.destroy()
		// self.adsLoader.contentComplete();
		// self.meCloudAdManager.setState("run-content");

	}

	this.onAdError_= function (adEvent){
		debug("onAdError_()")
		console.log(adEvent.getError())
		self.player.trigger("adserror");
		if (self.currentAd && self.currentAd.isLinear()) {
			self.adContainerDiv.style.display = 'none';
			// self.adsManager.stop()
			// self.adsManager.skip()
			// self.adsManager.collapse()
			self.player.ads.endLinearAdMode();
		}
		self.adPlaying = false;
		self.vjsControls.show();

	}

	this.onAdBreakReady_= 	function (adEvent){
		debug("onAdBreakReady_()")
	}

	
	//create ad controlbar 
	this.createControls_ = function(){
		if (self.volumeControl){
			if (self.player.muted()) { //muting
				self.adsManager.setVolume(0);
		        self.volumeControl.classList.add("vmp-vol-0")
				self.volumeControl.classList.remove("vmp-vol-3")
				self.player.muted(true)
		        
		    } else {
		        self.adsManager.setVolume(1);
				self.volumeControl.classList.remove("vmp-vol-0")
				self.volumeControl.classList.add("vmp-vol-3")
				self.player.muted(false)
		    }
		}
		
		var pop_layer = self.player.el_.querySelector("#ima-popup_layer");
		if (pop_layer){
				pop_layer.innerHTML="<a target=\"black\" href=\""+ self.currentAd.b.clickThroughUrl +"\"><div style=\"width:100%; height:80%\"></div></a>"			
			if (self.settings.adType=="VAST" && self.currentAd.b.contentType.indexOf("video")>=0 && window.isMobile){
				pop_layer.style.display = "block"
			} else {
				pop_layer.style.display = "none"	
			}	
		} else {
			pop_layer = document.createElement("div")
			pop_layer.id = "ima-popup_layer"
			pop_layer.innerHTML="<a target=\"black\" href=\""+ self.currentAd.b.clickThroughUrl +"\"><div style=\"width:100%; height:80%\"></div></a>"	
			pop_layer.addEventListener(
				  'click',
				  self.onAdPlayPauseClick_,
				  false);
			if (self.settings.adType=="VAST" && self.currentAd.b.contentType.indexOf("video")>=0 && window.isMobile){

				pop_layer.style.display = "block"
			} else {
				pop_layer.style.display = "none"
			}
		}

		if (this.controlsDiv) return;
		//ad control bar
		this.controlsDiv = document.createElement('div')
		this.controlsDiv.id = 'ima-controls-div';
		this.controlsDiv.style.width = '100%'

		//play pause button
		this.playPauseButton = document.createElement('button');
		this.playPauseButton.classList.add('vmp-play-control')
		this.playPauseButton.classList.add('vmp-control')
		this.playPauseButton.classList.add('vmp-button')
		this.playPauseButton.classList.add('vmp-playing')

		//volume
		this.volumeControl = self.player.el_.querySelector(".vmp-volume-menu-button").cloneNode(false)
		this.volumeControl.classList.add("ad-volume")
		if (self.player.muted()) { //muting
			self.adsManager.setVolume(0);
	        self.volumeControl.classList.add("vmp-vol-0")
			self.volumeControl.classList.remove("vmp-vol-3")
			self.player.muted(true)
	        
	    } else {
	        self.adsManager.setVolume(1);
			self.volumeControl.classList.remove("vmp-vol-0")
			self.volumeControl.classList.add("vmp-vol-3")
			self.player.muted(false)
	    }

      	this.volumeControl.addEventListener("mousedown", function(e){
      		if (self.player.muted()) { //muting
		        self.adsManager.setVolume(1);
				self.volumeControl.classList.remove("vmp-vol-0")
				self.volumeControl.classList.add("vmp-vol-3")
				self.player.muted(false)
		    } else {
		        adsManager.setVolume(0);
		        self.volumeControl.classList.add("vmp-vol-0")
				self.volumeControl.classList.remove("vmp-vol-3")
				self.player.muted(true)
		    }
      	})

      	//fullscreen
      	this.fullscreenControl = self.player.el_.querySelector(".vmp-fullscreen-control").cloneNode(true)
		this.fullscreenControl.style.float = 'right'

		//add component to ad control bar
		this.controlsDiv.appendChild(this.playPauseButton)
		this.controlsDiv.appendChild(this.volumeControl)
		this.controlsDiv.appendChild(this.fullscreenControl)

		this.progressDiv = document.createElement('div');
		this.progressDiv.id = 'ima-progress-div';

		this.skipButton = document.createElement('button');
		this.skipButton.id = 'ima-skip-button';
		this.skipButton.style.display = "none"

		this.skipInfo = document.createElement('span');
		this.skipInfo.innerHTML = "Skip After " + this.settings.skipTime +"s";

		var skipSpan = document.createElement('span');
		skipSpan.innerHTML = '<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="13.122px" height="13.274px" viewBox="0 0 13.122 13.274" enable-background="new 0 0 13.122 13.274" xml:space="preserve"><path d="M0,3.727V1.36c0-1.244,0.918-1.722,2.04-1.063l8.1,4.76c0-0.481,0-0.962,0-1.327V1.328C10.14,0.597,10.744,0,11.482,0h0.298	c0.738,0,1.342,0.597,1.342,1.328v10.619c0,0.73-0.604,1.327-1.342,1.327h-0.298c-0.738,0-1.342-0.597-1.342-1.327V9.545c0-0.365,0-0.846,0-1.327l-8.1,4.76C0.918,13.637,0,13.159,0,11.915V9.547V3.727z"/></svg>';
		this.skipButton.appendChild(this.skipInfo);
		this.skipButton.appendChild(skipSpan);
			
		this.infoDiv = document.createElement('div');
		this.infoDiv.id = 'ima-info-div';
		this.infoDiv.classList.add("vmp-current-time")		 
		this.infoDiv.classList.add("vmp-time-control")	
		this.infoDiv.classList.add("vmp-control")	
		
		this.infoDuration = document.createElement('span');
		this.infoDiv.appendChild(this.infoDuration);

		//add control bar to adContainer	
		this.adContainerDiv.insertBefore(
		  this.controlsDiv,
		  this.adContainerDiv.childNodes[this.adContainerDiv.childNodes.length]);

		this.adContainerDiv.insertBefore(
		 this.progressDiv,
		 this.controlsDiv);

		this.adContainerDiv.insertBefore(
		 this.skipButton,
		 this.progressDiv);
		
		if (self.settings.adType=="VAST" && self.currentAd.b.contentType.indexOf("video")>=0 && window.isMobile){
			this.adContainerDiv.insertBefore(
			 pop_layer,
			 this.skipButton);
		} 

		this.controlsDiv.appendChild(this.infoDiv)

		// this.controlsDiv.insertBefore(
		//  this.infoDiv,
		//  this.controlsDiv.);

		//event listener
		this.playPauseButton.addEventListener(
		  'click',
		  self.onAdPlayPauseClick_,
		  false);

		this.fullscreenControl.addEventListener(
		  'click',
		  self.onAdFullscreenClick_,
		  false);

		this.skipButton.addEventListener(
		  'click',
		  self.onAdSkip_, false);

	}

	this.updateControl = function(self){
		if (!self.adsManager) return;
		if (self.adsManager.width != self.player.el_.offsetWidth || 
			self.adsManager.height != self.player.el_.offsetHeight){
			if (self.currentAd.isLinear()){
				self.adsManager.resize(
	                self.player.el_.offsetWidth,
	            	self.player.el_.offsetHeight,
	            	google.ima.ViewMode.NORMAL);
				console.log("update adContainerDiv " + self.player.el_.offsetHeight)
				self.adContainerDiv.style.height = self.player.el_.offsetHeight + "px";
				self.adContainerDiv.style.width = self.player.el_.offsetWidth + "px";
			}
		    else {
		    	self.adsManager.resize(
	                self.player.el_.offsetWidth,
	            	self.currentAd.getHeight()+20,
	            	google.ima.ViewMode.NORMAL);
		    	self.adContainerDiv.style.height = (self.currentAd.getHeight()+20) + "px";
		    	self.adContainerDiv.style.width = self.player.el_.offsetWidth + "px";
		    }
		}
		
		self.adsManager.width = self.player.el_.offsetWidth;
		self.adsManager.height = self.player.el_.offsetHeight; 

		if (self.duration <= self.adsManager.getRemainingTime())
			self.duration = self.adsManager.getRemainingTime();

		window.adsManager = self.adsManager
		var remainingTime = self.adsManager.getRemainingTime();
		var currentTime = self.duration - remainingTime;

		// console.log("duration " + duration + " remainingTime " + self.adsManager.getRemainingTime())
		// currentTime = currentTime > 0 ? currentTime : 0;
		// var isPod = false;
		// var adPosition, totalAds;
		// if (currentAd.getAdPodInfo()) {
		// isPod = true;
		// adPosition = currentAd.getAdPodInfo().getAdPosition();
		// totalAds = currentAd.getAdPodInfo().getTotalAds();
		// }

		// // Update countdown timer data
		// var remainingMinutes = Math.floor(remainingTime / 60);
		// var remainingSeconds = Math.floor(remainingTime % 60);
		// if (remainingSeconds.toString().length < 2) {
		// remainingSeconds = '0' + remainingSeconds;
		// }
		// var podCount = ': ';
		// if (isPod) {
		// podCount = ' (' + adPosition + ' of ' + totalAds + '): ';
		// }
		// countdownDiv.innerHTML =
		//   'Advertisement' + podCount +
		//   remainingMinutes + ':' + remainingSeconds;

      // Update UI
      var playProgressRatio = currentTime / self.duration;
      var playProgressPercent = playProgressRatio * 100;
      self.progressDiv.style.width = playProgressPercent + '%';


		function getTimeFormat(s){
			var minute = String(Math.floor(s / 60));
			var second = String(Math.floor(s % 60));
			while (minute.length < 2) minute = "0" + minute;
			while (second.length < 2) second = "0" + second;
			return minute + ":" + second
		}
		if (self.settings.position!="preroll")	
      		self.infoDuration.innerHTML = getTimeFormat(Math.round(adsManager.getRemainingTime()));
      	else
      		self.infoDuration.innerHTML = "";
      
      //skip ads
      //just effect when return ad is not skippable and mecloud setting is skippable
      if (self.settings.adType=="VAST" && !self.currentAd.isSkippable() && self.currentAd.isLinear()){
			if (self.settings.skippable){
				self.skipButton.style.display = "block"
				// console.log(currentTime)
				var remainAd = Math.round(self.settings.skipTime - currentTime);
				if (remainAd>0)
					self.skipInfo.innerHTML = "Skip " + String(remainAd);
				else {
					self.skipInfo.innerHTML = "Skip";
					self.canSkipNow = true;
				}
			}
		} 
	}

	// ======================= START STEP =======================

	//STEP INFO: AdDisplayContainer()
	this.vjsControls = this.player.getChild('controlBar');
	this.adContainerDiv = 
		this.vjsControls.el().parentNode.insertBefore(
		document.createElement('div'),
		this.vjsControls.el());
	this.adContainerDiv.id = 'ima-ad-container';

	this.adContainerDiv.addEventListener(
      'mouseover',
    function(){
    	self.player.trigger("useractive")
    },
      false);

	//preroll banner overlay
	// this.prerollBannerOverlay = document.createElement("div");
	// this.prerollBannerOverlay.id = "preroll-banner-overlay";


	this.player.currentTime(0);

	adDisplayContainer = new google.ima.AdDisplayContainer(this.adContainerDiv, this.contentPlayer);

	//STEP INFO: AdsLoader()
	debug("AdsLoader()")
	this.adsLoader = new google.ima.AdsLoader(adDisplayContainer);

    this.adsLoader.getSettings().setVpaidMode(
        google.ima.ImaSdkSettings.VpaidMode.ENABLED);
    if (this.settings.vpaidAllowed == false) {
      this.adsLoader.getSettings().setVpaidMode(
          google.ima.ImaSdkSettings.VpaidMode.DISABLED);
    }
    if (this.settings.vpaidMode) {
      this.adsLoader.getSettings().setVpaidMode(this.settings.vpaidMode);
    }

    if (this.settings.locale) {
      this.adsLoader.getSettings().setLocale(this.settings.locale);
    }

    this.adsLoader.getSettings().setPlayerType(this.settings.playerType);
    this.adsLoader.getSettings().setPlayerVersion(this.settings.version);
    this.adsLoader.getSettings().setAutoPlayAdBreaks(this.autoPlayAdBreaks);

    this.adsLoader.addEventListener(
      google.ima.AdsManagerLoadedEvent.Type.ADS_MANAGER_LOADED,
      this.onAdsManagerLoaded_,
      false);
    this.adsLoader.addEventListener(
      google.ima.AdErrorEvent.Type.AD_ERROR,
      this.onAdsLoaderError_,
      false);


    //STEP INFO: AdDisplayContainer.initialize()
    debug("adDisplayContainer.initialize()")
    adDisplayContainer.initialize()

    //STEP INFO: AdRequest()
    debug("AdRequest")

    this.adsRequest = new google.ima.AdsRequest();
	this.adsRequest.adTagUrl = this.settings.adTagUrl;

	this.adsRequest.linearAdSlotWidth = this.player.el_.offsetWidth;
	this.adsRequest.linearAdSlotHeight = this.player.el_.offsetHeight;

	// if (this.settings.position!="preroll") {
	// 	adsRequest.nonLinearAdSlotWidth =
	//   this.settings.nonLinearWidth || player.el_.offsetWidth;
	// 	adsRequest.nonLinearAdSlotHeight =
	//   this.settings.nonLinearHeight || (player.el_.offsetHeight / 3);
	// } else{
	// 	console.log("xxxxxxxxxxx IMA preroll")
	// 	adsRequest.nonLinearAdSlotWidth = Math.min(player.el_.offsetWidth, player.el_.offsetHeight)
	// 	adsRequest.nonLinearAdSlotHeight = adsRequest.nonLinearAdSlotWidth;
	// }

	this.adsRequest.nonLinearAdSlotWidth = this.player.el_.offsetWidth
	this.adsRequest.nonLinearAdSlotHeight = this.player.el_.offsetHeight;

	//STEP INFO: adsLoader.requestAds()
	debug("requestAds")
	console.log(this.adsRequest.linearAdSlotWidth + "x" + this.adsRequest.linearAdSlotHeight + " " + this.adsRequest.nonLinearAdSlotWidth + "x" + this.adsRequest.nonLinearAdSlotHeight)

	this.adsLoader.requestAds(this.adsRequest);

	//STEP INFO: AdsManager.init()
	this.player.on('ima-ready', function(){
		try{
		debug("adsManager.init() " + self.player.el_.offsetWidth + " " + self.player.el_.offsetHeight )
		self.adsManager.init(
	          self.player.el_.offsetWidth,
	          self.player.el_.offsetHeight,
	          google.ima.ViewMode.NORMAL);
          self.adsManager.setVolume(self.player.muted() ? 0 : self.player.volume());
          debug("adsManager.start()")
          self.adsManager.start();
      	} catch(err){
      		debug(err)
      	}
	});
}

}







},{}],4:[function(require,module,exports){
/**
 * Basic Ad support plugin for video.js.
 *
 * Common code to support ad integrations.
 */
exports = module.exports = function(window, videojs, undefined) {
'use strict';

var

	VIDEO_EVENTS = videojs.getComponent('Html5').Events,

	/**
	 * If ads are not playing, pauses the player at the next available
	 * opportunity. Has no effect if ads have started. This function is necessary
	 * because pausing a video element while processing a `play` event on iOS can
	 * cause the video element to continuously toggle between playing and paused
	 * states.
	 *
	 * @param {object} player The video player
	 */
	cancelContentPlay = function(player) {
		if (player.ads.cancelPlayTimeout) {
			// another cancellation is already in flight, so do nothing
			return;
		}
		player.ads.cancelPlayTimeout = window.setTimeout(function() {
			// deregister the cancel timeout so subsequent cancels are scheduled
			player.ads.cancelPlayTimeout = null;

			// pause playback so ads can be handled.
			if (!player.paused()) {
				player.pause();
			}

			// add a contentplayback handler to resume playback when ads finish.
			player.one('contentplayback', function() {
				if (player.paused()) {
					player.play();
				}
			});
		}, 1);
	},

	/**
	 * Returns an object that captures the portions of player state relevant to
	 * video playback. The result of this function can be passed to
	 * restorePlayerSnapshot with a player to return the player to the state it
	 * was in when this function was invoked.
	 * @param {object} player The videojs player object
	 */
	getPlayerSnapshot = function(player) {
		var
			tech = player.$('.vmp-tech'),
			tracks = player.remoteTextTracks ? player.remoteTextTracks() : [],
			track,
			i,
			suppressedTracks = [],
			snapshot = {
				ended: player.ended(),
				currentSrc: player.currentSrc(),
				src: player.src(),
				currentTime: player.currentTime(),
				type: player.currentType()
			};

		if (tech) {
			snapshot.nativePoster = tech.poster;
			snapshot.style = tech.getAttribute('style');
		}

		i = tracks.length;
		while (i--) {
			track = tracks[i];
			suppressedTracks.push({
				track: track,
				mode: track.mode
			});
			track.mode = 'disabled';
		}
		snapshot.suppressedTracks = suppressedTracks;

		return snapshot;
	},

	/**
	 * Attempts to modify the specified player so that its state is equivalent to
	 * the state of the snapshot.
	 * @param {object} snapshot - the player state to apply
	 */
	restorePlayerSnapshot = function(player, snapshot) {
		var
			// the playback tech
			tech = player.$('.vmp-tech'),

			// the number of remaining attempts to restore the snapshot
			attempts = 20,

			suppressedTracks = snapshot.suppressedTracks,
			trackSnapshot,
			restoreTracks =	function() {
				var i = suppressedTracks.length;
				while (i--) {
					trackSnapshot = suppressedTracks[i];
					trackSnapshot.track.mode = trackSnapshot.mode;
				}
			},

			// finish restoring the playback state
			resume = function() {
				var
					ended = false,
					updateEnded = function() {
						ended = true;
					};
				player.currentTime(snapshot.currentTime);

				// Resume playback if this wasn't a postroll
				if (!snapshot.ended) {
					player.play();
				} else {
					// On iOS 8.1, the "ended" event will not fire if you seek
					// directly to the end of a video. To make that behavior
					// consistent with the standard, fire a synthetic event if
					// "ended" does not fire within 250ms. Note that the ended
					// event should occur whether the browser actually has data
					// available for that position
					// (https://html.spec.whatwg.org/multipage/embedded-content.html#seeking),
					// so it should not be necessary to wait for the seek to
					// indicate completion.
					player.ads.resumeEndedTimeout = window.setTimeout(function() {
						if (!ended) {
							player.play();
						}
						player.off('ended', updateEnded);
						player.ads.resumeEndedTimeout = null;
					}, 250);
					player.on('ended', updateEnded);

					// Need to clear the resume/ended timeout on dispose. If it fires
					// after a player is disposed, an error will be thrown!
					player.on('dispose', function() {
						window.clearTimeout(player.ads.resumeEndedTimeout);
					});
				}
			},

			// determine if the video element has loaded enough of the snapshot source
			// to be ready to apply the rest of the state
			tryToResume = function() {

				// tryToResume can either have been called through the `contentcanplay`
				// event or fired through setTimeout.
				// When tryToResume is called, we should make sure to clear out the other
				// way it could've been called by removing the listener and clearing out
				// the timeout.
				player.off('contentcanplay', tryToResume);
				if (player.ads.tryToResumeTimeout_) {
					player.clearTimeout(player.ads.tryToResumeTimeout_);
					player.ads.tryToResumeTimeout_ = null;
				}

				// Tech may have changed depending on the differences in sources of the
				// original video and that of the ad
				tech = player.el().querySelector('.vmp-tech');

				if (tech.readyState > 1) {
					// some browsers and media aren't "seekable".
					// readyState greater than 1 allows for seeking without exceptions
					return resume();
				}

				if (tech.seekable === undefined) {
					// if the tech doesn't expose the seekable time ranges, try to
					// resume playback immediately
					return resume();
				}

				if (tech.seekable.length > 0) {
					// if some period of the video is seekable, resume playback
					return resume();
				}

				// delay a bit and then check again unless we're out of attempts
				if (attempts--) {
					window.setTimeout(tryToResume, 50);
				} else {
					(function() {
						try {
							resume();
						} catch(e) {
							videojs.log.warn('Failed to resume the content after an advertisement', e);
						}
					})();
				}
			},

			// whether the video element has been modified since the
			// snapshot was taken
			srcChanged;

		if (snapshot.nativePoster) {
			tech.poster = snapshot.nativePoster;
		}

		if ('style' in snapshot) {
			// overwrite all css style properties to restore state precisely
			tech.setAttribute('style', snapshot.style || '');
		}

		// Determine whether the player needs to be restored to its state
		// before ad playback began. With a custom ad display or burned-in
		// ads, the content player state hasn't been modified and so no
		// restoration is required

		srcChanged = player.src() !== snapshot.src || player.currentSrc() !== snapshot.currentSrc;

		if (srcChanged) {
			// on ios7, fiddling with textTracks too early will cause safari to crash
			player.one('contentloadedmetadata', restoreTracks);

			// if the src changed for ad playback, reset it
			player.src({ src: snapshot.currentSrc, type: snapshot.type });
			// safari requires a call to `load` to pick up a changed source
			player.load();
			// and then resume from the snapshots time once the original src has loaded
			// in some browsers (firefox) `canplay` may not fire correctly.
			// Reace the `canplay` event with a timeout.
			player.one('contentcanplay', tryToResume);
			player.ads.tryToResumeTimeout_ = player.setTimeout(tryToResume, 2000);
		} else if (!player.ended() || !snapshot.ended) {
			// if we didn't change the src, just restore the tracks
			restoreTracks();
			// the src didn't change and this wasn't a postroll
			// just resume playback at the current time.
			player.play();
		}
	},

	/**
	 * Remove the poster attribute from the video element tech, if present. When
	 * reusing a video element for multiple videos, the poster image will briefly
	 * reappear while the new source loads. Removing the attribute ahead of time
	 * prevents the poster from showing up between videos.
	 * @param {object} player The videojs player object
	 */
	removeNativePoster = function(player) {
		var tech = player.$('.vmp-tech');
		if (tech) {
			tech.removeAttribute('poster');
		}
	},

	// ---------------------------------------------------------------------------
	// Ad Framework
	// ---------------------------------------------------------------------------

	// default framework settings
	defaults = {
		// maximum amount of time in ms to wait to receive `adsready` from the ad
		// implementation after play has been requested. Ad implementations are
		// expected to load any dynamic libraries and make any requests to determine
		// ad policies for a video during this time.
		timeout: 5000,

		// maximum amount of time in ms to wait for the ad implementation to start
		// linear ad mode after `readyforpreroll` has fired. This is in addition to
		// the standard timeout.
		prerollTimeout: 100,

		// maximum amount of time in ms to wait for the ad implementation to start
		// linear ad mode after `contentended` has fired.
		postrollTimeout: 100,

		// when truthy, instructs the plugin to output additional information about
		// plugin state to the video.js log. On most devices, the video.js log is
		// the same as the developer console.
		debug: true
	},

	adFramework = function(options) {
		var player = this;
		var settings = videojs.mergeOptions(defaults, options);
		var fsmHandler;

		// prefix all video element events during ad playback
		// if the video element emits ad-related events directly,
		// plugins that aren't ad-aware will break. prefixing allows
		// plugins that wish to handle ad events to do so while
		// avoiding the complexity for common usage
		(function() {
			var videoEvents = VIDEO_EVENTS.concat([
				'firstplay',
				'loadedalldata'
			]);

			var returnTrue = function() { return true; };

			var triggerEvent = function(type, event) {
				// pretend we called stopImmediatePropagation because we want the native
				// element events to continue propagating
				event.isImmediatePropagationStopped = returnTrue;
				event.cancelBubble = true;
				event.isPropagationStopped = returnTrue;
				player.trigger({
					type: type + event.type,
					state: player.ads.state,
					originalEvent: event
				});
			};

			player.on(videoEvents, function redispatch(event) {
				if (player.ads.state === 'ad-playback') {
					triggerEvent('ad', event);
				} else if (player.ads.state === 'content-playback' && event.type === 'ended') {
					triggerEvent('content', event);
				} else if (player.ads.state === 'content-resuming') {
					if (player.ads.snapshot) {
						// the video element was recycled for ad playback
						if (player.currentSrc() !== player.ads.snapshot.currentSrc) {
							if (event.type === 'loadstart') {
								return;
							}
							return triggerEvent('content', event);

						// we ended playing postrolls and the video itself
						// the content src is back in place
						} else if (player.ads.snapshot.ended) {
							if ((event.type === 'pause' ||
									event.type === 'ended')) {
								// after loading a video, the natural state is to not be started
								// in this case, it actually has, so, we do it manually
								player.addClass('vmp-has-started');
								// let `pause` and `ended` events through, naturally
								return;
							}
							// prefix all other events in content-resuming with `content`
							return triggerEvent('content', event);
						}
					}
					if (event.type !== 'playing') {
						triggerEvent('content', event);
					}
				}
			});
		})();

		// We now auto-play when an ad gets loaded if we're playing ads in the same video element as the content.
		// The problem is that in IE11, we cannot play in addurationchange but in iOS8, we cannot play from adcanplay.
		// This will allow ad-integrations from needing to do this themselves.
		player.on(['addurationchange', 'adcanplay'], function() {
			if (player.currentSrc() === player.ads.snapshot.currentSrc) {
				return;
			}

			player.play();
		});

		// replace the ad initializer with the ad namespace
		player.ads = {
			state: 'content-set',

			// Call this when an ad response has been received and there are
			// linear ads ready to be played.
			startLinearAdMode: function() {
				if (player.ads.state !== 'ad-playback') {
					player.trigger('adstart');
				}
			},

			// Call this when a linear ad pod has finished playing.
			endLinearAdMode: function() {
				if (player.ads.state === 'ad-playback') {
					player.trigger('adend');
				}
			},

			// Call this when an ad response has been received but there are no
			// linear ads to be played (i.e. no ads available, or overlays).
			// This has no effect if we are already in a linear ad mode.	Always
			// use endLinearAdMode() to exit from linear ad-playback state.
			skipLinearAdMode: function() {
				if (player.ads.state !== 'ad-playback') {
					player.trigger('adskip');
				}
			}
		};

		fsmHandler = function(event) {
			// Ad Playback State Machine
			var fsm = {
				'content-set': {
					events: {
						'adscanceled': function() {
							this.state = 'content-playback';
						},
						'adsready': function() {
							this.state = 'ads-ready';
						},
						'play': function() {
							this.state = 'ads-ready?';
							cancelContentPlay(player);
							// remove the poster so it doesn't flash between videos
							removeNativePoster(player);
						},
						'adserror': function() {
							this.state = 'content-playback';
						},
						'adskip': function() {
							this.state = 'content-playback';
						}
					}
				},
				'ads-ready': {
					events: {
						'play': function() {
							this.state = 'preroll?';
							cancelContentPlay(player);
						},
						'adskip': function() {
							this.state = 'content-playback';
						},
						'adserror': function() {
							this.state = 'content-playback';
						}
					}
				},
				'preroll?': {
					enter: function() {
						// change class to show that we're waiting on ads
						player.addClass('vmp-ad-loading');
						// schedule an adtimeout event to fire if we waited too long
						player.ads.adTimeoutTimeout = window.setTimeout(function() {
							player.trigger('adtimeout');
						}, settings.prerollTimeout);
						// signal to ad plugin that it's their opportunity to play a preroll
						player.trigger('readyforpreroll');
					},
					leave: function() {
						window.clearTimeout(player.ads.adTimeoutTimeout);
						player.removeClass('vmp-ad-loading');
					},
					events: {
						'play': function() {
							cancelContentPlay(player);
						},
						'adstart': function() {
							console.log("ad-playback")
							this.state = 'ad-playback';
						},
						'adskip': function() {
							this.state = 'content-playback';
						},
						'adtimeout': function() {
							this.state = 'content-playback';
						},
						'adserror': function() {
							this.state = 'content-playback';
						}
					}
				},
				'ads-ready?': {
					enter: function() {
						player.addClass('vmp-ad-loading');
						player.ads.adTimeoutTimeout = window.setTimeout(function() {
							player.trigger('adtimeout');
						}, settings.timeout);
					},
					leave: function() {
						window.clearTimeout(player.ads.adTimeoutTimeout);
						player.removeClass('vmp-ad-loading');
					},
					events: {
						'play': function() {
							cancelContentPlay(player);
						},
						'adscanceled': function() {
							this.state = 'content-playback';
						},
						'adsready': function() {
							this.state = 'preroll?';
						},
						'adskip': function() {
							this.state = 'content-playback';
						},
						'adtimeout': function() {
							this.state = 'content-playback';
						},
						'adserror': function() {
							this.state = 'content-playback';
						}
					}
				},
				'ad-playback': {
					enter: function() {
						// capture current player state snapshot (playing, currentTime, src)
						this.snapshot = getPlayerSnapshot(player);

						// add css to the element to indicate and ad is playing.
						player.addClass('vmp-ad-playing');

						// remove the poster so it doesn't flash between ads
						removeNativePoster(player);

						// We no longer need to supress play events once an ad is playing.
						// Clear it if we were.
						if (player.ads.cancelPlayTimeout) {
							window.clearTimeout(player.ads.cancelPlayTimeout);
							player.ads.cancelPlayTimeout = null;
						}
					},
					leave: function() {
						player.removeClass('vmp-ad-playing');
						restorePlayerSnapshot(player, this.snapshot);
						// trigger 'adend' as a consistent notification
						// event that we're exiting ad-playback.
						if (player.ads.triggerevent !== 'adend') {
							player.trigger('adend');
						}
					},
					events: {
						'adend': function() {
							this.state = 'content-resuming';
						},
						'adserror': function() {
							this.state = 'content-resuming';
						}
					}
				},
				'content-resuming': {
					enter: function() {
						if (this.snapshot.ended) {
							window.clearTimeout(player.ads._fireEndedTimeout);
							// in some cases, ads are played in a swf or another video element
							// so we do not get an ended event in this state automatically.
							// If we don't get an ended event we can use, we need to trigger
							// one ourselves or else we won't actually ever end the current video.
							player.ads._fireEndedTimeout = window.setTimeout(function() {
								player.trigger('ended');
							}, 1000);
						}
					},
					leave: function() {
						window.clearTimeout(player.ads._fireEndedTimeout);
					},
					events: {
						'contentupdate': function() {
							this.state = 'content-set';
						},
						contentresumed: function() {
							this.state = 'content-playback';
						},
						'playing': function() {
							this.state = 'content-playback';
						},
						'ended': function() {
							this.state = 'content-playback';
						}
					}
				},
				'postroll?': {
					enter: function() {
						this.snapshot = getPlayerSnapshot(player);

						player.addClass('vmp-ad-loading');

						player.ads.adTimeoutTimeout = window.setTimeout(function() {
							player.trigger('adtimeout');
						}, settings.postrollTimeout);
					},
					leave: function() {
						window.clearTimeout(player.ads.adTimeoutTimeout);
						player.removeClass('vmp-ad-loading');
					},
					events: {
						'adstart': function() {
							this.state = 'ad-playback';
						},
						'adskip': function() {
							this.state = 'content-resuming';
							window.setTimeout(function() {
								player.trigger('ended');
							}, 1);
						},
						'adtimeout': function() {
							this.state = 'content-resuming';
							window.setTimeout(function() {
								player.trigger('ended');
							}, 1);
						},
						'adserror': function() {
							this.state = 'content-resuming';
							window.setTimeout(function() {
								player.trigger('ended');
							}, 1);
						}
					}
				},
				'content-playback': {
					enter: function() {
						// make sure that any cancelPlayTimeout is cleared
						if (player.ads.cancelPlayTimeout) {
							window.clearTimeout(player.ads.cancelPlayTimeout);
							player.ads.cancelPlayTimeout = null;
						}
						// this will cause content to start if a user initiated
						// 'play' event was canceled earlier.
						player.trigger({
							type: 'contentplayback',
							triggerevent: player.ads.triggerevent
						});
					},
					events: {
						// in the case of a timeout, adsready might come in late.
						'adsready': function() {
							player.trigger('readyforpreroll');
						},
						'adstart': function() {
							this.state = 'ad-playback';
						},
						'contentupdate': function() {
							if (player.paused()) {
								this.state = 'content-set';
							} else {
								this.state = 'ads-ready?';
							}
						},
						'contentended': function() {
							this.state = 'postroll?';
						}
					}
				}
			};

			(function(state) {
				var noop = function() {};

				// process the current event with a noop default handler
				((fsm[state].events || {})[event.type] || noop).apply(player.ads);

				// check whether the state has changed
				if (state !== player.ads.state) {

					// record the event that caused the state transition
					player.ads.triggerevent = event.type;

					// execute leave/enter callbacks if present
					(fsm[state].leave || noop).apply(player.ads);
					(fsm[player.ads.state].enter || noop).apply(player.ads);

					// output debug logging
					if (settings.debug) {
						videojs.log('ads', player.ads.triggerevent + ' triggered: ' + state + ' -> ' + player.ads.state);
					}
				}

			})(player.ads.state);

		};

		// register for the events we're interested in
		player.on(VIDEO_EVENTS.concat([
			// events emitted by ad plugin
			'adtimeout',
			'contentupdate',
			'contentplaying',
			'contentended',
			'contentresumed',

			// events emitted by third party ad implementors
			'adsready',
			'adserror',
			'adscanceled',
			'adstart',	// startLinearAdMode()
			'adend',		// endLinearAdMode()
			'adskip'		// skipLinearAdMode()
		]), fsmHandler);

		// keep track of the current content source
		// if you want to change the src of the video without triggering
		// the ad workflow to restart, you can update this variable before
		// modifying the player's source
		player.ads.contentSrc = player.currentSrc();

		// implement 'contentupdate' event.
		(function(){
			var
				// check if a new src has been set, if so, trigger contentupdate
				checkSrc = function() {
					var src;
					if (player.ads.state !== 'ad-playback') {
						src = player.currentSrc();
						if (src !== player.ads.contentSrc) {
							player.trigger({
								type: 'contentupdate',
								oldValue: player.ads.contentSrc,
								newValue: src
							});
							player.ads.contentSrc = src;
						}
					}
				};
			// loadstart reliably indicates a new src has been set
			//player.on('loadstart', checkSrc);
			// check immediately in case we missed the loadstart
			window.setTimeout(checkSrc, 1);
		})();

		// kick off the fsm
		if (!player.paused()) {
			// simulate a play event if we're autoplaying
			fsmHandler({type:'play'});
		}

	};

	// register the ad plugin framework
	videojs.plugin('ads', adFramework);

}

},{}],5:[function(require,module,exports){
/*
Plugin: mecloud analytics 
Author: Van Quoc Dung
Source code refer from : https://github.com/mickey/videojs-ga/blob/master/dist/videojs.ga.js
Updated date: 14/12/2015
*/

//script in closed scope so that it cannot be accessed from other part
(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
m=s.getElementsByTagName(o)[0];a.async=0;a.src=g;m.parentNode.insertBefore(a,m)
})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

exports = module.exports = function(videojs, Logger){
	function checkMobile(){
		if( navigator.userAgent.match(/Android/i)
		 || navigator.userAgent.match(/webOS/i)
		 || navigator.userAgent.match(/PLAYSTATION/i)
		 || navigator.userAgent.match(/iPhone/i)
		 || navigator.userAgent.match(/iPad/i)
		 || navigator.userAgent.match(/iPod/i)
		 || navigator.userAgent.match(/BlackBerry/i)
		 || navigator.userAgent.match(/Windows Phone/i)
		 ){
			return true;
		} else
			return false;
	}
	
	function getBrowser(){
		var browser = navigator.userAgent;
		if (browser.indexOf("MSIE")>=0)
	    {
	      return "Internet Explorer";
	    } else if (browser.indexOf("Firefox")>=0)
	    {
	        return "Firefox";
	    } else if (browser.indexOf("Chrome")>=0)
	    {
	        return "Chrome";
	    } else if (browser.indexOf("Opera")>=0)
	    {
	        return "Opera";
	    } else if (browser.indexOf("Safari")>=0)
	    {
	        return "Safari";
	    } else if (browser.indexOf("FBAN/FBIOS")>=0)
	    {
	        return "Facebook App";
	    }
	    
	    return "-" ;
	  
	}

	function setCookie(cname, cvalue, exminute) {
	    var d = new Date();
	    d.setTime(d.getTime() + (exminute*60*1000));
	    var expires = "expires="+d.toUTCString();
	    document.cookie = cname + "=" + cvalue + "; " + expires+"; path=/";
	}

	function getCookie(cname) {
		var name = cname + "=";
		var ca = document.cookie.split(';');
		for(var i=0; i<ca.length; i++) {
		    var c = ca[i];
		    while (c.charAt(0)==' ') c = c.substring(1);
		    if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
		}
		return "";
	} 

	setCookie("_upc","1",3*365*24*60);
	var url = window.location.href;
	var referer = document.referrer;

	//domain
	var fid = url.indexOf("//")+2
	var eid = url.indexOf("/",fid+1)
	if (eid!=-1){
		var _upd = url.substring(fid,eid);
	} else
		var _upd = url.substring(fid);

	//traffic source
	var _ups = getCookie("_ups");
	if (_ups==""){
		if (referer){
			var fid = referer.indexOf("//")+2
			var eid = referer.indexOf("/",fid+1)
			if (eid!=-1){
				var rdomain = referer.substring(fid,eid);
			} else
				var rdomain = referer.substring(fid);

			var upd_arr = _upd.split(".")
			var rdomain_arr = rdomain.split(".")
			if (upd_arr.slice(upd_arr.length-2,upd_arr.length).join(".") == rdomain_arr.slice(rdomain_arr.length-2,rdomain_arr.length).join(".")){
				_ups = "direct";	
			} else {
				_ups = rdomain;	
			}
		} else {
			_ups = "direct";
		}
		setCookie("_ups",_ups,90);
	}
	
	//platform
	if (checkMobile()){
		var _upp = "mobile"
	} else {
		var _upp = "desktop"
	}

	var _upuid = getCookie("_upuid");
	var _upb = getBrowser()
    var _upsid = "";
	function UPPing(event, settings, data){
		
	}
	//declare default events to track (in case user dont indicate eventTrack options)
	var defaultEventTrack = ['ready', 'play', 'error', 'end', 'loadtime', 'playthrough', 'pause', 'seek', 'fullscreen', 'normalscreen', 'playerload', 'mute', 'unmute', 'importMeAd', 'meAdclick', 'meAdready', 'meAdstart'];

	function debug(settings, s){
		if (settings.debug)
			console.log(s)
	}

	//setting for each player
	var AnalyticsSetting = function(player, options){
		return	{
			muted: false,
			debug: false,
			eventTrack: options.eventTrack || defaultEventTrack,
			vid: options.vid,
			title: options.title,
			session: options.session,
			gaID: options.gaID,
			mecloudAnalytic: options.mecloudAnalytic,
			debug: (options.debug) ? true : false,
			player: player
		}; 
	}	

	var paused_counter = 0;
	//send signal to google analytics
	function sendSignal(settings, category, action, data){
		if (settings.mecloudAnalytic){
			//TODO: send signal to mecloud
			switch (category){
				case "Player Action":
					if (action=="Pause"){
						settings.player.ping("ev")
					}
					if (action=="Play"){ //hb
						UPPing("start",settings);
						// settings.player.ping("sv")
					}
					if (action=="CanPlay"){
						settings.player.ping("sv")
					}
					if (action=="meAdclick"){
						settings.player.ping("ac",0,data)
					}
					break
				case "Player Event":
					if (action=="loadingtime"){
						UPPing("loadingtime",settings,data);
					}
					if (action=="buffering"){
						UPPing("buffering",settings);
					}
					if (action=="ready"){
						UPPing("load",settings);
						settings.player.ping()
					}
					if (action=="viewing"){
						if (settings.player.paused()){
							paused_counter++;
							if (paused_counter <=40 )
								UPPing("paused",settings);
							settings.player.ping("p")
						}
						else{
							if (paused_counter!=0){
								paused_counter = 0;
								UPPing("play",settings);	
							} else {
								UPPing("playing",settings);	
							}
							
							settings.player.ping("sv")
						}
					}
					if (action=="importMeAd"){
						settings.player.ping('i')
					}
					if (action=="meAdready"){
						settings.player.ping('ar',0,data)
					}
					if (action=="meAdstart"){
						settings.player.ping('ai',0,data)
					}
					break;
				default:
					break;
			}
		}
		//google analytics
		if (settings.gaID){
			if (window.ga) { //using normal ga 
				ga(settings.random+'.send', 'event', {
					'eventCategory': category, //Player Event
					'eventAction': action, // Pause
					'eventLabel': settings.vid + "-" + settings.title  //Title
				});

				debug(settings,"send method 1 " + settings.vid + " " +category + " - " + action)
			} else if (window._gaq) { //using another method (legacy method)
				_gaq.push(['_trackEvent', category, action, settings.title]);
				debug(settings,"send method 2 " + settings.vid + " " + category + " - " + action)
			} else { //no google analytics embed
				debug(settings,"Google Analytics not detected");
			}
		}
		
	}

	function safeCall(fn) {
        setTimeout(fn, 500);
    }

    function hash(value, displayKey) {
        var length = 32, result = [], sum = 1024, len = value.length,
                len2 = displayKey.length, rsStr = "";
        for (var i = 0; i < length; i++)
            result.push(0);
        for (i = 0; i < len; i++) {
            sum += value.charCodeAt(i);
            result[i % length] = (sum % 256);
        }
        for (i = 0; i < sum; i++) {
            var index = result[i % length] + result[(i + 1) % length] + result[(i + 2) % length] +
                    value.charCodeAt(i % len) ^ displayKey.charCodeAt(i % len2) + sum % len;
            result[i % length] = displayKey.charCodeAt(index % len2);
        }
        for (i = 0; i < length; i++)
            rsStr += String.fromCharCode(result[i]);
        return rsStr;
    }

    function toParamStr(obj) {
        var arr = [];
        for (var key in obj) {
            arr.push(key + "=" + escape(obj[key]));
        }
        return arr.join('&');
    }

	function ping(ev, play, sub) {
        var self = this;
        safeCall(function() {
            var obj = {
                vid: self.playInfo.vid,
                session: self.playInfo.session,
                ref: window.location.toString(),
                time: +new Date(),
                source: self.playInfo.source ? self.playInfo.source : "null"
            };
            obj['ev'] = !ev ? (!self.paused() ? "p" : "l") : ev;
            obj['signkey'] = hash(obj.session + " - " + obj['ev'] + " - " + obj['time'], "CloudVideoPlayerVersion100");
            obj['play'] = Math.floor(!play ? self.currentTime() * 1000 : play);
            if (sub) {
                for (var key in sub) {
                    obj[key] = sub[key];
                }
            }
            postMessage && postMessage(obj.vid, obj.session, {
                signkey: hash(obj.session + "." + obj.vid, "01234656789abcdef"),
                params: toParamStr(obj)
            });
        });
    }


    function postMessage(vid, session, data){
    	var e = document.getElementById('AnalyticsBridge_' + vid + '_' + session);
        if (e && e.contentWindow) {
            if (window.top != window){
                    var ar = data.params.split('&');
                    for (var i = 0; i< ar.length; i++){
                            var p = ar[i].split('=');
                            if (p[0] == 'ref'){
                                    ar[i] = 'ref='+document.referrer;
                                    break;
                            }
                    }
                    data.params = ar.join('&');
            }
            debug("post message " + JSON.stringify(data))
            e.contentWindow.postMessage(JSON.stringify(data), "*");
        }
    }

 	// ga('create', "UA-49731670-2", 'auto', "dn");
	// ga('dn.send', "pageview");	

	//plugin function
	function mecloudAnalytic( options ){

		var settings = new AnalyticsSetting(this, options);

		//intiate setting ga
		var random = "player_" + Math.floor(Math.random()*1000);
		settings.random = random;
		if (options.ga && options.gaID){
			ga('create', settings.gaID, 'auto', random);
			ga(random+'.send', "pageview");	
		}

		//declare player object
		var player = this;
		player.playInfo = {
			vid: settings.vid,
			session: settings.session,
			source: document.referrer
		}
		
		player.ping = ping;
		var init_time;
		var send_loadtime = false;

		//init analytics
		var bridge = document.createElement('iframe');
        bridge.id = 'AnalyticsBridge_' + options.vid + '_' + options.session;
        bridge.src = options.analyticsUrl + "/bridge?session=" + options.session;
        bridge.style.setProperty('position', "fixed", "important");
        bridge.style.setProperty('top', "-99px", "important");
        bridge.style.setProperty('left', "-99px", "important");
        bridge.style.setProperty('width', "1px", "important");
        bridge.style.setProperty('height', "1px", "important");
        // console.log(bridge)
        player.el_.appendChild(bridge);

        //advertisement related
        if (settings.eventTrack.indexOf('importMeAd')!=-1){
			player.on("importMeAd",function(event, data){
				sendSignal(settings, "Player Event", "importMeAd", data);
			})
		}

		if (settings.eventTrack.indexOf('meAdclick')!=-1){
			player.on("meAdclick",function(event, data){
				sendSignal(settings, "Player Action", "meAdclick", data);
			})
		}

		if (settings.eventTrack.indexOf('meAdready')!=-1){
			player.on("meAdready",function(event, data){
				sendSignal(settings, "Player Event", "meAdready", data);
			})
		}

		if (settings.eventTrack.indexOf('meAdstart')!=-1){
			player.on("meAdstart",function(event, data){
				sendSignal(settings, "Player Event", "meAdstart", data);
			})
		}

        //send player ready
        if (settings.eventTrack.indexOf('ready')!=-1){
        	// console.log("register ready event .....")
			player.ready(function(){
				// console.log("player ready ......................")
				sendSignal(settings, "Player Event", "ready");
			})
		}

		//send loading time (Player Load Times)
		if (settings.eventTrack.indexOf('loadtime')!=-1){
			player.one("loadedmetadata", function(){
				sendSignal(settings, "Loading Time", new Date().getTime() - init_time);
				send_loadtime = true;
			})
		}

		//Play Through Rate
		percentsAlreadyTracked = []
		var seekEnd = 0, seekStart;
		var firstView = false;
		player.on('timeupdate', function(){
			var currentTime, duration, percent, percentPlayed, _i;
			currentTime = Math.round(this.currentTime());
			duration = Math.round(this.duration());
			percentPlayed = Math.round(currentTime / duration * 100);
			for (percent = _i = 0; _i <= 99; percent = _i += 10) {
			    if (percentPlayed >= percent && percentsAlreadyTracked.indexOf(percent) < 0) {
			        if (percentPlayed !== 0) {
			            if (settings.eventTrack.indexOf('playthrough')!=-1){
			            	debug ("playthrough " + percent)
			            	sendSignal(settings, "Play Through", percent);
			            }
			        }
			        if (percentPlayed > 0) {
			            percentsAlreadyTracked.push(percent);
			        }
			    }
			}
			
		    seekStart = seekEnd;
		    seekEnd = currentTime;
		    if (Math.abs(seekStart - seekEnd) > 1) {
		        seeking = true;
		        if (settings.eventTrack.indexOf('seek')!=-1){
		        	debug ("seek " + Math.abs(seekStart - seekEnd) / duration )
			        if (seekStart - seekEnd > 0) sendSignal(settings, "Backward Seek", Math.round((seekStart - seekEnd) / duration * 100));
			        if (seekEnd - seekStart > 0) sendSignal(settings, "Forward Seek", Math.round((seekEnd - seekStart) / duration * 100));
			    }
		    }
		})

		//send signal when player is loaded
		if (settings.eventTrack.indexOf('playerload')!=-1){
			player.one("loadstart", function(){
				init_time = new Date().getTime();
				debug(settings,"loadstart")
				sendSignal(settings, "Player Event", "Player Loaded")
				settings.mute = player.muted;
			})
		}
		
		//send signal when player end
		if (settings.eventTrack.indexOf('end')!=-1){
			player.on("ended", function(){
				debug(settings,"ended")
				sendSignal(settings, "Player Event", "Ended")
			})
		}

		//send signal when mute or unmute
		player.on('volumechange', function(){
			var curMuted = player.muted()
			if (curMuted != settings.muted) {
				settings.muted = curMuted;
				if (curMuted){ // muted
					if (settings.eventTrack.indexOf('muted')!=-1){
						debug(settings,"muted")
						sendSignal(settings, "Player Action", "Mute")
					}
				} else { //unmuted
					if (settings.eventTrack.indexOf('unmuted')!=-1){
						debug(settings,"unmuted")
						sendSignal(settings, "Player Action", "Un Mute")
					}
					
				}
			}
		})

		//send signal when user click fullscreen or back to normal screen
		player.on('fullscreenchange', function(){
			if (player.isFullscreen()){
				if (settings.eventTrack.indexOf('fullscreen')!=-1){
					debug(settings,"fullscreen")
					sendSignal(settings, "Player Action", "Full Screen")
				}
			} else{
				if (settings.eventTrack.indexOf('normalscreen')!=-1){
					debug(settings,"normal screen")
					sendSignal(settings, "Player Action", "Normal Screen")
				}
				
			}
		})
		
		//send signal when user click play
		if (settings.eventTrack.indexOf('play')!=-1){
			
			player.on("loadedmetadata", function(){
				var stream_url = player.src();
				if (!firstView && stream_url.indexOf("mecloud.vn")>=0){
					firstView = true;
					sendSignal(settings, "Player Action", "CanPlay")
				}
				
			})
			player.one('play', function(){
				sendSignal(settings, "Player Action", "Play")
			})
			
		}

		//send signal when user click pause
		if (settings.eventTrack.indexOf('pause')!=-1)
			player.on('pause', function(){
				if (!player.seeking() && send_loadtime===true && player.currentTime!= player.duration){
					debug(settings,"pause")
					sendSignal(settings, "Player Action", "Pause")
				}
			})

		//send signal when there is error
		// if (settings.eventTrack.indexOf('error')!=-1)
		var error_event = false;
		player.on('error', function(){
			var error = player.error();
			var stream_url = player.src();
			if (stream_url.indexOf("mecloud.vn")>=0 && error.code >=2) {
				// console.log("Player Error " + error.code + " " + error.message);
				// sendSignal(settings, "Player Error", error.message)
				error_event = true;
				var xhttp = new XMLHttpRequest();
				xhttp.onreadystatechange = function() {
				};
				xhttp.open("GET", "http://error.mecloud.vn/?code=" + error.code, true);
				xhttp.send();
			}
		})

		player.on("play", function(){
			var stream_url = player.src();
				if (stream_url.indexOf("mecloud.vn")>=0) {
					error_event = false;
				}
		})

		//periodically send signal
		player.one('play', function(){
			setInterval(function(){
				if (!error_event)
					sendSignal(settings, "Player Event", "viewing")
			},15*1000)
		})
		
	
		player.on('waiting', function(){
		
			player.off('checkBuffer')
			player.one("checkBuffer", function(){
				var time2 = (new Date()).getTime()
				// console.log("canplay time: " + time2)
				// console.log("loadingtime: " + (time2-time1))
				sendSignal(settings, "Player Event", "loadingtime",(time2-time1))
		
			})
			var time1 = (new Date()).getTime()
			// console.log("waiting: " + time1)
			player.one("canplaythrough", function(){
				player.trigger('checkBuffer')
			})
		})


		var buffer_ct = 0;
		var lastTime = 0;
		function checkBufferLoop(){
			var checkBuffer = setInterval(function(){
				// console.log("checkBuffer")
				if (!player.paused()){
					if (lastTime == player.currentTime()) {
						buffer_ct++;
					} else {
						lastTime = player.currentTime();
						buffer_ct=0;
					}
					if (buffer_ct==2){
						buffer_ct = 0;
						// console.log("buffer")
						sendSignal(settings, "Player Event", "buffering")	
						clearInterval(checkBuffer)
						setTimeout(checkBufferLoop,15000)						
					}
				} else {
					buffer_ct = 0;
				}

			},750)
		}	
		checkBufferLoop();
		
		
	}


	//register plugin
	var checkVideojs = setInterval(function(){
		if (typeof videojs != "undefined"){
			Logger.info("init analytics")
			videojs.plugin('mecloudAnalytic', mecloudAnalytic);
			clearInterval(checkVideojs)
		} else {
			Logger.info("waiting init analytics")
		}
	},50)
	
}
},{}],6:[function(require,module,exports){
/*! videojs-poster - v0.0.0 - 2015-12-15
 * Modified by Tanh Nguyen */
exports = module.exports = function(window, videojs) {
    'use strict';

    var defaults = {
        link:'http://mecloud.vn',
        image: 'brand.png',
        styleBrand: 'icon'
    }
    
    /**
    * Initialize the plugin.
    * @param options (optional) {object} configuration for the plugin
    */
    var videoJsBrand = function(options) {
        var settings = videojs.mergeOptions(defaults, options),
            player = this;

        var meBrand = new mmBrand(settings,player),
            dommeBrand = meBrand.createPoster();
        
        meBrand.el_ = dommeBrand;

        var getElementScreen = player.controlBar.el_.querySelector('.vmp-fullscreen-control');
        //console.log(getElementScreen);
        player.controlBar.el_.insertBefore(dommeBrand,getElementScreen);


        player.videoJsBrand.update = function(settings) {
            meBrand.updateBrand(settings)
        }
        /*player.videoJsBrand.getConfig = function(settings){
            return meBrand
        }*/

    }

    function mmBrand(settings,player) {
        this.player = player;
        this.link = settings.link;
        this.image = settings.image;
        this.imageHover = settings.imageHover;
        this.styleBrand = settings.styleBrand;
    }

    mmBrand.prototype.updateBrand = function(settings){ 
        this.link = settings.link || this.link;
        this.image = settings.image || this.image;
        this.imageHover = settings.imageHover;
        this.styleBrand = settings.styleBrand || this.styleBrand;

        this.el_.className = 'btn-brand vmp-control ' + this.styleBrand + '-style';
        this.el_.href = this.link;
        this.el_.querySelector('.img-brand').src = this.image;

        this.el_.querySelector('.img-brand').setAttribute('style', 'width:auto !important');
        console.log("xxxxxxx")
        window.imgBrand = this.el_.querySelector('.img-brand')

        //console.log(this.imageHover.length);
        var elementImageHover = this.el_.querySelector('img.img-brand-hover');

        if(this.imageHover == '') {
            if(elementImageHover != 'undefined' && elementImageHover != null) elementImageHover.parentNode.removeChild(this.el_.querySelector('.img-brand-hover'));
            
        }else {

            if(elementImageHover != 'undefined' && elementImageHover != null) {
                elementImageHover.src = this.imageHover;
            }else{
                var dImageHover = document.createElement('img');
                dImageHover.className = "img-brand-hover"; 
                dImageHover.src = this.imageHover;

                this.el_.querySelector('.wrapper-img-brand').appendChild(dImageHover);
            }
           
        }
        
    }

    mmBrand.prototype.createPoster = function(){ 
        //div brand btn
        var dvmpBrand = document.createElement('a');

        //setting
        dvmpBrand.className = 'btn-brand vmp-control ' + this.styleBrand + '-style';
        dvmpBrand.id = 'vmp-brand';
        dvmpBrand.href = this.link;
        dvmpBrand.target = "_blank";

        var dSpan = document.createElement('span');
        dvmpBrand.appendChild(dSpan);

        var wrapSpan = document.createElement('span');
        wrapSpan.className = "wrapper-img-brand";
        dSpan.appendChild(wrapSpan);

        var dImage = document.createElement('img');
        dImage.className = "img-brand";
        dImage.setAttribute('style', 'width:auto !important');

        dImage.src = this.image;
        wrapSpan.appendChild(dImage);

        var dImageHover = document.createElement('img');
        dImageHover.className = "img-brand-hover";
        dImageHover.setAttribute('style', 'width:auto !important');
        dImageHover.src = this.imageHover;
        wrapSpan.appendChild(dImageHover);

        return dvmpBrand;
    }   

    // register the plugin
    videojs.plugin('videoJsBrand', videoJsBrand);
}

