    /*
     * End function create button
     */

    //Adding the newly created button to Control Bar
    videoJsPlaylist = function (opt) {
        //var settings = videojs.mergeOptions(defaults, opt);
        var settings = videojs.mergeOptions(defaults, opt);
        var player = this;
        opt.player = this;

        /*
         * create playlist
         */
        var playlist = new mmPlaylist(opt);
        var pl = playlist.createPlayList();
        if (pl) {
            switch (opt.type) {
                case "innerPlaylistRight":
                case "innerPlaylistBottom":
                    player.el_.appendChild(pl);
                    break;

                case "outterPlaylistRight":
                    //wrapper all
                    var wraper_all = document.createElement('div');
                    wraper_all.style.float = "left";
					//calc wapper_all width
					var width_cur =  player.el_.offsetWidth;
					var with_player = width_cur - 300;
					
                    //example
					var height_player = (with_player*9/16);
                    wraper_all.style.width = (with_player)+"px";
					wraper_all.style.height = height_player+"px";
                    var div = player.el_.parentElement.parentElement;
                    wraper_all.appendChild(player.el_.parentNode);
                    div.appendChild(wraper_all);
                    div.appendChild(pl);
                    //example
                    pl.style.height = height_player+"px";;
                    //inner
                    break;

                case "outterPlaylistBottom":
                    player.el_.parentElement.parentElement.appendChild(pl);
                    break;

                case "outterPlaylistBottomList":
                    var wraper_all = document.createElement('div');
                    var div = player.el_.parentElement.parentElement;
                    wraper_all.appendChild(player.el_.parentNode);

                    div.appendChild(wraper_all);
                    pl.style.height = "300px";
                    div.appendChild(pl);
                    break;

                case "innerPlaylistBottomMobile":
                    player.el_.appendChild(pl);
                    break;
                default:
                    console.log("Type playlist error;");
                    return;

            }
        }


        //create time
        var idPlaylist = "-" + playlist.createTime;
         /*
         *Create button repeat playlist 
         */
/*        var optRepeatBtn = {
            "id": "repeat-playlist-btn" + idPlaylist,
            "name": "Next",
            "logo": new buttonNext(),
            "nameClass": "vmp-playlist-repeat-video-button"
        }
        var repeatBtn = new PlaylistButton(player, optRepeatBtn, settings);
        repeatBtn.el().classList.add('vmp-playlist-button');
        repeatBtn.el().classList.add('vmp-playlist-repeat-video-button');
        player.controlBar.addChild(repeatBtn);
		
	*/	
        /*
         *Create button next playlist 
         *create conf
         *create button
         */
        var optNextBtn = {
            "id": "next-playlist-btn" + idPlaylist,
            "name": "Next",
            "logo": new buttonNext(),
            "nameClass": "vmp-playlist-next-video-button"
        }
        var nextBtn = new PlaylistButton(player, optNextBtn, settings);
        nextBtn.el().classList.add('vmp-playlist-button');
        nextBtn.el().classList.add('vmp-playlist-next-video-button');
        player.controlBar.addChild(nextBtn);
		/*
         * Create button playlist
         * create conf
         * create btn
         */
		var optPlaylistBtn = {
            "id": "playlist-btn" + idPlaylist,
            "name": "Playlist",
            "logo": new LogoPlaylist(),
            "nameClass": "vj-playlist-button"
        };
        var newButton = new PlaylistButton(player, optPlaylistBtn, settings);
        newButton.el().classList.add('vmp-playlist-button');
        player.controlBar.addChild(newButton);


        
        /*
         * Create Button prev
         * create conf btn prev
         * call function PLaylistButton(player, conf , settings)
         */
        var optPrevBtn = {
            "id": "prev-playlist-btn" + idPlaylist,
            "name": "Prev",
            "logo": new buttonPrev(),
            "nameClass": "vmp-playlist-prev-video-button"
        }

        var prevBtn = new PlaylistButton(player, optPrevBtn, settings);
        prevBtn.el().classList.add('vmp-playlist-button');
        prevBtn.el().classList.add('vmp-playlist-prev-video-button');
		
        player.controlBar.addChild(prevBtn);



        //add event playlist btn
        var playlistBtn;
        if (playlistBtn = document.getElementById(optPlaylistBtn.id)) {
            //console.log("add event click playlist btn");
            playlistBtn.addEventListener('click', function () {
                //console.log("click ne");
				playlist.clickBtnPlaylist();
            });
            playlistBtn.addEventListener('touchstart', function () {
                //console.log("click ne");
				playlist.clickBtnPlaylist();
            });
        }
        //event prev
        var prevBtn;
        if (prevBtn = document.getElementById(optPrevBtn.id)) {
            prevBtn.addEventListener('click', function () {
                playlist.prevVideo();
            });
            prevBtn.addEventListener('touchstart', function () {
                playlist.prevVideo();
            });
        }
        var nextBtn;
        if (nextBtn = document.getElementById(optNextBtn.id)) {
            nextBtn.addEventListener('click', function () {
                playlist.nextVideo();
            });
            nextBtn.addEventListener('touchstart', function () {
                playlist.nextVideo();
            });
        }
		
		/*var repeatBtn;
		if(repeatBtn = document.getElementById()){
			repeatBtn.addEventListener('click',function(){
				
			});
		}
		*/
        playlist.start();
    }

    videojs.plugin('videoJsPlaylist', videoJsPlaylist);
}

},{}],10:[function(require,module,exports){
/*! videojs-poster - v0.0.0 - 2015-12-15
 * Modified by Tanh Nguyen */
exports = module.exports = function(window, videojs) {
    'use strict';

    var defaults = {
        title:'[Official Trailer] Tuấn Hưng - Word Tour Đam Mê in Australia',
        image: 'poster.png',
        position: 'top',
        autoplay: false,
        offTitle: false,
        buttonPlay: true,
        showControlBar: false
    }
        
	
	function loadScript(url,callback,r){

        if(window.lazyload){callback(); return;}

        r = document.querySelectorAll('script[src="'+url+'"]');
        if (r && r.length) { 
            setTimeout(function(){ 
                loadScript(url,callback); 
            },500); 
            return;
        }
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.src = url;
        script.onreadystatechange = function () {
            if (this.readyState == 'complete' || this.readyState == 'loaded')
                callback();
        }
        script.onload = callback;
        document.head.appendChild(script);
    }
	
	var __AutoPlay = {
		lazyload: function(player, aPlay){
			var srcLazyload = window.$_VConfig.pluginFolder + 'skin/autoplay-lazyload-1.01.js';
			loadScript(srcLazyload,function(){					
				if(player.el_){
					var el_div = document.createElement("div"); 
					el_div.id = "autoplay-lzld";
					el_div.style.position = "absolute"
					el_div.style.top = "50%";
					el_div.style.left = 0;
					el_div.style.right = 0;
					el_div.style.fontSize = "14px"
					el_div.style.zIndex = "4";
					el_div.innerHTML = "Tự động play video ";
					el_div.style.textAlign = "center";  
					var el_span = document.createElement("span");
					el_span.id = "countdown";
					el_span.innerHTML = aPlay.countdown;
					el_div.appendChild(el_span);
					var el_img = document.createElement("img");
					el_img.autoplay = {
						player: player,
						countdown: aPlay.countdown,
						volume: aPlay.volume
					};
					el_img.src = "data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==";
					el_img.setAttribute("data-src", "data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==?v1");
					el_img.setAttribute("onload", "lzld(this)");
					el_div.appendChild(el_img);
					player.el_.appendChild(el_div);
				}        
			});
			
		},
		mouseover: function(player, aPlay){
			var term_mouseover = false; 
			player.el_.addEventListener("mouseover", function(){
				if(!term_mouseover){
					console.log('====================MOUSEOVER=================');
					if(aPlay.countdown > 0){
						__AutoPlay.countdown(player, aPlay);
					}else{
						player.play();
					}
					term_mouseover = true;
				}
			});
			
		},
		countdown: function(player, aPlay){
			var el_div = document.createElement("div");  
			el_div.id = "autoplay-lzld";
			el_div.style.position = "absolute"
			el_div.style.top = "50%";
			el_div.style.left = 0;
			el_div.style.right = 0;
			el_div.style.fontSize = "14px"
			el_div.style.zIndex = "4";
			el_div.innerHTML = "Tự động play video ";
			el_div.style.textAlign = "center";  
			
			var el_span = document.createElement("span");
			el_span.id = "countdown";
			el_span.innerHTML = aPlay.countdown;
			el_div.appendChild(el_span);
			player.el_.appendChild(el_div);
			
			var countd = aPlay.countdown;
			window.interval_countdown = setInterval(function(){
				countd--;
				el_span.innerHTML = countd;
				if(countd <= 0){
					clearInterval(interval_countdown);
					player.play();
					el_div.parentNode.removeChild(el_div);
				} 				
			}, 1000);
		}
		
	}
	
    /**
    * Initialize the plugin.
    * @param options (optional) {object} configuration for the plugin
    */
    var videoJsPoster = function(options) {
        var settings = videojs.mergeOptions(defaults, options),
            player = this;

        var mePoster = new mmPoster(settings,player),
            domMePoster = mePoster.createPoster();

        mePoster.el_ = domMePoster;
        player.el_.appendChild(domMePoster);

        var domMePlay = mePoster.createPlay();
        player.el_.appendChild(domMePlay);
        mePoster.updatePoster(settings)

        if (settings.aPlay != null && typeof(settings.aPlay) != 'undefined' && settings.aPlay.play) {
			
			player.volume(settings.aPlay.volume / 100);
			
			if(settings.aPlay.lazyload){
				__AutoPlay.lazyload(player, settings.aPlay);
				
			}else{
				if(settings.aPlay.mouseover){
					__AutoPlay.mouseover(player, settings.aPlay);					
					
				}else if(settings.aPlay.countdown > 0){					
					__AutoPlay.countdown(player, settings.aPlay);
				}else{
					player.play();
					//mePoster.el_.style.display = 'none';
				}
			}
			
        }else if(settings.autoplay){
			player.play();
		}
        
        player.on("play", function(){
            
			mePoster.el_.style.display = 'none';
			
			if(typeof(interval_countdown) != 'undefined' && interval_countdown){ clearInterval(interval_countdown); }
			
			var el_lzld = player.el_.querySelector('#autoplay-lzld');
			if(el_lzld != null && typeof(el_lzld) != 'undefined'){
				el_lzld.parentNode.removeChild(el_lzld);
			}
        })

        player.mePoster = mePoster;
        //player.mePlay = mePlay;

    }
    //player-holder 
    /*
     * 
     */

    function mmPoster(settings,player) {
        this.player = player;
        this.title = settings.title;
        this.image = settings.image;
        this.position = settings.position;
        this.showControlBar = settings.showControlBar;
        this.autoplay = settings.autoplay;
        this.offTitle = settings.offTitle;
        this.buttonPlay = settings.buttonPlay == "true" ? true : settings.buttonPlay == "false" ? false : true; 

        if ((this.position == 'bottom' && this.showControlBar == false) || (this.position == 'bottom' && this.showControlBar == true) || this.showControlBar == false) {
            this.player.el_.querySelector('.vmp-control-bar').style.cssText = 'z-index: 4';
        }else {
            this.player.el_.querySelector('.vmp-control-bar').style.cssText = 'z-index: 1000';
        }

        player.on("play", function(){
            this.el_.querySelector('.vmp-poster').style.cssText = 'display: none';
        });

    }

    mmPoster.prototype.updatePoster = function(settings){ 
        //console.log("updatePoster " + this.player.el_.id)
        this.title = settings.title || this.title;
        this.image = settings.image || this.image ;
        this.position = settings.position || this.position;
        this.showControlBar = settings.showControlBar == "true" ? true : settings.showControlBar == "false" ? false : this.showControlBar;
        this.autoplay = settings.autoplay == "true" ? true : settings.autoplay == "false" ? false : this.autoplay;
        this.offTitle = settings.offTitle == "true" ? true : settings.offTitle == "false" ? false : this.offTitle; 
        this.buttonPlay = settings.buttonPlay == "true" ? true : settings.buttonPlay == "false" ? false : true; 

        if ((this.position == 'bottom' && this.showControlBar == false) || (this.position == 'bottom' && this.showControlBar == true) || this.showControlBar == false) {
            this.el_.parentNode.querySelector('.vmp-control-bar').style.cssText = 'z-index: 4';
        }else {
            this.el_.parentNode.querySelector('.vmp-control-bar').style.cssText = 'z-index: 1000';
        }
        
        if (this.offTitle){
            this.el_.className = 'vmp-poster ' + 'pos' + this.position + ' offTitle';
        }else {
            this.el_.className = 'vmp-poster ' + 'pos' + this.position;
        }
        
         if (this.player.el_.parentNode.offsetWidth < 540){
            this.el_.className += " smallContent";
            console.log(this.el_.className)
        }
        
        this.el_.style["background-image"]='url(' + this.image + ')';
        this.el_.querySelector(".title-mep").innerHTML = this.title;
        //console.log(this.el_.style)
        this.el_.style.display="block";
        if (this.autoplay){
            //this.player.play();
            //this.el_.style.display = 'none';
        }
    }

    mmPoster.prototype.getConfig = function(settings){
        return this
    }

    mmPoster.prototype.createPoster = function(){ 
        //div player holder
        var dvmpPoster = document.createElement('div');

        dvmpPoster.className = 'vmp-poster ' + 'pos' + this.position;
        dvmpPoster.id = 'vmp-poster';

        var attBackground  = document.createAttribute('style');
        attBackground.value="background-image:url(" + this.image + ")";
        dvmpPoster.setAttributeNode(attBackground);

        var wrapperBtnBig = document.createElement('div');
        wrapperBtnBig.className = 'wrapper-btn-big-play';
        dvmpPoster.appendChild(wrapperBtnBig);

        var btnBigPlay = document.createElement('div');
        btnBigPlay.className = 'btn-big-play';        
        wrapperBtnBig.appendChild(btnBigPlay);

        var iconBigPlay = document.createElement('div');
        iconBigPlay.className = 'icon-play';        
        btnBigPlay.appendChild(iconBigPlay);

        var iconSvg = '<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="16.6px" height="18.963px" viewBox="0 0 16.6 18.963" enable-background="new 0 0 16.6 18.963" xml:space="preserve"><path d="M0,1.942c0-1.777,1.31-2.461,2.911-1.519l12.488,7.346c1.601,0.942,1.601,2.483,0,3.425L2.911,18.54C1.31,19.481,0,18.798,0,17.021V1.942z"/></svg>'
        iconBigPlay.innerHTML = iconSvg;

                
        var titleMep = document.createElement('div');
        titleMep.className = "title-mep";
        titleMep.innerHTML = this.title;
        btnBigPlay.appendChild(titleMep);
        var self = this;

        if (this.offTitle){
            dvmpPoster.className = 'vjs-poster ' + 'pos' + this.position + ' offTitle';
        }else {
            dvmpPoster.className = 'vjs-poster ' + 'pos' + this.position;
        }

        if (this.player.el_.parentNode.offsetWidth < 540){
            dvmpPoster.className += " smallContent";
            console.log(dvmpPoster.className)
        } 

        dvmpPoster.addEventListener('click',function(){
                self.player.play();
                //this.style.display = 'none';
        });

        // dvmpPoster.addEventListener('tap',function(){
        //         self.player.play();
        //         this.style.display = 'none';
        // });

        // dvmpPoster.addEventListener('touchstart',function(){
        //         self.player.play();
        //         this.style.display = 'none';
        // });

        return dvmpPoster;

    } 



    mmPoster.prototype.createPlay = function(){ 

        var statusPlay = document.createElement('div');
        statusPlay.className = "status-player";

        var iconSvgPlay = '<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="16.6px" height="18.963px" viewBox="0 0 16.6 18.963" enable-background="new 0 0 16.6 18.963" xml:space="preserve"><path d="M0,1.942c0-1.777,1.31-2.461,2.911-1.519l12.488,7.346c1.601,0.942,1.601,2.483,0,3.425L2.911,18.54C1.31,19.481,0,18.798,0,17.021V1.942z"/></svg>';
        statusPlay.innerHTML = iconSvgPlay;

        var self = this;
        
        statusPlay.addEventListener("click",function(){
            self.player.play();
        });
        
        this.player.on('pause',function(){
            if (self.buttonPlay)
                this.el_.querySelector('.status-player').classList.add('flashButton');
        })
        this.player.on('play',function(){
			
            this.el_.querySelector('.status-player').classList.remove('flashButton');
            setInterval(function(){
                if(!self.player.paused()){
                    self.player.el_.querySelector('.status-player').classList.remove('flashButton');
                } else
                    self.player.el_.querySelector('.status-player').classList.add('flashButton');
            },500)
        })
        console.log('createPlay')
        this.statusPlay = statusPlay
        return statusPlay;

    }   

    mmPoster.prototype.hideButtonPlay = function() {
        this.statusPlay.classList.remove('flashButton');
    }
    mmPoster.prototype.showButtonPlay = function() {
        if (this.buttonPlay)
            this.statusPlay.classList.add('flashButton');
    }

    // register the plugin
    videojs.plugin('videoJsPoster', videoJsPoster);
}
