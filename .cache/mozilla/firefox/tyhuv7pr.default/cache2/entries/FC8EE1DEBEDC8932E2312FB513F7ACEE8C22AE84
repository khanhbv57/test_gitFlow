	var lastStatus = true;
	var lastShowOverlay = false;
	function ShareTools (player, settings) {
    // return share tools functions
    return {
      open: false,
      setup: function() {

        this.open = true;

        // pause video
		lastStatus = !player.paused();//  (false) is playing
		
		
        if (!player.paused()) {
          player.pause();
        }
		var video = player.currentSrc();
		
		

		if(typeof(settings.lang) == 'undefined' || settings.lang == null)
		{
			settings.lang = "vn";
		}



        // prepare overlay
        var overlay = document.createElement("div");
        overlay.className = "sharetools-overlay";
		var contentShareToolDiv = document.createElement("div"),contentNetwork = document.createElement("div") ;
		contentShareToolDiv.className = "sharetool";
		contentNetwork.className = "network-share-mep";
		setTimeout(function(){
			contentNetwork.className = "network-share-mep show";
		},10);
		contentNetwork.innerHTML = "<div class=\"wrapper-share-mep\"><div class=\"content-share-mep\"><h2 class=\"title-share-mep\">"+language[settings.lang].share+"</h2><ul class=\"social-share-mep\"></ul><div class=\"input-share-mep1\" onclick=\"document.execCommand('selectAll',false,null)\"><input type=\"text\" value="+window.location+"/></div></div></div>";
		
		
		var embedDiv = null;
        var shareToolDiv;
        if(!player.ended() ){
        	console.log("show click");
        	if(settings.shareWebsitePlaying == true){
				contentNetwork.innerHTML = "<div class=\"wrapper-share-mep\"><div class=\"content-share-mep\"><h2 class=\"title-share-mep\">"+language[settings.lang].share+"</h2><ul class=\"social-share-mep\"></ul><div class=\"input-share-mep1\" onclick=\"document.execCommand('selectAll',false,null)\"><input type=\"text\" value="+window.location+"/></div></div></div>";
			}else{
				contentNetwork.innerHTML = "<div class=\"wrapper-share-mep\"><div class=\"content-share-mep\"><h2 class=\"title-share-mep\">"+language[settings.lang].share+"</h2><ul class=\"social-share-mep\"></ul></div></div>";
			}
			shareToolDiv = contentNetwork.getElementsByTagName("ul")[0];
			if(settings.facebook && settings.shareFacebookPlaying == true) {
			  var li = document.createElement('li');
			  var facebook = document.createElement('a');
	          facebook.innerHTML = "<svg version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\" width=\"26.079px\" height=\"26.079px\" viewBox=\"0 0 26.079 26.079\" enable-background=\"new 0 0 26.079 26.079\" xml:space=\"preserve\"><g><path d=\"M19.789,4.352h-2.453c-1.922,0-2.281,0.922-2.281,2.25v2.953h4.578l-0.609,4.625h-3.969v11.858h-4.781V14.18H6.29V9.555 h3.984V6.149c0-3.953,2.422-6.109,5.953-6.109c1.688,0,3.141,0.125,3.563,0.188V4.352z\"/></g></svg>";
			  li.appendChild(facebook);
	          shareToolDiv.appendChild(li);
			  
			  facebook.onclick = function(e){
	            e.stopPropagation();
	            winOpen(settings.facebook(settings));
	            return false;
	          };
			  
	        }
			
			
