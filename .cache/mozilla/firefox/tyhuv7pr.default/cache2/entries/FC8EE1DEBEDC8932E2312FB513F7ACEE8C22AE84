    var defaults = {
	  	showButtonShare: true,
	    showOnPause: false,
		showBeforePlay:false,
		showEnd:false, // hiển thị btn share khi video kết thúc
		embed : "", // hiển thị btn share embed
		shareFacebookPlaying:true, // hiển thị btn chia sẻ facebook khi overlay xuất hiện khi video đang chạy
		shareGooglePlusPlaying:true, // hiển thị btn chia sẻ google plus khi overlay xuất hiện khi video đang chạy
		shareWebsitePlaying:true, //hiển thị thẻ input chia sẻ link website khi overlay xuất hiện khi video đang chạy
		shareEmbedPlaying:true,
		shareFacebookEnd:true, // hiển thị btn chia sẻ facebook khi overlay xuất hiện khi video kết thúc
		shareGooglePlusEnd:true, // hiển thị btn chia sẻ google plus khi overlay xuất hiện khi video kết thúc
		shareWebsiteEnd:true, //hiển thị thẻ input chia sẻ link website khi overlay xuất hiện khi video kết thúc
		showOverlayEnd:true, // hiển thị overlay khi video kết thúc
		shareEmbedEnd:true,
		showDuringPlaying:false,
	    shareUrl: window.location,
	    facebook: function(settings){
	        return "https://www.facebook.com/sharer/sharer.php?u=" + settings.shareUrl;
	    },
		googleplus:function(settings){
	        return "https://plus.google.com/share?url=" + settings.shareUrl;
	    },
	    lang: "vn"
  };
  var language = new Array(2);
		language["vn"] = {
			
			manhung :"Mã nhúng",
		  	copy: "Sao chép",
		  	share : "Chia sẻ",
